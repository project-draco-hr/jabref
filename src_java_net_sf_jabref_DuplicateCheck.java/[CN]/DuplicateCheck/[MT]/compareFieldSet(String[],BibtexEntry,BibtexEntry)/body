{
  int res=0, empty=0;
  for (int i=0; i < fields.length; i++) {
    int result=compareSingleField(fields[i],one,two);
    if (result == Util.EQUAL) {
      res++;
    }
 else     if (result == Util.EMPTY_IN_BOTH)     empty++;
  }
  if (fields.length > empty)   return ((float)res) / ((float)(fields.length - empty));
 else   return 0.5f;
}
