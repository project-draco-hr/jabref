{
  if (s1.length() == 1 && s2.length() == 1) {
    return s1.equals(s2) ? 1.0 : 0.0;
  }
  double[] n1=numberizeString(s1), n2=numberizeString(s2);
  if (n1.length < n2.length)   n1=stretchArray(n1,n2.length);
 else   if (n2.length < n1.length)   n2=stretchArray(n2,n1.length);
  return corrCoef(n1,n2);
}
