{
  String pth=f.getPath();
  for (  File dir : dirs) {
    if (pth.startsWith(dir.getPath())) {
      String subs=pth.substring(dir.getPath().length());
      if ((subs.length() > 0) && ((subs.charAt(0) == '/') || (subs.charAt(0) == '\\')))       subs=subs.substring(1);
      return new File(subs);
    }
  }
  return f;
}
