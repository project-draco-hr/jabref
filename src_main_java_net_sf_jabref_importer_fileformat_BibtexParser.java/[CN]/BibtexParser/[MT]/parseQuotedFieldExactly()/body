{
  StringBuffer value=new StringBuffer();
  consume('"');
  int brackets=0;
  while (!((peek() == '"') && (brackets == 0))) {
    int j=read();
    if (isEOFCharacter(j)) {
      throw new RuntimeException("Error in line " + line + ": EOF in mid-string");
    }
 else     if (j == '{') {
      brackets++;
    }
 else     if (j == '}') {
      brackets--;
    }
    value.append((char)j);
  }
  consume('"');
  return value;
}
