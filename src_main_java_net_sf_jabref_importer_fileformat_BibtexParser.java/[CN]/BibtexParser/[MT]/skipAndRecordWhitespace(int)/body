{
  int c;
  StringBuilder sb=new StringBuilder();
  if (j != ' ') {
    sb.append((char)j);
  }
  while (true) {
    c=read();
    if (c == -1 || c == 65535) {
      eof=true;
      return sb.toString();
    }
    if (Character.isWhitespace((char)c)) {
      if (c != ' ') {
        sb.append((char)c);
      }
      continue;
    }
 else {
      unread(c);
      break;
    }
  }
  return sb.toString();
}
