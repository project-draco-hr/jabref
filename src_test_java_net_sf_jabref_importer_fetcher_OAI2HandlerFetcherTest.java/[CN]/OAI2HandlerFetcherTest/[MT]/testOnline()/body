    Assert.assertEquals(Optional.of("Javier L??pez Pe??a and Gabriel Navarro"),be.getFieldOptional("author"));
    Assert.assertEquals(Optional.of("2007"),be.getFieldOptional("year"));
    Thread.sleep(20000);
  }
