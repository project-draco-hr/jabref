{
  try {
    if (Objects.nonNull(databaseID)) {
      StringBuilder stringBuilder=new StringBuilder().append("% ").append(DATABASE_ID_PREFIX).append(" ").append(databaseID).append(OS.NEWLINE);
      getWriter().write(stringBuilder.toString());
    }
  }
 catch (  IOException e) {
    throw new SaveException(e);
  }
}
