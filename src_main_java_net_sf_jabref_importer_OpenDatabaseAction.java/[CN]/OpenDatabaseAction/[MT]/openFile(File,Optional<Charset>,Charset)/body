{
  if (encoding.isPresent()) {
    try {
      return ImportFormatReader.getReader(fileToOpen,encoding.get());
    }
 catch (    Exception ex) {
      ex.printStackTrace();
      return ImportFormatReader.getReader(fileToOpen,defaultEncoding);
    }
  }
 else {
    return ImportFormatReader.getReader(fileToOpen,defaultEncoding);
  }
}
