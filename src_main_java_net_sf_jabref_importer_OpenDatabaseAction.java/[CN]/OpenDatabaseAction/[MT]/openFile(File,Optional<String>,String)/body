{
  if (encoding.isPresent()) {
    try {
      return ImportFormatReader.getReader(fileToOpen,encoding.get());
    }
 catch (    Exception ex) {
      LOGGER.error("Could not open file " + fileToOpen,ex);
      return ImportFormatReader.getReader(fileToOpen,fallbackEncoding);
    }
  }
 else {
    return ImportFormatReader.getReader(fileToOpen,fallbackEncoding);
  }
}
