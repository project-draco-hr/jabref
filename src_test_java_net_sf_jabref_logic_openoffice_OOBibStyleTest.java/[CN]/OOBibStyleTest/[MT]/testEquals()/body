{
  OOBibStyle style1=new OOBibStyle(StyleLoader.DEFAULT_NUMERICAL_STYLE_PATH,LayoutFormatterPreferences.fromPreferences(Globals.prefs,mock(JournalAbbreviationLoader.class)));
  OOBibStyle style2=new OOBibStyle(StyleLoader.DEFAULT_NUMERICAL_STYLE_PATH,LayoutFormatterPreferences.fromPreferences(Globals.prefs,mock(JournalAbbreviationLoader.class)));
  assertEquals(style1,style2);
}
