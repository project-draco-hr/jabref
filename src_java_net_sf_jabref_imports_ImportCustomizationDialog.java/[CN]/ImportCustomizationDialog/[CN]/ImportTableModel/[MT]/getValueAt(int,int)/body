{
  Object value=null;
  CustomImportList.Importer importer=getImporter(rowIndex);
  if (columnIndex == 0) {
    value=importer.getName();
  }
 else   if (columnIndex == 1) {
    value=importer.getClassName();
  }
 else   if (columnIndex == 2) {
    value=importer.getBasePath();
  }
  return value;
}
