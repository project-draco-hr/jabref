{
  assertEquals("Newton",LabelPatternUtil.firstAuthor("I. Newton and J. Maxwell and A. Einstein and N. Bohr and Harry Unknown"));
  assertEquals("Newton",LabelPatternUtil.firstAuthor("I. Newton"));
  assertEquals("K{\\\"o}ning",LabelPatternUtil.firstAuthor("K{\\\"o}ning"));
  assertEquals("",LabelPatternUtil.firstAuthor(""));
  try {
    LabelPatternUtil.firstAuthor(null);
    fail();
  }
 catch (  NullPointerException e) {
  }
}
