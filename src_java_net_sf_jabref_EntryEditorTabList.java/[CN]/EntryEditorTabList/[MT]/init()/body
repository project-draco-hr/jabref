{
  list=new ArrayList();
  names=new ArrayList();
  int i=0;
  String name=null;
  String[] fields=null;
  while ((name=Globals.prefs.get(Globals.prefs.CUSTOM_TAB_NAME + i)) != null) {
    fields=Globals.prefs.get(Globals.prefs.CUSTOM_TAB_FIELDS + i).split(";");
    List entry=Arrays.asList(fields);
    names.add(name);
    list.add(entry);
    i++;
  }
}
