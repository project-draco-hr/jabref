{
  String[] parts=AbstractParamLayoutFormatter.parseArgument(arg);
  for (  String part : parts) {
    int index=part.indexOf("=");
    if (index > 0) {
      String key=part.substring(0,index);
      String value=part.substring(index + 1);
      handleArgument(key,value);
    }
 else {
      handleArgument(part,"");
    }
  }
}
