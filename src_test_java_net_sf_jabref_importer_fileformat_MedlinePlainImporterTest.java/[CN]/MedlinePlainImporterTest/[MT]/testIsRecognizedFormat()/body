{
  List<String> list=Arrays.asList("CopacImporterTest1.txt","CopacImporterTest2.txt","IsiImporterTest1.isi","IsiImporterTestInspec.isi","IsiImporterTestWOS.isi","IsiImporterTestMedline.isi");
