{
  Assert.assertEquals("",AuthorList.getAuthors("").getAsNatbib());
  Assert.assertEquals("Smith",AuthorList.getAuthors("John Smith").getAsNatbib());
  Assert.assertEquals("Smith and Black Brown",AuthorList.getAuthors("John Smith and Black Brown, Peter").getAsNatbib());
  Assert.assertEquals("von Neumann et al.",AuthorList.getAuthors("John von Neumann and John Smith and Black Brown, Peter").getAsNatbib());
  Assert.assertEquals("Last-Name et al.",AuthorList.getAuthors("First Second Last-Name" + " and John Smith and Black Brown, Peter").getAsNatbib());
  AuthorList al=AuthorList.getAuthors("John von Neumann and John Smith and Black Brown, Peter");
  Assert.assertTrue(al.getAsNatbib().equals(al.getAsNatbib()));
}
