{
  Assert.assertEquals("Smith, John",AuthorList.parse("John Smith").getAsLastFirstNamesWithAnd(false));
  Assert.assertEquals("Smith, John",AuthorList.parse("John Smith").getAsLastFirstNamesWithAnd(false));
  Assert.assertEquals("Smith, J.",AuthorList.parse("John Smith").getAsLastFirstNamesWithAnd(true));
  Assert.assertEquals("Smith, J.",AuthorList.parse("John Smith").getAsLastFirstNamesWithAnd(true));
}
