{
  Assert.assertEquals("Smith, J.",AuthorList.getAuthors("John Smith").getForAlphabetization());
  Assert.assertEquals("Neumann, J.",AuthorList.getAuthors("John von Neumann").getForAlphabetization());
  Assert.assertEquals("Neumann, J.",AuthorList.getAuthors("J. von Neumann").getForAlphabetization());
  Assert.assertEquals("Neumann, J. and Smith, J. and Black Brown, Jr., P.",AuthorList.getAuthors("John von Neumann and John Smith and de Black Brown, Jr., Peter").getForAlphabetization());
}
