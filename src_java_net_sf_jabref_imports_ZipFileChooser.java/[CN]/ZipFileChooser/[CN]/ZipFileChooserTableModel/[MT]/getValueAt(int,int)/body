{
  Object value=null;
  ZipEntry entry=getZipEntry(rowIndex);
  if (columnIndex == 0) {
    value=entry.getName();
  }
 else   if (columnIndex == 1) {
    value=SimpleDateFormat.getDateTimeInstance().format(new Date(entry.getTime()));
  }
 else   if (columnIndex == 2) {
    value=new Long(entry.getSize());
  }
  return value;
}
