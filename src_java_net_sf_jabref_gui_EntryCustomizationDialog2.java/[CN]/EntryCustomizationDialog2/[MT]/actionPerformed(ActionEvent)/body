{
  if (e.getSource() == ok) {
    applyChanges();
    setVisible(false);
  }
 else   if (e.getSource() == cancel) {
    setVisible(false);
  }
 else   if (e.getSource() == apply) {
    applyChanges();
  }
 else   if (e.getSource() == typeComp) {
    typeComp.selectField(e.getActionCommand());
  }
}
