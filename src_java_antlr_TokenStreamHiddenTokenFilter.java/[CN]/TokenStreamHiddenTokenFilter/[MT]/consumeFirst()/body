{
  consume();
  CommonHiddenStreamToken p=null;
  while (hideMask.member(LA(1).getType()) || discardMask.member(LA(1).getType())) {
    if (hideMask.member(LA(1).getType())) {
      if (p == null) {
        p=LA(1);
      }
 else {
        p.setHiddenAfter(LA(1));
        LA(1).setHiddenBefore(p);
        p=LA(1);
      }
      lastHiddenToken=p;
      if (firstHidden == null) {
        firstHidden=p;
      }
    }
    consume();
  }
}
