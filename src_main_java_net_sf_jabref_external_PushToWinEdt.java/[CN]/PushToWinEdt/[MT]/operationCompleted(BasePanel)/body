{
  if (notDefined) {
    panel.output(Globals.lang("Error") + ": " + Globals.lang("Path to %0 not defined",getApplicationName())+ ".");
  }
 else   if (couldNotCall) {
    panel.output(Globals.lang("Error") + ": " + Globals.lang("Could not call executable")+ " '"+ Globals.prefs.get(JabRefPreferences.WIN_EDT_PATH)+ "'.");
  }
 else {
    Globals.lang("Pushed citations to WinEdt");
  }
}
