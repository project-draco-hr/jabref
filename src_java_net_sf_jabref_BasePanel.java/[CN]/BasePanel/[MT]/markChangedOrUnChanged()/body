{
  if (undoManager.hasChanged()) {
    if (!baseChanged)     markBaseChanged();
  }
 else   if (baseChanged && !nonUndoableChange) {
    baseChanged=false;
    if (file != null)     frame.setTabTitle(BasePanel.this,file.getName(),file.getAbsolutePath());
 else     frame.setTabTitle(BasePanel.this,Globals.lang("untitled"),null);
  }
}
