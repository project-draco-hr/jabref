{
  super(JSplitPane.HORIZONTAL_SPLIT,true);
  this.frame=frame;
  database=db;
  this.prefs=prefs;
  parseMetaData(meta);
  setupActions();
  setupMainPanel();
  this.file=file;
  if (file != null)   try {
