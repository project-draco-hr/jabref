{
  this.frame=frame;
  database=db;
  this.prefs=prefs;
  if (meta != null)   parseMetaData(meta);
 else   metaData=new MetaData();
  setupActions();
  setupMainPanel();
  this.file=file;
  if (file != null)   try {
    fileMonitorHandle=Globals.fileUpdateMonitor.addUpdateListener(this,file);
  }
 catch (  IOException ex) {
  }
}
