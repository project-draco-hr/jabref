{
  IdBasedFetcher fetcher;
switch (field) {
case FieldName.DOI:
    fetcher=new DoiFetcher(preferences);
  break;
case FieldName.ISBN:
fetcher=new IsbnFetcher(preferences);
break;
case FieldName.EPRINT:
fetcher=new ArXiv();
break;
default :
return Optional.empty();
}
return Optional.of(fetcher);
}
