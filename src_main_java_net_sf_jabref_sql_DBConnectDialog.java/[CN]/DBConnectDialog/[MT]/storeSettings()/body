{
  DBStringsPreferences preferences=new DBStringsPreferences(cmbServerType.getSelectedItem().toString(),txtServerHostname.getText(),txtUsername.getText(),txtDatabase.getText());
  preferences.storeToPreferences(Globals.prefs);
  dbStrings.setDbPreferences(preferences);
  char[] pwd=pwdPassword.getPassword();
  String tmp=new String(pwd);
  dbStrings.setPassword(tmp);
  Arrays.fill(pwd,'0');
}
