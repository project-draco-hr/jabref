{
  Optional<BibEntry> entry=BibtexParser.singleFromStringOptional("@ARTICLE{kohn, author={{Link{\\\"{o}}ping University, Department of Electrical Engineering}}}",importFormatPreferences);
  assertEquals("UniLinkoepingEE",BibtexKeyPatternUtil.checkLegalKey(BibtexKeyPatternUtil.makeLabel(entry.get(),"auth"),true));
}
