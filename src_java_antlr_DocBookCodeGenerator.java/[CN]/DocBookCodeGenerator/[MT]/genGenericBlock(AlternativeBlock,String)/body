{
  if (blk.alternatives.size() > 1) {
    _println("");
    if (!firstElementInAlt) {
      _println("(");
    }
 else {
      _print("(");
    }
  }
 else {
    _print("( ");
  }
  genCommonBlock(blk);
  if (blk.alternatives.size() > 1) {
    _println("");
    print(")" + blkOp + " ");
    if (!(blk.next instanceof BlockEndElement)) {
      _println("");
      print("");
    }
  }
 else {
    _print(")" + blkOp + " ");
  }
}
