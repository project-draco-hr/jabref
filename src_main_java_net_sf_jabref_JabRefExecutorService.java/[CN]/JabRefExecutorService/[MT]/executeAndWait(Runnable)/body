{
  if (command == null) {
    return;
  }
  Future<?> future=executorService.submit(command);
  while (true) {
    try {
      future.get();
      return;
    }
 catch (    InterruptedException e) {
      e.printStackTrace();
    }
catch (    ExecutionException e) {
      e.printStackTrace();
    }
  }
}
