{
  if (metaData == null) {
    throw new IllegalArgumentException("MetaData must not be null");
  }
  actions=new HashMap<>();
  setAvailableFormatters();
  List<String> formatters=metaData.getData(META_KEY);
  if (formatters == null) {
    return;
  }
 else {
    parseEnabledStatus(formatters);
    parseSaveActions(formatters);
  }
}
