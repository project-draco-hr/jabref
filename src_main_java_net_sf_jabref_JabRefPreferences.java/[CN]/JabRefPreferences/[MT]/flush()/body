{
  if (getBoolean(MEMORY_STICK_MODE)) {
    try {
      exportPreferences("jabref.xml");
    }
 catch (    JabRefException e) {
      LOGGER.warn("Could not export preferences for memory stick mode: " + e.getMessage(),e);
    }
  }
  try {
    prefs.flush();
  }
 catch (  BackingStoreException ex) {
    ex.printStackTrace();
  }
}
