{
  if ((value == null)) {
    remove(key);
    return;
  }
  if (value.isEmpty()) {
    put(key,"");
  }
 else {
    StringBuilder linked=new StringBuilder();
    for (int i=0; i < (value.size() - 1); i++) {
      linked.append(makeEscape(value.get(i)));
      linked.append(';');
    }
    linked.append(makeEscape(value.get(value.size() - 1)));
    put(key,linked.toString());
  }
}
