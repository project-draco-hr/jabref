{
  String chosenFile=Globals.getNewFile(ths,prefs,new File(prefs.get("workingDirectory")),".csv",JFileChooser.SAVE_DIALOG,true);
  if (chosenFile == null)   return;
  try {
    FileActions.exportToCSV(basePanel().database(),new File(chosenFile),prefs);
  }
 catch (  Exception ex) {
    ex.printStackTrace();
  }
}
