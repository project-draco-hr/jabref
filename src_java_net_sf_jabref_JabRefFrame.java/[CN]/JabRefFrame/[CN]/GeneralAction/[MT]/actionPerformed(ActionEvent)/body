{
  if (tabbedPane.getTabCount() > 0) {
    try {
      ((BasePanel)(tabbedPane.getSelectedComponent())).runCommand(command);
    }
 catch (    Throwable ex) {
      ex.printStackTrace();
    }
  }
 else {
    Util.pr("Action '" + command + "' must be disabled when no "+ "database is open.");
  }
}
