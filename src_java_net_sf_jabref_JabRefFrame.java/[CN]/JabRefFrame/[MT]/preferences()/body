{
  AbstractWorker worker=new AbstractWorker(){
    PrefsDialog2 pd=null;
    public void run(){
      output(Globals.lang("Opening preferences..."));
      pd=new PrefsDialog2(ths,prefs);
      Util.placeDialog(pd,ths);
    }
    public void update(){
      pd.show();
      output("");
    }
  }
;
  worker.getWorker().run();
  worker.getCallBack().update();
}
