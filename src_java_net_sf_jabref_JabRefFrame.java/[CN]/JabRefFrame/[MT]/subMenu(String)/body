{
  name=Globals.menuTitle(name);
  int i=name.indexOf('&');
  JMenu res;
  if (i >= 0) {
    res=new JMenu(name.substring(0,i) + name.substring(i + 1));
    char mnemonic=Character.toUpperCase(name.charAt(i + 1));
    res.setMnemonic((int)mnemonic);
  }
 else   res=new JMenu(name);
  return res;
}
