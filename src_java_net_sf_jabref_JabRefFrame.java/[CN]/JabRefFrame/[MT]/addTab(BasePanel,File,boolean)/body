{
  tabbedPane.add((file != null ? file.getName() : Globals.lang(GUIGlobals.untitledTitle)),bp);
  tabbedPane.setToolTipTextAt(tabbedPane.getTabCount() - 1,file != null ? file.getAbsolutePath() : null);
  if (raisePanel) {
    tabbedPane.setSelectedComponent(bp);
  }
  if (tabbedPane.getTabCount() == 1) {
    setNonEmptyState();
  }
 else   if (tabbedPane.getTabCount() == 2) {
    setMultiple();
  }
}
