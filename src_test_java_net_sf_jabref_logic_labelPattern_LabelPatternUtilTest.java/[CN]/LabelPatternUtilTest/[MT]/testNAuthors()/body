{
  Assert.assertEquals("Newton",LabelPatternUtil.NAuthors("I. Newton",3));
  Assert.assertEquals("NewtonMaxwell",LabelPatternUtil.NAuthors("I. Newton and J. Maxwell",3));
  Assert.assertEquals("NewtonMaxwellEinstein",LabelPatternUtil.NAuthors("I. Newton and J. Maxwell and A. Einstein",3));
  Assert.assertEquals("NewtonMaxwellEinsteinEtAl",LabelPatternUtil.NAuthors("I. Newton and J. Maxwell and A. Einstein and N. Bohr",3));
}
