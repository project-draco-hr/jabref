{
  Assert.assertEquals("Newton",LabelPatternUtil.NAuthors(AUTHOR_STRING_FIRSTNAME_INITIAL_LASTNAME_FULL_COUNT_1,3));
  Assert.assertEquals("NewtonMaxwell",LabelPatternUtil.NAuthors(AUTHOR_STRING_FIRSTNAME_INITIAL_LASTNAME_FULL_COUNT_2,3));
  Assert.assertEquals("NewtonMaxwellEinstein",LabelPatternUtil.NAuthors(AUTHOR_STRING_FIRSTNAME_INITIAL_LASTNAME_FULL_COUNT_3,3));
  Assert.assertEquals("NewtonMaxwellEinsteinEtAl",LabelPatternUtil.NAuthors(AUTHOR_STRING_FIRSTNAME_INITIAL_LASTNAME_FULL_COUNT_4,3));
}
