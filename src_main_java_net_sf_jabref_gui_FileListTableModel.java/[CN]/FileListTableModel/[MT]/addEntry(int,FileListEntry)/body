{
synchronized (list) {
    list.add(index,entry);
    if (!SwingUtilities.isEventDispatchThread()) {
      SwingUtilities.invokeLater(new Runnable(){
        @Override public void run(){
          fireTableRowsInserted(index,index);
        }
      }
);
    }
 else {
      fireTableRowsInserted(index,index);
    }
  }
}
