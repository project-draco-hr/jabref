{
  if (col == 0) {
    return GUIGlobals.NUMBER_COL;
  }
 else   if (getIconTypeForColumn(col) != null) {
    return "";
  }
 else {
    String disName=BibtexFields.getFieldDisplayName(columns[col - padleft]);
    if (disName != null) {
      return disName;
    }
  }
  return Util.nCase(columns[col - padleft]);
}
