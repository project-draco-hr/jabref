  if (fieldText.equalsIgnoreCase("Incollection")) {
    return "5";
  }
  if (fieldText.equalsIgnoreCase("Inproceedings")) {
    return "6";
  }
  if (fieldText.equalsIgnoreCase("Manual")) {
    return "8";
