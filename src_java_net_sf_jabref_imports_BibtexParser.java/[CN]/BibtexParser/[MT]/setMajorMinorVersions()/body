{
  String v=_pr.getJabrefVersion();
  Pattern p=Pattern.compile("([0-9]+)\\.([0-9]+).*");
  Matcher m=p.matcher(v);
  if (!m.matches())   return;
  if (m.groupCount() >= 2) {
    _pr.setJabrefMajorVersion(Integer.parseInt(m.group(1)));
    _pr.setJabrefMinorVersion(Integer.parseInt(m.group(2)));
  }
}
