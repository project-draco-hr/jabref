{
  String id=Util.createNeutralId();
  BibtexEntry result=new BibtexEntry(id,tp);
  skipWhitespace();
  consume('{','(');
  skipWhitespace();
  String key=null;
  boolean doAgain=true;
  while (doAgain) {
    doAgain=false;
    try {
      if (key != null)       key=key + parseKey();
 else       key=parseKey();
    }
 catch (    NoLabelException ex) {
      char c=(char)peek();
      if (Character.isWhitespace(c) || (c == '{') || (c == '\"')) {
        String fieldName=ex.getMessage().trim().toLowerCase();
        String cont=parseFieldContent();
        result.setField(fieldName,cont);
      }
 else {
        if (key != null)         key=key + ex.getMessage() + "=";
 else         key=ex.getMessage() + "=";
        doAgain=true;
      }
    }
  }
  if ((key != null) && key.equals(""))   key=null;
  if (result != null)   result.setField(BibtexFields.KEY_FIELD,key);
  skipWhitespace();
  while (true) {
    int c=peek();
    if ((c == '}') || (c == ')')) {
      break;
    }
    if (c == ',')     consume(',');
    skipWhitespace();
    c=peek();
    if ((c == '}') || (c == ')')) {
      break;
    }
    parseField(result);
  }
  consume('}',')');
  return result;
}
