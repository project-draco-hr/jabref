{
  String key=parseTextToken().toLowerCase();
  skipWhitespace();
  consume('=');
  String content=parseFieldContent();
  if (content.length() > 0) {
    if (entry.getField(key) == null)     entry.setField(key,content);
 else {
      if (key.equals("author") || key.equals("editor"))       entry.setField(key,entry.getField(key) + " and " + content);
    }
  }
}
