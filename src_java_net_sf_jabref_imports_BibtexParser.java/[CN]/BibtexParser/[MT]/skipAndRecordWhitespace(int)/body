{
  int c;
  StringBuffer sb=new StringBuffer();
  if (j != ' ')   sb.append((char)j);
  while (true) {
    c=read();
    if ((c == -1) || (c == 65535)) {
      _eof=true;
      return sb.toString();
    }
    if (Character.isWhitespace((char)c)) {
      if (c != ' ')       sb.append((char)c);
      continue;
    }
 else     unread(c);
    break;
  }
  return sb.toString();
}
