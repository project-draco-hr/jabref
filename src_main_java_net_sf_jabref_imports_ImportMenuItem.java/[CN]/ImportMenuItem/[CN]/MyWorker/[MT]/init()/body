{
  importError=null;
  filenames=FileDialogs.getMultipleFiles(frame,new File(Globals.prefs.get("workingDirectory")),(importer != null ? importer.getExtensions() : null),true);
  if ((filenames != null) && (filenames.length > 0)) {
    frame.block();
    frame.output(Globals.lang("Starting import"));
    fileOk=true;
    Globals.prefs.put("workingDirectory",filenames[0]);
  }
}
