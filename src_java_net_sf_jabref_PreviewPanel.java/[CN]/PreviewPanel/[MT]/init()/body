{
  setEditable(false);
  setContentType(CONTENT_TYPE);
  addHyperlinkListener(new HyperlinkListener(){
    public void hyperlinkUpdate(    HyperlinkEvent hyperlinkEvent){
      try {
        Util.openExternalViewer(hyperlinkEvent.getURL().toString(),"url",Globals.prefs);
      }
 catch (      IOException e) {
        e.printStackTrace();
      }
    }
  }
);
}
