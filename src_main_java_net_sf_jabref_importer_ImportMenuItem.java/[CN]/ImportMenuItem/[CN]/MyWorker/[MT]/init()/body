{
  importError=null;
  filenames=FileDialogs.getMultipleFiles(frame,new File(Globals.prefs.get(JabRefPreferences.WORKING_DIRECTORY)),importer == null ? null : importer.getExtensions(),true);
  if ((filenames != null) && (filenames.length > 0)) {
    frame.block();
    frame.output(Localization.lang("Starting import"));
    fileOk=true;
    Globals.prefs.put(JabRefPreferences.WORKING_DIRECTORY,filenames[0]);
  }
}
