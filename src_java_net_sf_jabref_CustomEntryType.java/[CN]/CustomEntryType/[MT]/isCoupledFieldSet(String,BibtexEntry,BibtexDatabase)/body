{
  if (reqSets == null)   return false;
  for (int i=0; i < reqSets.length; i++) {
    boolean takesPart=false, oneSet=false;
    for (int j=0; j < reqSets[i].length; j++) {
      if (reqSets[i][j].equalsIgnoreCase(field))       takesPart=true;
 else       if (BibtexDatabase.getResolvedField(reqSets[i][j],entry,database) != null)       oneSet=true;
    }
    if (takesPart && oneSet)     return true;
  }
  return false;
}
