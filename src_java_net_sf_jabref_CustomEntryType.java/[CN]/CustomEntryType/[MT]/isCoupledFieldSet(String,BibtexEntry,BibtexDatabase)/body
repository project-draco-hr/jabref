{
  if (reqSets == null)   return false;
  for (  String[] reqSet : reqSets) {
    boolean takesPart=false, oneSet=false;
    for (    String aReqSet : reqSet) {
      if (aReqSet.equalsIgnoreCase(field))       takesPart=true;
 else       if (BibtexDatabase.getResolvedField(aReqSet,entry,database) != null)       oneSet=true;
    }
    if (takesPart && oneSet)     return true;
  }
  return false;
}
