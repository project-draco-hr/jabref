{
  if (reqSets == null)   return false;
  for (  String[] reqSet : reqSets) {
    boolean takesPart=false, oneSet=false;
    for (int j=0; j < reqSet.length; j++) {
      if (reqSet[j].equalsIgnoreCase(field))       takesPart=true;
 else       if (BibtexDatabase.getResolvedField(reqSet[j],entry,database) != null)       oneSet=true;
    }
    if (takesPart && oneSet)     return true;
  }
  return false;
}
