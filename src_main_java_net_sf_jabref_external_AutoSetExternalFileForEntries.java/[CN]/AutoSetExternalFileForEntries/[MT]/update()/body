{
  if (!goOn) {
    return;
  }
  panel.output(Localization.lang("Finished synchronizing %0 links. Entries changed%c %1.",new String[]{fieldName.toUpperCase(),String.valueOf(entriesChanged)}));
  panel.frame().setProgressBarVisible(false);
  if (entriesChanged > 0) {
    panel.markBaseChanged();
  }
}
