{
  String url=JOptionPane.showInputDialog(Globals.lang("Enter download URL"));
  if (url == null)   return;
  try {
    installFromURL(new URL(url));
  }
 catch (  MalformedURLException e) {
    JOptionPane.showMessageDialog(frame,Globals.lang("Invalid URL"),Globals.lang("Plugin installer"),JOptionPane.ERROR_MESSAGE);
  }
}
