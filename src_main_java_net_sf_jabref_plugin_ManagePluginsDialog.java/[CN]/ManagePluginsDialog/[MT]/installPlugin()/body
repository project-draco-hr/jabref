{
  String filename=FileDialogs.getNewFile(frame,new File(System.getProperty("user.home")),".jar",JFileChooser.OPEN_DIALOG,false);
  if (filename == null) {
    return;
  }
  File f=new File(filename);
  if (!f.exists()) {
    JOptionPane.showMessageDialog(frame,Globals.lang("File not found") + ".",Globals.lang("Plugin installer"),JOptionPane.ERROR_MESSAGE);
  }
 else {
    installFromFile(f);
  }
}
