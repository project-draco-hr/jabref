{
  int row=getSelectedRow();
  if (row >= 0) {
    FileListEntry entry=tableModel.getEntry(row);
    try {
      ExternalFileType type=ExternalFileTypes.getInstance().getExternalFileTypeByName(entry.type.getName());
      JabRefDesktop.openExternalFileAnyFormat(metaData,entry.link,type != null ? type : entry.type);
    }
 catch (    IOException e) {
      LOGGER.warn("Cannot open selected file.",e);
    }
  }
}
