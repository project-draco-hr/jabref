{
  try (Connection conn=this.connectToDB(databaseStrings);Statement statement=SQLUtil.queryAllFromTable(conn,"jabref_database");ResultSet rs=statement.getResultSet()){
    Vector<String> v;
    Vector<Vector<String>> matrix=new Vector<>();
    dbNames.clear();
    v=new Vector<>();
    v.add(Localization.lang("< CREATE NEW DATABASE >"));
    matrix.add(v);
    while (rs.next()) {
      v=new Vector<>();
      v.add(rs.getString("database_name"));
      matrix.add(v);
      dbNames.add(rs.getString("database_name"));
    }
    return matrix;
  }
 }
