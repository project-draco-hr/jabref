{
  if (!StringUtil.isNullOrEmpty(preamble)) {
    try {
      getWriter().write(Globals.NEWLINE);
      getWriter().write(PREAMBLE_PREFIX + "{");
      getWriter().write(preamble);
      getWriter().write('}' + Globals.NEWLINE);
    }
 catch (    IOException e) {
      throw new SaveException(e);
    }
  }
}
