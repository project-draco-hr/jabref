{
  int x=line.getPosX();
  int y=line.getPosY();
  if ((x > borders[0] - 10) && (x < borders[1] + 10) && (y > borders[2] - 10)&& (y < borders[3] + 10)) {
    if (line.getVisible()) {
      g.setFont(line.getFont());
      g.setColor(line.getColor());
      g.drawString(line.getText(),line.getPosX(),line.getPosY());
    }
  }
}
