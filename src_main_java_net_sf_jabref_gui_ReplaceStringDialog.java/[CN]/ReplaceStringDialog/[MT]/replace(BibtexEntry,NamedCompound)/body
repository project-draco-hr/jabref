{
  int counter=0;
  if (allFields()) {
    for (    String s : be.getAllFields()) {
      if (!s.equals(BibtexEntry.KEY_FIELD)) {
        counter+=replaceField(be,s,ce);
      }
    }
  }
 else {
    for (    String fld : flds) {
      if (!fld.equals(BibtexEntry.KEY_FIELD)) {
        counter+=replaceField(be,fld,ce);
      }
    }
  }
  return counter;
}
