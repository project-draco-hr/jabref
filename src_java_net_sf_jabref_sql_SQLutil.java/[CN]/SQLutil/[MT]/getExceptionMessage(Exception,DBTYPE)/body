{
  String errorMessage=null;
switch (dbtype) {
case MYSQL:
    errorMessage=getExceptionMessage_MySQL(ex);
  break;
case POSTGRESQL:
errorMessage=getExceptionMessage_MySQL(ex);
break;
case DERBY:
errorMessage=getExceptionMessage_MySQL(ex);
break;
default :
errorMessage=Globals.lang("Could not determine exception message.");
break;
}
return errorMessage;
}
