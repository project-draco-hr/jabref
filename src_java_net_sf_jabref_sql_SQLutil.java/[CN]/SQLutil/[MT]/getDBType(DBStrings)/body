{
  DBTYPE dbtype=null;
  String srvtype=dbstrings.getServerType();
  if (srvtype.equalsIgnoreCase("mysql")) {
    dbtype=DBTYPE.MYSQL;
  }
 else   if (srvtype.equalsIgnoreCase("derby")) {
    dbtype=DBTYPE.DERBY;
  }
 else   if (srvtype.equalsIgnoreCase("postgresql")) {
    dbtype=DBTYPE.POSTGRESQL;
  }
  return dbtype;
}
