{
  if (fields == null) {
    fields=new ArrayList<String>();
  }
 else {
    fields.clear();
  }
  uniqueInsert(fields,BibtexFields.getAllFieldNames());
  fieldstr="";
  for (int i=0; i < fields.size(); i++) {
    if (i > 0)     fieldstr=fieldstr + ", ";
    fieldstr=fieldstr + fields.get(i);
  }
}
