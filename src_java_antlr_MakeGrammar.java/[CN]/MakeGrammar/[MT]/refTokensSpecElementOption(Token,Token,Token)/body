{
  TokenSymbol ts=(TokenSymbol)grammar.tokenManager.getTokenSymbol(tok.getText());
  if (ts == null) {
    tool.panic("cannot find " + tok.getText() + "in tokens {...}");
  }
  if (option.getText().equals("AST")) {
    ts.setASTNodeType(value.getText());
  }
 else {
    grammar.tool.error("invalid tokens {...} element option:" + option.getText(),grammar.getFilename(),option.getLine());
  }
}
