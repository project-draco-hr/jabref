{
  if (entry == null) {
    return;
  }
  String fieldValue=entry.getField(fieldName);
  if (fieldValue != null) {
    StringTokenizer tok=new StringTokenizer(fieldValue,Globals.SEPARATING_CHARS);
    while (tok.hasMoreTokens()) {
      String word=tok.nextToken();
      addWordToIndex(word);
    }
  }
}
