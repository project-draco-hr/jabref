{
  if (!isVisible()) {
    Util.placeDialog(this,frame);
    setVisible(true);
    content.reset();
    forward.setEnabled(false);
    back.setEnabled(false);
  }
 else {
    setVisible(true);
    forward.setEnabled(false);
    back.setEnabled(true);
  }
  content.setPage(url);
  content.requestFocus();
}
