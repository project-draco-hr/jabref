{
  String msg=fmt.format(record);
  logOutput.add(msg);
  if (logOutput.size() < ErrorConsole.MAXLOGLINES) {
    logOutputCache=logOutputCache + msg;
  }
 else {
    logOutputCacheRefreshNeeded=true;
    while (logOutput.size() > ErrorConsole.MAXLOGLINES) {
      logOutput.remove(0);
    }
  }
  logOutputCacheRefreshNeeded=true;
}
