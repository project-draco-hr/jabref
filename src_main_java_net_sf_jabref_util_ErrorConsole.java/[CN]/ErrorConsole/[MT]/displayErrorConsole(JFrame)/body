{
  JTabbedPane tabbed=new JTabbedPane();
  addTextArea(tabbed,Globals.lang("Output"),getOutput(),null);
  addTextArea(tabbed,Globals.lang("Exceptions"),getErrorMessages(),Globals.lang("No exceptions have ocurred."));
  addTextArea(tabbed,Globals.lang("Log"),getLog(),null);
  tabbed.setPreferredSize(new Dimension(500,500));
  JOptionPane.showMessageDialog(parent,tabbed,Globals.lang("Program output"),JOptionPane.ERROR_MESSAGE);
}
