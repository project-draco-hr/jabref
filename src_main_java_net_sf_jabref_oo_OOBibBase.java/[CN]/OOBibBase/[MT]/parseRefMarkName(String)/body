{
  ArrayList<String> keys=new ArrayList<>();
  Matcher m=citePattern.matcher(name);
  if (m.find()) {
    String[] keystring=m.group(2).split(",");
    for (    String aKeystring : keystring) {
      if (!keys.contains(aKeystring)) {
        keys.add(aKeystring);
      }
    }
  }
  return keys;
}
