{
  String word=wordTf.getText().trim();
  if (wordSet.contains(word)) {
    wordSet.remove(word);
    metaData.putData(Globals.SELECTOR_META_PREFIX + currentField,new Vector<String>(wordSet));
    fillWordSelector();
    frame.basePanel().markNonUndoableBaseChanged();
    wordTf.setText("");
    wordTf.requestFocus();
  }
}
