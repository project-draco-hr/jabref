{
  tableRows.clear();
  String[] names=JabRefPreferences.getInstance().getStringArray("xmpPrivacyFilters");
  for (  String name : names) {
    tableRows.add(name);
  }
  rowCount=tableRows.size() + 5;
  privacyFilterCheckBox.setSelected(JabRefPreferences.getInstance().getBoolean("useXmpPrivacyFilter"));
}
