{
  int[] rows=table.getSelectedRows();
  if (rows.length == 0)   return;
  for (int i=rows.length - 1; i >= 0; i--) {
    if (rows[i] < tableRows.size()) {
      tableRows.remove(rows[i]);
    }
  }
  rowCount-=rows.length;
  if (rows.length > 1)   table.clearSelection();
  table.revalidate();
  table.repaint();
  tableChanged=true;
}
