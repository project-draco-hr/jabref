{
  if (!s.startsWith(ID))   throw new Exception("Internal error: ExplicitGroup cannot be created from \"" + s + "\". "+ "Please report this on www.sf.net/projects/jabref");
  QuotedStringTokenizer tok=new QuotedStringTokenizer(s.substring(ID.length()),SEPARATOR,QUOTE_CHAR);
switch (version) {
case 0:
case 1:
case 2:
    ExplicitGroup newGroup=new ExplicitGroup(tok.nextToken(),db);
  BibtexEntry[] entries;
while (tok.hasMoreTokens()) {
  entries=db.getEntriesByKey(Util.unquote(tok.nextToken(),QUOTE_CHAR));
  for (int i=0; i < entries.length; ++i)   newGroup.m_entries.add(entries[i]);
}
return newGroup;
default :
throw new UnsupportedVersionException("ExplicitGroup",version);
}
}
