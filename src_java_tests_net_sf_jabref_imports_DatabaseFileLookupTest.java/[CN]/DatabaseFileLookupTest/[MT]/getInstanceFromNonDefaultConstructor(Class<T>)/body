{
  Constructor<?>[] constructors=targetClass.getDeclaredConstructors();
  constructors=orderByParamCount(constructors);
  for (int i=0; i < constructors.length; i++) {
    Constructor<?> constructor=constructors[i];
    constructor.setAccessible(true);
    Class<?>[] parameterTypes=constructor.getParameterTypes();
    try {
      @SuppressWarnings("unchecked") T instance=(T)constructor.newInstance(new Object[parameterTypes.length]);
      return instance;
    }
 catch (    Exception ignored) {
    }
    Object[] arguments=createArguments(parameterTypes,targetClass);
    if (arguments == null) {
      continue;
    }
    try {
      @SuppressWarnings("unchecked") T instance=(T)constructor.newInstance(arguments);
      return instance;
    }
 catch (    Exception e) {
      continue;
    }
  }
  return null;
}
