{
  super(manager,GUIGlobals.saveIconFile,Globals.lang("File changed"));
  this.panel=panel;
  this.frame=frame;
  this.manager=manager;
  JPanel main=new JPanel();
  main.setLayout(new BorderLayout());
  JLabel lab=new JLabel("<html><center>Your file has<BR>been modified<BR>by another process!</center></html>",JLabel.CENTER);
  main.add(lab,BorderLayout.CENTER);
  main.add(test,BorderLayout.SOUTH);
  main.setBorder(BorderFactory.createEmptyBorder(1,1,1,1));
  add(main,BorderLayout.CENTER);
  test.addActionListener(this);
}
