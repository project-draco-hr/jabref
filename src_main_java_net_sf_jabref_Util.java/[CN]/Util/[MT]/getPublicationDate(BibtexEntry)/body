{
  Object o=entry.getField("year");
  if (o == null)   return null;
  String year=toFourDigitYear(o.toString());
  o=entry.getField("month");
  if (o != null) {
    int month=Util.getMonthNumber(o.toString());
    if (month != -1) {
      return year + "-" + (month + 1 < 10 ? "0" : "")+ (month + 1);
    }
  }
  return year;
}
