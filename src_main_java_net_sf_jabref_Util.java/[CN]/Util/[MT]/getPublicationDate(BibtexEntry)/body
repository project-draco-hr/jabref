{
  Object o=entry.getField("year");
  if (o == null)   return null;
  String year=toFourDigitYear(o.toString());
  o=entry.getField("month");
  if (o != null) {
    MonthUtil.Month month=MonthUtil.getMonth(o.toString());
    if (month.isValid()) {
      return year + "-" + month.twoDigitNumber;
    }
  }
  return year;
}
