{
  StringBuffer sb=new StringBuffer();
  char c;
  boolean quoted=false;
  for (int i=0; i < s.length(); ++i) {
    c=s.charAt(i);
    if (quoted) {
      if (c != '\n') {
        sb.append(c);
      }
      quoted=false;
    }
 else     if (c != quoteChar) {
      sb.append(c);
    }
 else {
      quoted=true;
    }
  }
  return sb.toString();
}
