{
  StringBuilder sb=new StringBuilder(ACMPortalFetcher.START_URL).append(ACMPortalFetcher.SEARCH_URL_PART).append(terms.replaceAll(" ","%20")).append(ACMPortalFetcher.SEARCH_URL_PART_II);
  if (acmOrGuide) {
    sb.append("ACM");
  }
 else {
    sb.append("GUIDE");
  }
  sb.append(ACMPortalFetcher.END_URL);
  return sb.toString();
}
