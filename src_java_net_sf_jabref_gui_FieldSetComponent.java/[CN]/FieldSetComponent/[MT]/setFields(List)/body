{
  DefaultListModel newListModel=new DefaultListModel();
  for (Iterator i=fields.iterator(); i.hasNext(); )   newListModel.addElement(i.next());
  this.listModel=newListModel;
  for (Iterator i=modelListeners.iterator(); i.hasNext(); )   newListModel.addListDataListener((ListDataListener)i.next());
  list.setModel(newListModel);
}
