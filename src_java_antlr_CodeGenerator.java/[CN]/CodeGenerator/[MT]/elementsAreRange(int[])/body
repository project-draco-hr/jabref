{
  if (elems.length == 0) {
    return false;
  }
  int begin=elems[0];
  int end=elems[elems.length - 1];
  if (elems.length <= 2) {
    return false;
  }
  if (end - begin + 1 > elems.length) {
    return false;
  }
  int v=begin + 1;
  for (int i=1; i < elems.length - 1; i++) {
    if (v != elems[i]) {
      return false;
    }
    v++;
  }
  return true;
}
