{
  if (actions.get(_command) == null)   Util.pr("No action defined for '" + _command + "'");
 else {
    Object o=actions.get(_command);
    try {
      if (o instanceof BaseAction)       ((BaseAction)o).action();
 else {
        Worker wrk=((AbstractWorker)o).getWorker();
        CallBack clb=((AbstractWorker)o).getCallBack();
        ((AbstractWorker)o).init();
        wrk.run();
        clb.update();
      }
    }
 catch (    Throwable ex) {
      frame.unblock();
      ex.printStackTrace();
    }
  }
}
