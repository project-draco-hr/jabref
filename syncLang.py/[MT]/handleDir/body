def handleDir(lists, dirname, fnames):
    (keyList, notTermList) = lists
    for file in fnames:
        if ((len(file) > 6) and (file[(len(file) - 5):len(file)] == '.java')):
            fl = open(((dirname + os.sep) + file))
            lines = fl.read()
            fl.close()
            handleJavaCode(((dirname + os.sep) + file), lines, keyList, notTermList)
