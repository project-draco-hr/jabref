{
  BibtexEntry e1=(BibtexEntry)o1, e2=(BibtexEntry)o2;
  for (int i=0; i < fields.length; i++) {
    Object val1=e1.getField(fields[i]), val2=e2.getField(fields[i]);
    if (val1 == null) {
      if (val2 != null)       return 1;
    }
 else {
      if (val2 == null)       return -1;
    }
  }
  return 0;
}
