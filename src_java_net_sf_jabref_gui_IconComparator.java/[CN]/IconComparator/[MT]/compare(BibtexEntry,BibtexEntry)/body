{
  for (int i=0; i < fields.length; i++) {
    String val1=e1.getField(fields[i]), val2=e2.getField(fields[i]);
    int v1=0;
    try {
      v1=Integer.valueOf(val1);
    }
 catch (    Exception e) {
    }
    int v2=0;
    try {
      v2=Integer.valueOf(val2);
    }
 catch (    Exception e) {
    }
    if (v1 < v2) {
      return -1;
    }
 else     if (v1 > v2) {
      return 1;
    }
 else {
      return 0;
    }
  }
  return 0;
}
