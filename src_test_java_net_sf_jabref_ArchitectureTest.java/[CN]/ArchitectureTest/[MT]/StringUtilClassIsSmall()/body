{
  Path path=Paths.get("src","main","java",StringUtil.class.getName().replace('.','/') + ".java");
  int lineCount=Files.readAllLines(path,StandardCharsets.UTF_8).size();
  Assert.assertTrue("StringUtil increased in size. " + "We try to keep this class as small as possible. " + "Thus think twice if you add something to StringUtil.",lineCount <= 680);
}
