{
  if (column == 0) {
    return Localization.lang("type");
  }
 else   if (column == 1) {
    return Localization.lang("key");
  }
 else   if (column == 2) {
    return Localization.lang("value");
  }
 else   if (column == 3) {
    return Localization.lang("default");
  }
 else {
    return "n/a";
  }
}
