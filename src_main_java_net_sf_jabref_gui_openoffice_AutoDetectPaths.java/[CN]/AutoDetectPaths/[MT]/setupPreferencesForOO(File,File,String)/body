{
  preferences.setExecutablePath(new File(inUsr,sofficeName).getPath());
  File jurt=fileSearch.findFileInDir(rootDir,"jurt.jar");
  if (fileSearchCancelled) {
    return false;
  }
  if (jurt == null) {
    return false;
  }
 else {
    preferences.setJarsPath(jurt.getPath());
    return true;
  }
}
