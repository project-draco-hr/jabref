{
switch (mode) {
case Incremental:
    updateIncremental();
  break;
case Float:
updateFloat();
break;
case Filter:
case LiveFilter:
updateFilter();
break;
case ResultsInDialog:
updateResultsInDialog();
break;
case Global:
updateGlobal();
break;
}
if (mode != SearchMode.Incremental) {
frame.basePanel().output(Globals.lang("Searched database. Number of hits") + ": " + hits);
}
}
