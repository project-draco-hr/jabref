{
  System.out.println(Globals.lang("Opening") + ": " + name);
  try {
    File file=new File(name);
    String encoding=Globals.prefs.get("defaultEncoding");
    ParserResult pr=ImportFormatReader.loadDatabase(file,encoding);
    pr.setFile(file);
    if (pr.hasWarnings()) {
      String[] warn=pr.warnings();
      for (int i=0; i < warn.length; i++)       System.out.println(Globals.lang("Warning") + ": " + warn[i]);
    }
    return pr;
  }
 catch (  Throwable ex) {
    System.err.println(Globals.lang("Error opening file") + ": " + ex.getMessage());
  }
  return null;
}
