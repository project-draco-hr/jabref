{
  if (radioButtonXmp.isSelected())   return XMP;
 else   if (radioButtonPDFcontent.isSelected())   return CONTENT;
 else   if (radioButtonMrDlib.isSelected())   return MRDLIB;
 else   if (radioButtonNoMeta.isSelected())   return NOMETA;
 else   if (radioButtononlyAttachPDF.isSelected())   return ONLYATTACH;
 else   if (radioButtonUpdateEmptyFields.isSelected())   return UPDATEEMPTYFIELDS;
 else   throw new IllegalStateException();
}
