{
  super(name,context);
  this.searchExpression=searchExpression;
  this.caseSensitive=caseSensitive;
  this.regExp=regExp;
  AST ast;
  try {
    SearchExpressionParser parser=new SearchExpressionParser(new SearchExpressionLexer(new StringReader(this.searchExpression)));
    parser.caseSensitive=this.caseSensitive;
    parser.regex=this.regExp;
    parser.searchExpression();
    ast=parser.getAST();
  }
 catch (  Exception e) {
    ast=null;
  }
  this.ast=ast;
  if (this.ast != null) {
    searchRule=this;
  }
 else {
    if (this.regExp) {
      searchRule=new RegExpSearchRule(this.caseSensitive);
    }
 else {
      searchRule=new SimpleSearchRule(this.caseSensitive);
    }
  }
}
