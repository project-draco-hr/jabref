{
  if (!(o instanceof SearchGroup)) {
    return false;
  }
  SearchGroup other=(SearchGroup)o;
  return name.equals(other.name) && searchExpression.equals(other.searchExpression) && (caseSensitive == other.caseSensitive)&& (regExp == other.regExp)&& (getHierarchicalContext() == other.getHierarchicalContext());
}
