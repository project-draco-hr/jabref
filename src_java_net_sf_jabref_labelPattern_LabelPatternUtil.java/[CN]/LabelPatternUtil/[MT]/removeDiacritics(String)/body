{
  if (content == null)   return null;
  content=content.replaceAll("\\{\\\\\"([a-zA-Z])\\}","$1e");
  content=content.replaceAll("\\\\\"\\{([a-zA-Z])\\}","$1e");
  content=content.replaceAll("\\\\\"([a-zA-Z])","$1e");
  content=content.replaceAll("\\{\\\\.([a-zA-Z])\\}","$1");
  content=content.replaceAll("\\\\.\\{([a-zA-Z])\\}","$1");
  content=content.replaceAll("\\\\.([a-zA-Z])","$1");
  return content;
}
