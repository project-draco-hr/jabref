{
  boolean _isUnique=true;
  BibtexEntry _entry;
  int _dbSize=_db.getEntryCount();
  for (int i=0; i < _dbSize; i++) {
    _entry=_db.getEntryById(String.valueOf(i));
    if (_entry.getField(BibtexFields.KEY_FIELD).equals(label)) {
      _isUnique=false;
      break;
    }
  }
  return _isUnique;
}
