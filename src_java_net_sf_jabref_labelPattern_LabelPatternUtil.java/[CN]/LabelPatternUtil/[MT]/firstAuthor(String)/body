{
  String author="";
  String[] tokens=authorField.split("\\band\\b");
  if (tokens.length > 0) {
    if (tokens[0].indexOf(",") > 0)     tokens[0]=ImportFormatReader.fixAuthor(tokens[0]);
    String[] firstAuthor=tokens[0].replaceAll("\\s+"," ").split(" ");
    author+=firstAuthor[firstAuthor.length - 1];
  }
  return author;
}
