{
  super(preferences);
  this.fieldNames=Objects.requireNonNull(fieldNames);
  this.lastNameOnlyAndSeparationBySpace=lastNameOnlyAndSeparationBySpace;
  if (preferences.getOnlyCompleteFirstLast()) {
    autoCompFF=true;
    autoCompLF=false;
  }
 else   if (preferences.getOnlyCompleteLastFirst()) {
    autoCompFF=false;
    autoCompLF=true;
  }
 else {
    autoCompFF=true;
    autoCompLF=true;
  }
  autoCompFirstnameMode=preferences.getFirstnameMode() != null ? preferences.getFirstnameMode() : AutoCompleteFirstNameMode.BOTH;
}
