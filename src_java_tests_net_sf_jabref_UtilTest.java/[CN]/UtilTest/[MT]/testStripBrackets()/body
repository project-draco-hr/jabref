{
  assertEquals("foo",Util.stripBrackets("[foo]"));
  assertEquals("[foo]",Util.stripBrackets("[[foo]]"));
  assertEquals("foo",Util.stripBrackets("foo]"));
  assertEquals("foo",Util.stripBrackets("[foo"));
  assertEquals("",Util.stripBrackets(""));
  assertEquals("",Util.stripBrackets("[]"));
  assertEquals("",Util.stripBrackets("["));
  assertEquals("",Util.stripBrackets("]"));
  assertEquals("f[]f",Util.stripBrackets("f[]f"));
  try {
    Util.stripBrackets(null);
    fail();
  }
 catch (  NullPointerException npe) {
  }
}
