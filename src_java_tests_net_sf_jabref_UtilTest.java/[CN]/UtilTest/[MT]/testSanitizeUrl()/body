{
  try {
    assertEquals("http://www.vg.no",Util.sanitizeUrl("http://www.vg.no"));
    assertEquals("http://www.vg.no/fil%20e.html",Util.sanitizeUrl("http://www.vg.no/fil e.html"));
    assertEquals("http://www.vg.no/fil%20e.html",Util.sanitizeUrl("http://www.vg.no/fil%20e.html"));
    assertEquals("http://www.vg.no/fil%20e.html",Util.sanitizeUrl("www.vg.no/fil%20e.html"));
  }
 catch (  URISyntaxException e) {
    e.printStackTrace();
  }
}
