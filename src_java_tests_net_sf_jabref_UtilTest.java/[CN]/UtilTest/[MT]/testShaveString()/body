{
  assertEquals(null,Util.shaveString(null));
  assertEquals("",Util.shaveString(""));
  assertEquals("aaa",Util.shaveString("   aaa\t\t\n\r"));
  assertEquals("a",Util.shaveString("  {a}    "));
  assertEquals("a",Util.shaveString("  \"a\"    "));
  assertEquals("{a}",Util.shaveString("  {{a}}    "));
  assertEquals("{a}",Util.shaveString("  \"{a}\"    "));
  assertEquals("\"{a\"}",Util.shaveString("  \"{a\"}    "));
}
