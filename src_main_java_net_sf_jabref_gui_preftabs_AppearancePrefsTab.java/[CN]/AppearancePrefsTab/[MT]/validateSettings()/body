{
  if (!validateIntegerField(Localization.lang("Menu and label font size"),fontSize.getText(),Localization.lang("Changed font settings"))) {
    return false;
  }
  if (!validateIntegerField(Localization.lang("Table row height padding"),rowPadding.getText(),Localization.lang("Changed table appearance settings"))) {
    return false;
  }
  return true;
}
