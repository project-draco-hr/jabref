{
  try {
    Integer.parseInt(fieldValue);
  }
 catch (  NumberFormatException ex) {
    JOptionPane.showMessageDialog(null,Localization.lang("You must enter an integer value in the text field for") + " '" + Localization.lang(fieldName)+ "'",Localization.lang(errorTitle),JOptionPane.ERROR_MESSAGE);
    return false;
  }
  return true;
}
