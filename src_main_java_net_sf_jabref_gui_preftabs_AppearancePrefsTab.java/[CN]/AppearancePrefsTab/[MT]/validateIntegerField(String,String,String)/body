{
  try {
    Integer.parseInt(fieldValue);
  }
 catch (  NumberFormatException ex) {
    JOptionPane.showMessageDialog(null,Globals.lang("You must enter an integer value in the text field for") + " '" + Globals.lang(fieldName)+ "'",Globals.lang(errorTitle),JOptionPane.ERROR_MESSAGE);
    return false;
  }
  return true;
}
