{
  XBookmarksSupplier xBookmarksSupplier=(XBookmarksSupplier)UnoRuntime.queryInterface(XBookmarksSupplier.class,xCurrentComponent);
  if (xBookmarksSupplier.getBookmarks().hasByName(name)) {
    Object o=xBookmarksSupplier.getBookmarks().getByName(name);
    XTextContent bm=(XTextContent)UnoRuntime.queryInterface(XTextContent.class,o);
    text.removeTextContent(bm);
  }
}
