{
  String fullName="";
  if ((givenName != null) && !givenName.equals("")) {
    fullName+=givenName + " ";
  }
  if ((middleName != null) && !middleName.equals("")) {
    fullName+=middleName + " ";
  }
  if ((surname != null) && !surname.equals("")) {
    fullName+=surname;
  }
  return fullName.trim();
}
