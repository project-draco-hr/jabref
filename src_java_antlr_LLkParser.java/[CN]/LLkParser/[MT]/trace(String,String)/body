{
  traceIndent();
  System.out.print(ee + rname + ((inputState.guessing > 0) ? "; [guessing]" : "; "));
  for (int i=1; i <= k; i++) {
    if (i != 1) {
      System.out.print(", ");
    }
    if (LT(i) != null) {
      System.out.print("LA(" + i + ")=="+ LT(i).getText());
    }
 else {
      System.out.print("LA(" + i + ")==null");
    }
  }
  System.out.println("");
}
