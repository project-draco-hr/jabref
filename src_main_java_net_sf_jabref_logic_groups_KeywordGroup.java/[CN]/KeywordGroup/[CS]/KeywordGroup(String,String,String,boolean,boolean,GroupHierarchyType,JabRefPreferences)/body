{
  super(name,context);
  this.searchField=searchField;
  this.searchExpression=searchExpression;
  this.caseSensitive=caseSensitive;
  this.regExp=regExp;
  if (this.regExp) {
    compilePattern();
  }
  this.jabRefPreferences=jabRefPreferences;
  this.searchWords=EntryUtil.getStringAsWords(searchExpression);
}
