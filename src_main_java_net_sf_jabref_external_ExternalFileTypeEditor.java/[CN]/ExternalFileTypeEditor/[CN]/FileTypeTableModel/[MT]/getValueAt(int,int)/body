{
  ExternalFileType type=fileTypes.get(rowIndex);
switch (columnIndex) {
case 0:
    return type.getIcon();
case 1:
  return type.getName();
case 2:
return type.getExtension();
case 3:
return type.getMimeType();
default :
if (!type.getOpenWithApplication().isPresent()) {
return "";
}
 else {
return type.getOpenWithApplication().get();
}
}
}
