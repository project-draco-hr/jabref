{
  StringBuilder res=new StringBuilder();
  if (abbr) {
    res.append(firstAbbr == null ? "" : firstAbbr + ' ').append(getLastOnly());
  }
 else {
    res.append(firstPart == null ? "" : firstPart + ' ').append(getLastOnly());
  }
  if (jrPart != null) {
    res.append(", ").append(jrPart);
  }
  return res.toString();
}
