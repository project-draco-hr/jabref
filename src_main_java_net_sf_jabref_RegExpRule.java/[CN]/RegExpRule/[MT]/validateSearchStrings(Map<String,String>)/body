{
  int score=0;
  String searchString=searchStrings.values().iterator().next();
  int flags=0;
  if (!m_caseSensitiveSearch)   flags=Pattern.CASE_INSENSITIVE;
  try {
    Pattern pattern=Pattern.compile(searchString,flags);
  }
 catch (  PatternSyntaxException ex) {
    return false;
  }
  return true;
}
