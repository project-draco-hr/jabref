{
  if (tf.getText().trim().length() == 0)   return;
  final ImportInspectionDialog dialog=new ImportInspectionDialog(frame,frame.basePanel(),BibtexFields.DEFAULT_INSPECTION_FIELDS,fetcher.getTitle(),false);
  dialog.addCallBack(fetcher);
  Util.placeDialog(dialog,frame);
  dialog.setVisible(true);
  new Thread(new Runnable(){
    public void run(){
      if (fetcher.processQuery(tf.getText().trim(),dialog,frame)) {
        dialog.entryListComplete();
      }
 else {
        dialog.dispose();
      }
    }
  }
).start();
}
