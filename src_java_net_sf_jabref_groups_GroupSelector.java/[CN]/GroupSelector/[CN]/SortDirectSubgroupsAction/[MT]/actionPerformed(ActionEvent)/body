{
  final GroupTreeNode node=getNodeToUse();
  final UndoableModifySubtree undo=new UndoableModifySubtree(GroupSelector.this,getGroupTreeRoot(),node,Globals.lang("sort subgroups"));
  groupsTree.sort(node,false);
  panel.undoManager.addEdit(undo);
  panel.markBaseChanged();
  frame.output(Globals.lang("Sorted immediate subgroups."));
}
