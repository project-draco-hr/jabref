{
  if (col == 0) {
    return OTHER;
  }
  if (getIconTypeForColumn(col) != null) {
    return ICON_COL;
  }
  BibtexEntryType type=(db.getEntryById(getNameFromNumber(row))).getType();
  if (columns[col - padleft].equals(GUIGlobals.KEY_FIELD) || type.isRequired(columns[col - padleft])) {
    return REQUIRED;
  }
  if (type.isOptional(columns[col - padleft])) {
    return OPTIONAL;
  }
  return OTHER;
}
