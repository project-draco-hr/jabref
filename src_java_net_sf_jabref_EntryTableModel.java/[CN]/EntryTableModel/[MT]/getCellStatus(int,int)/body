{
  if (col == 0)   return OTHER;
  BibtexEntryType type=(db.getEntryById(getNameFromNumber(row))).getType();
  if (columns[col - 1].equals(GUIGlobals.KEY_FIELD) || type.isRequired(columns[col - PADLEFT]))   return REQUIRED;
  if (type.isOptional(columns[col - PADLEFT]))   return OPTIONAL;
  return OTHER;
}
