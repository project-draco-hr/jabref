{
  if (!Globals.prefs.getBoolean("allowTableEditing"))   return false;
  if (col < padleft) {
    return false;
  }
  try {
    if (!getFieldName(col).equals(GUIGlobals.TYPE_HEADER)) {
      return true;
    }
 else {
      return false;
    }
  }
 catch (  NullPointerException ex) {
    return false;
  }
}
