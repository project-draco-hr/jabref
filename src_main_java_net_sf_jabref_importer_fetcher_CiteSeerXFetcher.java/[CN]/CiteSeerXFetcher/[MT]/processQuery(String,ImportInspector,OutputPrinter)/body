{
  stopFetching=false;
  try {
    List<String> citations=getCitations(query);
    for (    String citation : citations) {
      if (stopFetching) {
        break;
      }
      BibEntry entry=getSingleCitation(citation);
      if (entry != null) {
        inspector.addEntry(entry);
      }
    }
    return true;
  }
 catch (  IOException e) {
    LOGGER.warn("Could not download",e);
    return false;
  }
}
