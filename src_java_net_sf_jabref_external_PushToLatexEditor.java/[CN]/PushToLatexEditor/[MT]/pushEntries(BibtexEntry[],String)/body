{
  couldNotCall=false;
  String led=Globals.prefs.get("latexEditorPath");
  try {
    StringBuffer toSend=new StringBuffer("-i \\").append(Globals.prefs.get("citeCommand")).append("{").append(keyString).append("}");
    Runtime.getRuntime().exec(led + " " + toSend.toString());
  }
 catch (  IOException excep) {
    couldNotCall=true;
    excep.printStackTrace();
  }
}
