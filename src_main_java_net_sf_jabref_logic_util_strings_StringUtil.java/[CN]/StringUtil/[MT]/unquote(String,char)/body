{
  StringBuilder result=new StringBuilder();
  char c;
  boolean quoted=false;
  for (int i=0; i < toUnquote.length(); ++i) {
    c=toUnquote.charAt(i);
    if (quoted) {
      if (c != '\n') {
        result.append(c);
      }
      quoted=false;
    }
 else     if (c != quoteChar) {
      result.append(c);
    }
 else {
      quoted=true;
    }
  }
  return result.toString();
}
