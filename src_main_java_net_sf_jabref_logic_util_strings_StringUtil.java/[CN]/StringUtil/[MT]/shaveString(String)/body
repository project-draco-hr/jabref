{
  if ((toShave == null) || (toShave.length() == 0)) {
    return toShave;
  }
  toShave=toShave.trim();
  if ((toShave.startsWith("{") && toShave.endsWith("}")) || (toShave.startsWith("\"") && toShave.endsWith("\""))) {
    return toShave.substring(1,toShave.length() - 1);
  }
  return toShave;
}
