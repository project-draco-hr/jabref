{
  if ((toShave == null) || (toShave.isEmpty())) {
    return toShave;
  }
  toShave=toShave.trim();
  if (((toShave.charAt(0) == '{') && toShave.endsWith("}")) || ((toShave.charAt(0) == '"') && toShave.endsWith("\""))) {
    return toShave.substring(1,toShave.length() - 1);
  }
  return toShave;
}
