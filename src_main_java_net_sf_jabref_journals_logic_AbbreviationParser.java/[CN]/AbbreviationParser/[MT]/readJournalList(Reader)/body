{
  BufferedReader reader=new BufferedReader(in);
  try {
    String line;
    while ((line=reader.readLine()) != null) {
      addLine(line);
    }
  }
 catch (  IOException ex) {
    ex.printStackTrace();
  }
 finally {
    try {
      reader.close();
    }
 catch (    IOException ex2) {
      ex2.printStackTrace();
    }
  }
}
