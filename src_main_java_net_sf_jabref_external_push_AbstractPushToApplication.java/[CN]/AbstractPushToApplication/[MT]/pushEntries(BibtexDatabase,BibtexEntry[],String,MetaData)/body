{
  couldNotCall=false;
  notDefined=false;
  initParameters();
  searchPath=Globals.prefs.get(searchPathPreferenceKey);
  if ((searchPath == null) || searchPath.trim().isEmpty()) {
    notDefined=true;
    return;
  }
  try {
    Runtime.getRuntime().exec(getCommandLine(keyString));
  }
 catch (  IOException excep) {
    couldNotCall=true;
    excep.printStackTrace();
  }
}
