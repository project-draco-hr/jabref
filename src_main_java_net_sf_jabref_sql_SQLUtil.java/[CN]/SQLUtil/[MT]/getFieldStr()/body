{
  String field;
  List<String> fieldNames=new ArrayList<>();
  for (int i=0; i < SQLUtil.getAllFields().size(); i++) {
    field=SQLUtil.allFields.get(i);
    if (SQLUtil.RESERVED_DB_WORDS.contains(field)) {
      field+="_";
    }
    fieldNames.add(field);
  }
  return String.join(", ",fieldNames);
}
