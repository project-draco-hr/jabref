{
  String fieldstr="";
  String field;
  for (int i=0; i < SQLUtil.getAllFields().size(); i++) {
    field=SQLUtil.allFields.get(i);
    if (i > 0) {
      fieldstr=fieldstr + ", ";
    }
    if (SQLUtil.reservedDBWords.contains(field)) {
      field+="_";
    }
    fieldstr=fieldstr + field;
  }
  return fieldstr;
}
