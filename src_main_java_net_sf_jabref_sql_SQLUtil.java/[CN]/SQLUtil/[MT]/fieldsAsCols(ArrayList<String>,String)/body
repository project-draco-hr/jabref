{
  String field;
  ArrayList<String> newFields=new ArrayList<>();
  ListIterator<String> li=fields.listIterator();
  while (li.hasNext()) {
    field=li.next();
    if (SQLUtil.reservedDBWords.contains(field)) {
      field=field + '_';
    }
    newFields.add(field + datatype);
  }
  return String.join(", ",newFields);
}
