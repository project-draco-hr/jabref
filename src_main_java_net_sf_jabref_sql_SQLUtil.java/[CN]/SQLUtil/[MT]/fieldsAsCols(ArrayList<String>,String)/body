{
  String str="";
  String field;
  ListIterator<String> li=fields.listIterator();
  while (li.hasNext()) {
    field=li.next();
    if (SQLUtil.reservedDBWords.contains(field)) {
      field=field + '_';
    }
    str=str + field + datatype;
    if (li.hasNext()) {
      str=str + ", ";
    }
  }
  return str;
}
