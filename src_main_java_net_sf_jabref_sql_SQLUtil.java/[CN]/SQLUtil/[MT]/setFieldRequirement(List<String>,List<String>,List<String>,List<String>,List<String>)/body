{
  String currentField;
  for (int i=0; i < allFields.size(); i++) {
    currentField=allFields.get(i);
    if (reqFields.contains(currentField)) {
      origList.set(i,"req");
    }
 else     if (optFields.contains(currentField)) {
      origList.set(i,"opt");
    }
 else     if (utiFields.contains(currentField)) {
      origList.set(i,"uti");
    }
  }
  return origList;
}
