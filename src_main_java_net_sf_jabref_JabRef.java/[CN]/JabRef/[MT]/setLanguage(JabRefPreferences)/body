{
  String langStr=prefs.get(JabRefPreferences.LANGUAGE);
  String[] parts=langStr.split("_");
  String language;
  String country;
  if (parts.length == 1) {
    language=langStr;
    country="";
  }
 else {
    language=parts[0];
    country=parts[1];
  }
  Globals.setLanguage(language,country);
}
