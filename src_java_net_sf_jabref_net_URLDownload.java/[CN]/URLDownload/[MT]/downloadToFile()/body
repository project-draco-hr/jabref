{
  InputStream input=new BufferedInputStream(con.getInputStream());
  OutputStream output=new BufferedOutputStream(new FileOutputStream(dest));
  try {
    copy(input,output);
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
 finally {
    try {
      input.close();
      output.close();
    }
 catch (    Exception e) {
    }
  }
}
