{
  try {
    updating=true;
    for (    FieldEditor editor : editors.values()) {
      Object content=entry.getField(editor.getFieldName());
      String toSet=(content == null) ? "" : content.toString();
      if (!toSet.equals(editor.getText()))       editor.setText(toSet);
    }
    this.entry=entry;
  }
  finally {
    updating=false;
  }
}
