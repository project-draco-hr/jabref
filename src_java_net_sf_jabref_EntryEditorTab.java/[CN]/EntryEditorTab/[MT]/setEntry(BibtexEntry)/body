{
  try {
    updating=true;
    Iterator i=editors.values().iterator();
    while (i.hasNext()) {
      FieldEditor editor=(FieldEditor)i.next();
      Object content=entry.getField(editor.getFieldName());
      editor.setText((content == null) ? "" : content.toString());
    }
    this.entry=entry;
  }
  finally {
    updating=false;
  }
}
