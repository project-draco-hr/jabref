{
  int i=0;
  List<String> s;
  while (!((s=prefs.getStringList(JabRefPreferences.CUSTOM_IMPORT_FORMAT + i)).isEmpty())) {
    try {
      super.add(new CustomImporter(s));
    }
 catch (    Exception e) {
      LOGGER.warn("Could not load " + s.get(0) + " from preferences. Will ignore.",e);
    }
    i++;
  }
}
