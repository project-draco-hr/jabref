{
  int i=0;
  String[] s;
  while ((s=prefs.getStringArray(JabRefPreferences.CUSTOM_IMPORT_FORMAT + i)) != null) {
    try {
      super.add(new CustomImporter(s));
    }
 catch (    Exception e) {
      LOGGER.warn("Could not load " + s[0] + " from preferences. Will ignore.",e);
    }
    i++;
  }
}
