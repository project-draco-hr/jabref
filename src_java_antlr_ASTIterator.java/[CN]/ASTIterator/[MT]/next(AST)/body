{
  AST t=null;
  AST sibling=null;
  if (cursor == null) {
    return null;
  }
  for (; cursor != null; cursor=cursor.getNextSibling()) {
    if (cursor.getType() == template.getType()) {
      if (cursor.getFirstChild() != null) {
        if (isSubtree(cursor.getFirstChild(),template.getFirstChild())) {
          return cursor;
        }
      }
    }
  }
  return t;
}
