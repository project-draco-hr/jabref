{
  StringBuffer sb=new StringBuffer();
  if (getChildCount() > 0)   sb.append("(");
  sb.append(Util.quote(getGroup().toString(),"(),;",'\\'));
  for (int i=0; i < getChildCount(); ++i) {
    sb.append("," + getChildAt(i).toString());
  }
  if (getChildCount() > 0)   sb.append(")");
  return sb.toString();
}
