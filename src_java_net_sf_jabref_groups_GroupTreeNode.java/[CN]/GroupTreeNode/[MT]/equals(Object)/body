{
  if (!(other instanceof GroupTreeNode))   return false;
  final GroupTreeNode otherNode=(GroupTreeNode)other;
  if (getChildCount() != otherNode.getChildCount())   return false;
  if (!getGroup().equals(otherNode.getGroup()))   return false;
  for (int i=0; i < getChildCount(); ++i) {
    if (!((GroupTreeNode)getChildAt(i)).getGroup().equals(((GroupTreeNode)otherNode.getChildAt(i)).getGroup()))     return false;
  }
  return true;
}
