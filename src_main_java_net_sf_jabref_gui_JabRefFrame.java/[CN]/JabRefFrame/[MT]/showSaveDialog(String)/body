{
  Object[] options={Localization.lang("Save changes"),Localization.lang("Discard changes"),Localization.lang("Return to JabRef")};
  return JOptionPane.showOptionDialog(JabRefFrame.this,Localization.lang("Database '%0' has changed.",filename),Localization.lang("Save before closing"),JOptionPane.YES_NO_CANCEL_OPTION,JOptionPane.WARNING_MESSAGE,null,options,options[2]);
}
