{
  if (pr.toOpenTab()) {
    BasePanel panel=getCurrentBasePanel();
    if (panel == null) {
      addTab(pr.getDatabaseContext(),pr.getEncoding(),raisePanel);
    }
 else {
      List<BibEntry> entries=new ArrayList<>(pr.getDatabase().getEntries());
      addImportedEntries(panel,entries,false);
    }
  }
 else {
    addTab(pr.getDatabaseContext(),pr.getEncoding(),raisePanel);
  }
}
