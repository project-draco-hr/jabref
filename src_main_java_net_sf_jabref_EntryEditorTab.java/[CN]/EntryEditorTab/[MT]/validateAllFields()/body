{
  for (  Map.Entry<String,FieldEditor> stringFieldEditorEntry : editors.entrySet()) {
    FieldEditor ed=stringFieldEditorEntry.getValue();
    ed.updateFontColor();
    ed.setEnabled(true);
    if (((Component)ed).hasFocus()) {
      ed.setActiveBackgroundColor();
    }
 else {
      ed.setValidBackgroundColor();
    }
  }
}
