{
  if (evt.getSource() == btnDefaultAll) {
    Iterator<String> i=textFields.keySet().iterator();
    while (i.hasNext()) {
      String s=i.next();
      JTextField tf=textFields.get(s);
      tf.setText("");
    }
    return;
  }
  JTextField tf=textFields.get(evt.getActionCommand());
  tf.setText("");
}
