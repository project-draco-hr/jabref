{
  if (evt.getSource() == btnDefaultAll) {
    Iterator i=textFields.keySet().iterator();
    while (i.hasNext()) {
      String s=(String)i.next();
      JTextField tf=(JTextField)textFields.get(s);
      tf.setText("");
    }
    return;
  }
  JTextField tf=(JTextField)textFields.get(evt.getActionCommand());
  tf.setText("");
}
