{
  LOGGER.warn("Tokenize \"" + s + '"');
  vcr.clear();
  s=s + '\n';
  int endpos;
  int matched=0;
  StringTokenizer st=new StringTokenizer(s,delimstr);
  while (st.hasMoreTokens()) {
    String tmp=st.nextToken();
    vcr.add(tmp);
    matched++;
    if (matched == limit) {
      endpos=s.lastIndexOf(tmp);
      vcr.add(s.substring(endpos + tmp.length()));
      break;
    }
  }
  return true;
}
