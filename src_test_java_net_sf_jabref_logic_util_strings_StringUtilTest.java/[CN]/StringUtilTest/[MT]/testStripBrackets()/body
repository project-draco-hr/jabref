{
  assertEquals("foo",StringUtil.stripBrackets("[foo]"));
  assertEquals("[foo]",StringUtil.stripBrackets("[[foo]]"));
  assertEquals("foo",StringUtil.stripBrackets("foo]"));
  assertEquals("foo",StringUtil.stripBrackets("[foo"));
  assertEquals("",StringUtil.stripBrackets(""));
  assertEquals("",StringUtil.stripBrackets("[]"));
  assertEquals("",StringUtil.stripBrackets("["));
  assertEquals("",StringUtil.stripBrackets("]"));
  assertEquals("f[]f",StringUtil.stripBrackets("f[]f"));
  try {
    StringUtil.stripBrackets(null);
    fail();
  }
 catch (  NullPointerException ignored) {
  }
}
