{
  if (g.debuggingOutput)   semPreds=new Vector();
  if (g.charVocabulary.size() > 256)   antlrTool.warning(g.getFilename() + ": C++ mode does not support more than 8 bit characters (vocabulary size now: " + g.charVocabulary.size()+ ")");
  setGrammar(g);
  if (!(grammar instanceof LexerGrammar)) {
    antlrTool.panic("Internal error generating lexer");
  }
  genBody(g);
  genInclude(g);
}
