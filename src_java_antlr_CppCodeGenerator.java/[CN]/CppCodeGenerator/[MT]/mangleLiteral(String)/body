{
  String mangled=antlr.Tool.literalsPrefix;
  for (int i=1; i < s.length() - 1; i++) {
    if (!Character.isLetter(s.charAt(i)) && s.charAt(i) != '_') {
      return null;
    }
    mangled+=s.charAt(i);
  }
  if (antlr.Tool.upperCaseMangledLiterals) {
    mangled=mangled.toUpperCase();
  }
  return mangled;
}
