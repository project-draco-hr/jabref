{
  if (declaredASTVariables.contains(el))   return;
  String init=labeledElementASTInit;
  if (el instanceof GrammarAtom && ((GrammarAtom)el).getASTNodeType() != null)   init="Ref" + ((GrammarAtom)el).getASTNodeType() + "("+ labeledElementASTInit+ ")";
  println(node_type + " " + var_name+ "_AST = "+ init+ ";");
  declaredASTVariables.put(el,el);
}
