{
  return Localization.lang("change field %0 of entry %1 from %2 to %3",StringUtil.boldHTML(field),StringUtil.boldHTML(entry.getCiteKeyOptional().orElse(Localization.lang("undefined"))),StringUtil.boldHTML(oldValue,Localization.lang("undefined")),StringUtil.boldHTML(newValue,Localization.lang("undefined")));
}
