{
  for (  String field : fields) {
    String val1=e1.getField(field);
    String val2=e2.getField(field);
    if (val1 == null) {
      if (val2 != null) {
        return 1;
      }
 else {
      }
    }
 else {
      if (val2 == null) {
        return -1;
      }
 else {
        int compareToRes=val1.compareTo(val2);
        if (compareToRes != 0) {
          return compareToRes;
        }
 else {
        }
      }
    }
  }
  return 0;
}
