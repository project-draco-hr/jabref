{
  String dir=panel.metaData().getFileDirectory(fileType.getFieldName());
  if ((dir == null) || !(new File(dir)).exists()) {
    System.out.println("dir: " + dir + "\t ext: "+ fileType.getExtension());
    return false;
  }
  File destFile=new File(new StringBuffer(dir).append(System.getProperty("file.separator")).append(toFile).toString());
  if (destFile.exists()) {
    int answer=JOptionPane.showConfirmDialog(frame,"'" + destFile.getPath() + "' "+ Globals.lang("exists.Overwrite?"),Globals.lang("File exists"),JOptionPane.YES_NO_OPTION,JOptionPane.QUESTION_MESSAGE);
    if (answer == JOptionPane.NO_OPTION)     return false;
  }
  try {
    Util.copyFile(new File(fileName),destFile,true);
  }
 catch (  IOException e) {
    e.printStackTrace();
    return false;
  }
  return true;
}
