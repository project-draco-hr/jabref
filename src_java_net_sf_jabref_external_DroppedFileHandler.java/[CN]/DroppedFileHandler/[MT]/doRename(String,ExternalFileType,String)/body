{
  String dir=panel.metaData().getFileDirectory(fileType.getFieldName());
  if ((dir == null) || !(new File(dir)).exists()) {
    return false;
  }
  File f=new File(fileName);
  File destFile=new File(new StringBuffer(dir).append(System.getProperty("file.separator")).append(destFilename).toString());
  f.renameTo(destFile);
  return true;
}
