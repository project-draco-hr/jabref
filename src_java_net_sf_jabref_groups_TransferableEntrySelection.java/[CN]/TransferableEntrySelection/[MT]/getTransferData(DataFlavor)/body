{
  if (!isDataFlavorSupported(someFlavor))   throw new UnsupportedFlavorException(someFlavor);
  if (someFlavor.equals(flavorInternal))   return this;
  String s=includeCiteKeyword ? "\\cite{" + selectedEntriesCiteKeys + "}" : selectedEntriesCiteKeys;
  return new ByteArrayInputStream(s.getBytes(flavorExternal.getParameter("charset").trim()));
}
