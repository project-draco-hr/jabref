{
  String t=null;
  Token a, b;
  match(LITERAL_throws);
  a=id();
  if (inputState.guessing == 0) {
    t=a.getText();
  }
{
    _loop84:     do {
      if ((LA(1) == COMMA)) {
        match(COMMA);
        b=id();
        if (inputState.guessing == 0) {
          t+="," + b.getText();
        }
      }
 else {
        break _loop84;
      }
    }
 while (true);
  }
  if (inputState.guessing == 0) {
    behavior.setUserExceptions(t);
  }
}
