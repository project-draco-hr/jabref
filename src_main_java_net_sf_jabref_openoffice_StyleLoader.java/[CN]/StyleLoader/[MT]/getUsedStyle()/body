{
  String filename=preferences.getUsedStyleFile();
  if (filename != null) {
    for (    OOBibStyle style : getStyles()) {
      if (filename.equals(style.getPath())) {
        return style;
      }
    }
  }
  preferences.setUsedStyleFile(internalStyles.get(0).getPath());
  return internalStyles.get(0);
}
