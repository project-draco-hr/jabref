{
  m_groupSelector=gs;
  m_groupsRootHandle=groupsRoot;
  m_editType=editType;
  m_subtreeRootChildCount=editedNode.getChildCount();
  m_subtreeBackup=editType == UndoableAddOrRemoveGroup.REMOVE_NODE_KEEP_CHILDREN ? new GroupTreeNode(editedNode.getGroup().deepCopy()) : editedNode.deepCopy();
  m_pathToNode=editedNode.getIndexedPath();
}
