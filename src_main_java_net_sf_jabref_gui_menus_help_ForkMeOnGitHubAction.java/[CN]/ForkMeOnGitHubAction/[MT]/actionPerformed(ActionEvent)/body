{
  try {
    JabRefDesktop.openBrowser("https://github.com/JabRef/jabref");
  }
 catch (  IOException ex) {
    LOGGER.warn("Could not open browser",ex);
    JabRefGUI.getMainFrame().getCurrentBasePanel().output(Localization.lang("Could not open browser.") + " " + Localization.lang("Please open http://github.com/JabRef/jabref manually."));
  }
}
