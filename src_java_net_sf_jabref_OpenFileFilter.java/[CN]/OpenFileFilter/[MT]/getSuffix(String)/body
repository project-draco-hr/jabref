{
  int dotPos;
  String suffix;
  dotPos=filenm.lastIndexOf(".");
  if (dotPos == -1)   return null;
  suffix=filenm.substring(dotPos);
  if (extSet.contains(suffix))   return suffix;
  dotPos=filenm.lastIndexOf(".",dotPos - 1);
  if (dotPos == -1)   return null;
  suffix=filenm.substring(dotPos);
  if (extSet.contains(suffix))   return suffix;
  return null;
}
