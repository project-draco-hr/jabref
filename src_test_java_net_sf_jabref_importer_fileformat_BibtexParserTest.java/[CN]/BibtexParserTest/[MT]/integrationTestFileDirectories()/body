{
  ParserResult result=BibtexParser.parse(new StringReader("@comment{jabref-meta: fileDirectory:\\\\Literature\\\\;}" + "@comment{jabref-meta: fileDirectory-defaultOwner-user:D:\\\\Documents;}"));
  assertEquals("\\Literature\\",result.getMetaData().getDefaultFileDirectory().get());
  assertEquals("D:\\Documents",result.getMetaData().getUserFileDirectory("defaultOwner-user").get());
}
