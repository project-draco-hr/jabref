{
  ArrayList<String> al=new ArrayList<>();
  for (  String anAll : all) {
    boolean found=false;
    for (    String aSubset : subset) {
      if (aSubset.equals(anAll)) {
        found=true;
        break;
      }
    }
    if (!found) {
      al.add(anAll);
    }
  }
  return al.toArray(new String[al.size()]);
}
