{
  int answer=JOptionPane.showConfirmDialog(panel.frame(),Globals.lang("This will move all external links from the 'pdf' and 'ps' fields " + "into the '%0' field. Proceed?",GUIGlobals.FILE_FIELD),Globals.lang("Upgrade external links"),JOptionPane.YES_NO_OPTION);
  if (answer != JOptionPane.YES_OPTION)   return;
  NamedCompound ce=Util.upgradePdfPsToFile(panel.database(),new String[]{"pdf","ps"});
  panel.undoManager.addEdit(ce);
  panel.markBaseChanged();
  panel.output(Globals.lang("Upgraded links."));
}
