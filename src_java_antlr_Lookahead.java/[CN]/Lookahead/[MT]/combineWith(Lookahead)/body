{
  if (cycle == null) {
    cycle=q.cycle;
  }
  if (q.containsEpsilon()) {
    hasEpsilon=true;
  }
  if (epsilonDepth != null) {
    if (q.epsilonDepth != null) {
      epsilonDepth.orInPlace(q.epsilonDepth);
    }
  }
 else   if (q.epsilonDepth != null) {
    epsilonDepth=(BitSet)q.epsilonDepth.clone();
  }
  fset.orInPlace(q.fset);
}
