{
  if (entry != null) {
    String fieldValue=entry.getField(_fieldName);
    if (fieldValue == null) {
      return;
    }
 else {
      StringTokenizer tok=new StringTokenizer(fieldValue.toString()," .,\n");
      while (tok.hasMoreTokens()) {
        String word=tok.nextToken();
        addWordToIndex(word);
      }
    }
  }
}
