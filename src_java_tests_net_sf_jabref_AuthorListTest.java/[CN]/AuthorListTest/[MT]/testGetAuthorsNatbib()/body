{
  assertEquals("",AuthorList.getAuthorList("").getAuthorsNatbib());
  assertEquals("Smith",AuthorList.getAuthorList("John Smith").getAuthorsNatbib());
  assertEquals("Smith and Black Brown",AuthorList.getAuthorList("John Smith and Black Brown, Peter").getAuthorsNatbib());
  assertEquals("von Neumann et al.",AuthorList.getAuthorList("John von Neumann and John Smith and Black Brown, Peter").getAuthorsNatbib());
  assertEquals("Last-Name et al.",AuthorList.getAuthorList("First Second Last-Name" + " and John Smith and Black Brown, Peter").getAuthorsNatbib());
  AuthorList al=AuthorList.getAuthorList("John von Neumann and John Smith and Black Brown, Peter");
  assertTrue(al.getAuthorsNatbib() == al.getAuthorsNatbib());
}
