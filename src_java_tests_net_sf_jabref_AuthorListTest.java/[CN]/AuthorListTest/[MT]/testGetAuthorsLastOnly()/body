{
  assertEquals("",new AuthorList("").getAuthorsLastOnly());
  assertEquals("Smith",new AuthorList("John Smith").getAuthorsLastOnly());
  assertEquals("Smith",new AuthorList("Smith, Jr, John").getAuthorsLastOnly());
  assertEquals("von Neumann, Smith and Black Brown",new AuthorList("John von Neumann and John Smith and Black Brown, Peter").getAuthorsLastOnly());
}
