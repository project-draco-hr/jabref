{
  assertEquals("",AuthorList.getAuthorList("").getAuthorsLastOnly());
  assertEquals("Smith",AuthorList.getAuthorList("John Smith").getAuthorsLastOnly());
  assertEquals("Smith",AuthorList.getAuthorList("Smith, Jr, John").getAuthorsLastOnly());
  assertEquals("von Neumann, Smith and Black Brown",AuthorList.getAuthorList("John von Neumann and John Smith and Black Brown, Peter").getAuthorsLastOnly());
}
