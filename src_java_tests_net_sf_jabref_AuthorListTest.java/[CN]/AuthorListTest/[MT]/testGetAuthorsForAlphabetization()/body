{
  assertEquals("Smith, J.",new AuthorList("John Smith").getAuthorsForAlphabetization());
  assertEquals("Neumann, J.",new AuthorList("John von Neumann").getAuthorsForAlphabetization());
  assertEquals("Neumann, J.",new AuthorList("J. von Neumann").getAuthorsForAlphabetization());
  assertEquals("Neumann, J. and Smith, J. and Black Brown, Jr., P.",new AuthorList("John von Neumann and John Smith and de Black Brown, Jr., Peter").getAuthorsForAlphabetization());
}
