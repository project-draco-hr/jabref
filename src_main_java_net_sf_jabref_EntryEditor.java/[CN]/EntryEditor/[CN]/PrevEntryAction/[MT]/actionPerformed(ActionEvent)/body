{
  int thisRow=panel.mainTable.findEntry(entry);
  int newRow;
  if ((thisRow - 1) >= 0) {
    newRow=thisRow - 1;
  }
 else   if (thisRow != (panel.database.getEntryCount() - 1)) {
    newRow=panel.database.getEntryCount() - 1;
  }
 else {
    return;
  }
  scrollTo(newRow);
  panel.mainTable.setRowSelectionInterval(newRow,newRow);
}
