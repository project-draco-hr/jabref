{
  this.target=option;
  this.value=option.getStringValue();
  String longOption=option.getLongOption();
  char shortOption=option.getShortOption();
  if (longOption != null) {
    command=longOption;
  }
 else   if (shortOption != '\0') {
    command=Character.toString(shortOption);
  }
 else {
    command="Default";
  }
}
