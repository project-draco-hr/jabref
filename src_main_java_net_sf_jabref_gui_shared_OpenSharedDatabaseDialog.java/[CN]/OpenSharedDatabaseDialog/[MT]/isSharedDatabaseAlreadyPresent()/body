{
  List<BasePanel> panels=JabRefGUI.getMainFrame().getBasePanelList();
  for (  BasePanel panel : panels) {
    DBMSConnectionProperties dbmsConnectionProperties=panel.getBibDatabaseContext().getDBMSSynchronizer().getDBProcessor().getDBMSConnectionProperties();
    if (this.connectionProperties.equals(dbmsConnectionProperties)) {
      return true;
    }
  }
  return false;
}
