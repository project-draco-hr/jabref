{
  Objects.requireNonNull(jabRefPreferences);
  Objects.requireNonNull(journalAbbreviationLoader);
  return new LayoutFormatterPreferences(NameFormatterPreferences.fromPreferences(jabRefPreferences),JournalAbbreviationPreferences.fromPreferences(jabRefPreferences),FileLinkPreferences.fromPreferences(jabRefPreferences),jabRefPreferences.customExportNameFormatters,journalAbbreviationLoader);
}
