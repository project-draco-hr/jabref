{
  String[] subsup={"title","journal","publisher"};
  for (  String aSubsup : subsup) {
    if (map.containsKey(aSubsup)) {
      String s=map.get(aSubsup);
      if (s.toUpperCase().equals(s)) {
        s=CaseChangers.UPPER_EACH_FIRST_SKIP_SMALL_WORDS.changeCase(s);
        map.put(aSubsup,s);
      }
    }
  }
}
