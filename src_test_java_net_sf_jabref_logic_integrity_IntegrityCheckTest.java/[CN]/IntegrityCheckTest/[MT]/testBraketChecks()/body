{
  assertCorrect("x",IntegrityCheck.BRACKET_CHECKER);
  assertCorrect("{x}",IntegrityCheck.BRACKET_CHECKER);
  assertCorrect("{x}x{}x{{}}",IntegrityCheck.BRACKET_CHECKER);
  assertWrong("{x}x{}}x{{}}",IntegrityCheck.BRACKET_CHECKER);
  assertWrong("}",IntegrityCheck.BRACKET_CHECKER);
  assertWrong("{",IntegrityCheck.BRACKET_CHECKER);
}
