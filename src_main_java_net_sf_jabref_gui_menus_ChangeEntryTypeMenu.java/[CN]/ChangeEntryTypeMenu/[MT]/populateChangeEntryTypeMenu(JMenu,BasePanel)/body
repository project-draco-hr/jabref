{
  menu.removeAll();
  if (panel.getLoadedDatabase().isBiblatexMode()) {
    for (    String key : EntryTypes.getAllTypes(BibDatabaseType.BIBLATEX)) {
      menu.add(new ChangeTypeAction(EntryTypes.getType(key,BibDatabaseType.BIBLATEX),panel));
    }
  }
 else {
    createEntryTypeSection(panel,menu,"BibTeX Entries",BibtexEntryTypes.ALL);
    menu.addSeparator();
    createEntryTypeSection(panel,menu,"IEEETran Entries",IEEETranEntryTypes.ALL);
    menu.addSeparator();
    createEntryTypeSection(panel,menu,"Custom Entries",CustomEntryTypesManager.ALL);
  }
}
