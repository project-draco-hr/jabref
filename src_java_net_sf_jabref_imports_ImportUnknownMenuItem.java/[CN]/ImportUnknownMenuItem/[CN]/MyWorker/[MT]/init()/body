{
  filename=frame.getNewFile();
  if ((filename != null) && !(new File(filename)).exists()) {
    JOptionPane.showMessageDialog(frame,Globals.lang("File not found") + ": '" + filename+ "'",Globals.lang("Error"),JOptionPane.ERROR_MESSAGE);
  }
 else   if (filename != null) {
    frame.block();
    frame.output(Globals.lang("Importing file") + ": '" + filename+ "'");
    fileOk=true;
    Globals.prefs.put("workingDirectory",filename);
  }
}
