{
  BibEntry entry=new BibEntry();
  entry.setType(BibtexEntryTypes.ARTICLE);
  database.insertEntry(entry);
  StringSaveSession session=databaseWriter.savePartOfDatabase(bibtexContext,Collections.singletonList(entry),new SavePreferences());
  assertEquals(Globals.NEWLINE + "@Article{," + Globals.NEWLINE+ "}"+ Globals.NEWLINE+ Globals.NEWLINE+ "@Comment{jabref-meta: databaseType:bibtex;}"+ Globals.NEWLINE,session.getStringValue());
}
