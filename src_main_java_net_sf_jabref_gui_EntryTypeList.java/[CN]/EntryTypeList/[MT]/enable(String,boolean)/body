{
  if (EntryTypes.getStandardType(typeName,type) == null) {
    def.setEnabled(false);
    remove.setEnabled(true);
  }
 else {
    if (isChanged || (EntryTypes.getType(typeName,type) instanceof CustomEntryType)) {
      def.setEnabled(true);
    }
 else {
      def.setEnabled(false);
    }
    remove.setEnabled(false);
  }
}
