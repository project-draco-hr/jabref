{
  if (BibtexEntryType.getStandardType(typeName) != null) {
    if (isChanged || (BibtexEntryType.getType(typeName) instanceof CustomEntryType)) {
      def.setEnabled(true);
    }
 else {
      def.setEnabled(false);
    }
    remove.setEnabled(false);
  }
 else {
    def.setEnabled(false);
    remove.setEnabled(true);
  }
}
