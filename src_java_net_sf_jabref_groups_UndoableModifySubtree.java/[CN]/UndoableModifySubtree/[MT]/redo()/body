{
  super.redo();
  final GroupTreeNode subtreeRoot=m_groupSelector.getGroupTreeRoot().getNode(m_subtreeRootPath);
  subtreeRoot.removeAllChildren();
  for (int i=0; i < m_modifiedSubtree.size(); ++i)   subtreeRoot.add((GroupTreeNode)m_modifiedSubtree.elementAt(i));
  if (m_revalidate)   m_groupSelector.revalidateGroups();
}
