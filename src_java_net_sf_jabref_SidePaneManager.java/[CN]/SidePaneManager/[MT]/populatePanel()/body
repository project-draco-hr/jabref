{
  if (prefs.getBoolean("groupSelectorVisible") && (metaData.getData("groups") != null)) {
    GroupSelector gs=new GroupSelector(frame,panel,metaData.getData("groups"),this,prefs);
    add("groups",gs);
  }
  if (components.size() > 0) {
    panel.setLeftComponent(sidep);
  }
 else   panel.setLeftComponent(null);
}
