{
  int[] rows=table.getSelectedRows();
  if (rows.length == 0) {
    rowCount++;
    table.revalidate();
    table.repaint();
    return;
  }
  for (int i=0; i < rows.length; i++) {
    if (rows[i] + i - 1 < tableRows.size())     tableRows.add(Math.max(0,rows[i] + i - 1),new TableRow());
  }
  rowCount+=rows.length;
  if (rows.length > 1)   table.clearSelection();
  table.revalidate();
  table.repaint();
  tableChanged=true;
}
