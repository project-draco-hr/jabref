{
  @SuppressWarnings("unchecked") boolean allAccepted=true;
  Enumeration<Change> e=children();
  for (; e.hasMoreElements(); ) {
    Change c=e.nextElement();
    if (c.isAcceptable() && c.isAccepted())     c.makeChange(panel,secondary,undoEdit);
 else     allAccepted=false;
  }
  return allAccepted;
}
