{
  assertEquals("",CaseChangers.UPPER_EACH_FIRST_SKIP_SMALL_WORDS.changeCase(""));
  assertEquals("Upper Each First and",CaseChangers.UPPER_EACH_FIRST_SKIP_SMALL_WORDS.changeCase("upper each First and"));
  assertEquals("Upper Each First and",CaseChangers.UPPER_EACH_FIRST_SKIP_SMALL_WORDS.changeCase("upper each First aNd"));
  assertEquals("Upper Each First and",CaseChangers.UPPER_EACH_FIRST_SKIP_SMALL_WORDS.changeCase("upper each First AND"));
}
