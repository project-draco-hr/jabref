{
  int len=number.length();
  int digitCounter=0;
  boolean fourDigitsBlock=false;
  boolean containsFourDigits=false;
  for (int t=0; t < len; t++) {
    char ch=number.charAt(t);
    if (Character.isDigit(ch)) {
      digitCounter++;
      fourDigitsBlock=digitCounter == 4;
    }
 else {
      if (fourDigitsBlock)       containsFourDigits=true;
      digitCounter=0;
    }
  }
  if ((!containsFourDigits) && (!fourDigitsBlock)) {
    messages.add(new IntegrityMessage(IntegrityMessage.FOUR_DIGITS_HINT,entry,fieldName,null));
  }
}
