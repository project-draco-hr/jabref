{
  StringBuffer sb=new StringBuffer(startUrl).append(searchUrlPart);
  sb.append(terms.replaceAll(" ","%20"));
  sb.append("&start=").append(String.valueOf(startIndex));
  sb.append(searchUrlPartII);
  if (acmOrGuide) {
    sb.append("ACM");
  }
 else {
    sb.append("GUIDE");
  }
  sb.append(endUrl);
  return sb.toString();
}
