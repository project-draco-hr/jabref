{
  piv=startIndex;
  int entryNumber=firstEntryNumber;
  BibtexEntry entry;
  while (((entry=parseNextEntry(text,piv,entryNumber)) != null) && shouldContinue) {
    if (entry.getField("title") != null) {
      dialog.addEntry(entry);
      dialog.setProgress(parsed + unparseable,hits);
      parsed++;
    }
    entryNumber++;
  }
}
