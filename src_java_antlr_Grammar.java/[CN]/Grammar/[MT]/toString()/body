{
  StringBuffer buf=new StringBuffer(20000);
  Enumeration ids=rules.elements();
  while (ids.hasMoreElements()) {
    RuleSymbol rs=(RuleSymbol)ids.nextElement();
    if (!rs.id.equals("mnextToken")) {
      buf.append(rs.getBlock().toString());
      buf.append("\n\n");
    }
  }
  return buf.toString();
}
