{
  if (s == null)   return "";
  StringBuilder sb=new StringBuilder();
  String[] keywords=s.split(",[ ]*");
  for (int i=0; i < keywords.length; i++) {
    sb.append("KW  - ");
    sb.append(keywords[i]);
    if (i < keywords.length - 1)     sb.append(Globals.NEWLINE);
  }
  return sb.toString();
}
