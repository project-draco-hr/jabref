{
  if (s == null)   return "";
  StringBuilder sb=new StringBuilder();
  List<String> keywords=Util.getSeparatedKeywords(s);
  for (int i=0; i < keywords.size(); i++) {
    sb.append("KW  - ");
    sb.append(keywords.get(i));
    if (i < keywords.size() - 1)     sb.append(Globals.NEWLINE);
  }
  return sb.toString();
}
