{
  if (isOpen()) {
    return;
  }
  RemoteListenerServerThread result;
  try {
    result=new RemoteListenerServerThread(messageHandler,port);
  }
 catch (  IOException e) {
    if (!e.getMessage().startsWith("Address already in use")) {
      e.printStackTrace();
    }
    result=null;
  }
  remoteListenerServerThread=result;
}
