def get_keys_from_lines(lines):
    '\n    Builds a list of all translation keys in the list of lines.\n\n    :param lines: a list of strings\n    :return: the sorted keys within the list of strings\n    '
    keys = []
    for line in lines:
        comment = line.find('#')
        if (comment != 0):
            index = line.find('=')
            while ((index > 0) and (line[(index - 1)] == '\\')):
                index = line.find('=', (index + 1))
            if (index > 0):
                keys.append(line[0:index])
    return keys
