def get_keys_dict(property_lines):
    "\n    Saves all the localizations (the whole String 'translationKey=translation')\n    in a dict with their translationKey as the dict key\n\n    :param property_lines: a list\n    :return: a dict\n    "
    keys = {}
    for line in property_lines:
        key = get_key_from_line(line)
        if (key is not None):
            keys[key] = line
    return keys
