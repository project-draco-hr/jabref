{
  FieldContentSelector ws=new FieldContentSelector(frame,panel,frame,editor,panel.getBibDatabaseContext().getMetaData(),storeFieldAction,false,InternalBibtexFields.getFieldExtras(editor.getFieldName()).contains(FieldProperties.PERSON_NAMES) ? " and " : ", ");
  contentSelectors.add(ws);
  return Optional.of(ws);
}
