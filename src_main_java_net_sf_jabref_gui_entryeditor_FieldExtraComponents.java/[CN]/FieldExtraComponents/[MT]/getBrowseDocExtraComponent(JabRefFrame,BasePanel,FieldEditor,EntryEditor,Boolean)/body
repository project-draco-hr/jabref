{
  final String ext='.' + fieldEditor.getFieldName().toLowerCase();
  OpenFileFilter off;
  if (isZip) {
    off=new OpenFileFilter(new String[]{ext,ext + ".gz",ext + ".bz2"});
  }
 else {
    off=new OpenFileFilter(new String[]{ext});
  }
  return Optional.of(new ExternalFilePanel(frame,panel.getBibDatabaseContext().getMetaData(),entryEditor,fieldEditor.getFieldName(),off,fieldEditor));
}
