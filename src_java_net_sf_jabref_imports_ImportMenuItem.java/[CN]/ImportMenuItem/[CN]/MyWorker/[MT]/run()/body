{
  if (!fileOk)   return;
  List imports=new ArrayList();
  for (int i=0; i < filenames.length; i++) {
    String filename=filenames[i];
    if (importer != null)     try {
      imports.add(new Object[]{importer.getFormatName(),Globals.importFormatReader.importFromFile(importer,filename)});
    }
 catch (    IOException e) {
    }
 else     imports.add(Globals.importFormatReader.importUnknownFormat(filename));
  }
  bibtexResult=mergeImportResults(imports);
}
