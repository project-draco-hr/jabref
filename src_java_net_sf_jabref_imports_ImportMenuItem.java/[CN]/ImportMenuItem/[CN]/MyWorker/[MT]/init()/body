{
  filename=Globals.getNewFile(frame,Globals.prefs,new File(Globals.prefs.get("workingDirectory")),importer.getExtensions(),JFileChooser.OPEN_DIALOG,false);
  if ((filename != null) && !(new File(filename)).exists()) {
    JOptionPane.showMessageDialog(frame,Globals.lang("File not found") + ": '" + filename+ "'",Globals.lang("Import failed"),JOptionPane.ERROR_MESSAGE);
  }
 else   if (filename != null) {
    frame.block();
    frame.output(Globals.lang("Importing file") + ": '" + filename+ "'");
    fileOk=true;
  }
}
