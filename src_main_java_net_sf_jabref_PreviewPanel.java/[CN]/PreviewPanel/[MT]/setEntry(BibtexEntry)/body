{
  if (newEntry != entry) {
    if (entry != null) {
      entry.removePropertyChangeListener(this);
    }
    newEntry.addPropertyChangeListener(this);
  }
  entry=newEntry;
  try {
    readLayout();
    update();
  }
 catch (  StringIndexOutOfBoundsException ex) {
    throw ex;
  }
catch (  Exception ex) {
    ex.printStackTrace();
  }
}
