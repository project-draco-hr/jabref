{
  if (isExpanded == true) {
    if (null != m_iconExpanded) {
      g.drawImage(m_iconExpanded.getImage(),(int)bounds.x - 17,(int)bounds.y + 4,null);
    }
  }
 else {
    if (null != m_iconCollapsed) {
      g.drawImage(m_iconCollapsed.getImage(),(int)bounds.x - 17,(int)bounds.y + 4,null);
    }
  }
}
