{
  super(name,context);
  m_searchExpression=searchExpression;
  m_caseSensitive=caseSensitive;
  m_regExp=regExp;
  AST ast=null;
  try {
    SearchExpressionParser parser=new SearchExpressionParser(new SearchExpressionLexer(new StringReader(m_searchExpression)));
    parser.caseSensitive=m_caseSensitive;
    parser.regex=m_regExp;
    parser.searchExpression();
    ast=parser.getAST();
  }
 catch (  Exception e) {
    ast=null;
  }
  m_ast=ast;
  if (m_ast != null) {
    m_searchRule=this;
  }
 else {
    if (m_regExp)     m_searchRule=new RegExpRule(m_caseSensitive);
 else     m_searchRule=new SimpleSearchRule(m_caseSensitive);
  }
}
