{
  super(name);
  m_searchExpression=searchExpression;
  m_caseSensitive=caseSensitive;
  m_regExp=regExp;
  m_searchAllFields=searchAllFields;
  m_searchRequiredFields=searchRequiredFields;
  m_searchOptionalFields=searchOptionalFields;
  m_searchGeneralFields=searchGeneralFields;
  AST ast=null;
  try {
    SearchExpressionParser parser=new SearchExpressionParser(new SearchExpressionLexer(new StringReader(m_searchExpression)));
    parser.caseSensitive=m_caseSensitive;
    parser.regex=m_regExp;
    parser.searchExpression();
    ast=parser.getAST();
  }
 catch (  Exception e) {
    ast=null;
  }
  m_ast=ast;
  if (m_ast != null) {
    m_searchRule=this;
  }
 else {
    if (m_regExp)     m_searchRule=new RegExpRule(m_caseSensitive,m_searchAllFields,m_searchRequiredFields,m_searchOptionalFields,m_searchGeneralFields);
 else     m_searchRule=new SimpleSearchRule(m_caseSensitive,m_searchAllFields,m_searchRequiredFields,m_searchOptionalFields,m_searchGeneralFields);
  }
}
