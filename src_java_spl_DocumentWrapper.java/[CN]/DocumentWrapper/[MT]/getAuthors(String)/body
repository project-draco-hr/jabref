{
  if (this.hasAuthors()) {
    List<XmlAuthor> authors=xmlDocument.getAuthors().getAuthors();
    String value="";
    int i=1;
    for (    XmlAuthor author : authors) {
      if (i < authors.size()) {
        value=value + author.getNameComplete();
        value=value + seperator + " ";
      }
 else {
        value=value + author.getNameComplete();
      }
      i++;
    }
    return value;
  }
 else {
    return "";
  }
}
