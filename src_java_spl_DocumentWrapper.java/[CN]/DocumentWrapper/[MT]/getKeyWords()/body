{
  if (this.hasKeyWords()) {
    List<XmlKeyword> keywords=xmlDocument.getKeywords().getKeywords();
    String value="";
    int i=1;
    for (    XmlKeyword keyword : keywords) {
      if (i < keywords.size()) {
        value=value + keyword.getValue();
        value=value + ", ";
      }
 else {
        value=value + keyword.getValue();
      }
      i++;
    }
    return value;
  }
 else {
    return "";
  }
}
