{
  if (success) {
    frame.setTabTitle(panel,panel.getFile().getName(),panel.getFile().getAbsolutePath());
    frame.output(Globals.lang("Saved database") + " '" + panel.getFile().getPath()+ "'.");
  }
 else   if (!cancelled) {
    if (fileLockedError) {
      frame.output(Globals.lang("Could not save, file locked by another JabRef instance."));
    }
 else     frame.output(Globals.lang("Save failed"));
  }
}
