{
  if (selectionModel.getSelected().size() != 1) {
    return;
  }
  BibEntry entry=selectionModel.getSelected().get(0);
  String result=JOptionPane.showInputDialog(ImportInspectionDialog.this,Localization.lang("Enter URL"),entry.getField(URL_FIELD));
  entries.getReadWriteLock().writeLock().lock();
  try {
    if (result != null) {
      if (result.isEmpty()) {
        entry.clearField(URL_FIELD);
      }
 else {
        entry.setField(URL_FIELD,result);
      }
    }
  }
  finally {
    entries.getReadWriteLock().writeLock().unlock();
  }
  glTable.repaint();
}
