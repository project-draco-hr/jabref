{
  String url=constructUrl(key);
  try {
    HttpURLConnection conn=(HttpURLConnection)new URL(url).openConnection();
    conn.setRequestProperty("User-Agent","JabRef");
    InputStream inputStream=conn.getInputStream();
    try (INSPIREBibtexFilterReader reader=new INSPIREBibtexFilterReader(new InputStreamReader(inputStream,Charset.forName("UTF-8")))){
      ParserResult pr=BibtexParser.parse(reader,Globals.prefs.getImportFormatPreferences());
      return pr.getDatabase();
    }
   }
 catch (  RuntimeException|IOException e) {
    frame.showMessage(Localization.lang("An exception occurred while accessing '%0'",url) + "\n\n" + e,getTitle(),JOptionPane.ERROR_MESSAGE);
  }
  return null;
}
