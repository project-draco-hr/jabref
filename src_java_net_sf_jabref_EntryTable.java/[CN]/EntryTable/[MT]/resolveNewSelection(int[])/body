{
  HashSet newSel=new HashSet();
  for (int i=0; i < rows.length; i++) {
    Integer row=new Integer(rows[i]);
    newSel.add(row);
  }
  HashSet tmp=new HashSet(newSel);
  newSel.removeAll(lastSelection);
  lastSelection=tmp;
  int result=-1;
  if (newSel.size() == 1)   result=((Integer)newSel.iterator().next()).intValue();
  if ((result < 0) && (rows.length == 1))   result=rows[0];
  return result;
}
