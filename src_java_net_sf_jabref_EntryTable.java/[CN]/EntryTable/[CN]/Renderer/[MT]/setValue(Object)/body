{
  if (value instanceof Icon) {
    setIcon((Icon)value);
    super.setValue(null);
  }
 else   if (value instanceof JLabel) {
    JLabel lab=(JLabel)value;
    super.setIcon(lab.getIcon());
    super.setToolTipText(lab.getToolTipText());
    super.setText(null);
  }
 else {
    setIcon(null);
    super.setToolTipText(null);
    super.setValue(value);
  }
}
