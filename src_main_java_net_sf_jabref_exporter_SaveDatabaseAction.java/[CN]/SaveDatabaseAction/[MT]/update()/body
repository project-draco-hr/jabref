{
  if (success) {
    frame.setTabTitle(panel,panel.getFile().getName(),panel.getFile().getAbsolutePath());
    frame.output(Localization.lang("Saved database") + " '" + panel.getFile().getPath()+ "'.");
    frame.setWindowTitle();
  }
 else   if (!cancelled) {
    if (fileLockedError) {
      frame.output(Localization.lang("Could not save, file locked by another JabRef instance."));
    }
 else {
      frame.output(Localization.lang("Save failed"));
    }
  }
}
