{
  Globals.prefs=JabRefPreferences.getInstance();
  try {
    URLDownload dl=new URLDownload(new URL("http://www.google.com"));
    Assert.assertTrue("google.com should contain google",dl.downloadToString(Globals.prefs.getDefaultEncoding()).contains("Google"));
  }
  finally {
    Globals.prefs=null;
  }
}
