{
  if (newEntry != entry) {
    if (entry != null) {
      entry.removePropertyChangeListener(this);
    }
    newEntry.addPropertyChangeListener(this);
  }
  entry=newEntry;
  try {
    updateLayout();
    update();
  }
 catch (  StringIndexOutOfBoundsException ex) {
    throw ex;
  }
catch (  IOException ex) {
    LOGGER.debug("exception occurred while reading layout",ex);
  }
}
