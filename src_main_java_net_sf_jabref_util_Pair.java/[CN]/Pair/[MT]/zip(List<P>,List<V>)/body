{
  List<Pair<P,V>> result=new LinkedList<Pair<P,V>>();
  Iterator<P> pI=ps.iterator();
  Iterator<V> vI=vs.iterator();
  while (pI.hasNext()) {
    V nextV=(vI.hasNext() ? vI.next() : null);
    result.add(new Pair<P,V>(pI.next(),nextV));
  }
  while (vI.hasNext()) {
    result.add(new Pair<P,V>(null,vI.next()));
  }
  return result;
}
