{
  String bibtexEntry="@Article{test," + Globals.NEWLINE + "  Author                   = {Foo Bar},"+ Globals.NEWLINE+ "  Journal                  = {International Journal of Something},"+ Globals.NEWLINE+ "  Note                     = {some note},"+ Globals.NEWLINE+ "  Number                   = {1}"+ Globals.NEWLINE+ "}";
  String result=testSingleWrite(bibtexEntry);
  result=testSingleWrite(result);
  result=testSingleWrite(result);
  assertEquals(bibtexEntry,result);
}
