{
  super(frame,Localization.lang("Set general fields"),false);
  parent=frame;
  HelpAction help=new HelpAction(parent.helpDiag,GUIGlobals.generalFieldsHelp,"Help",IconTheme.getImage("helpSmall"));
  helpBut=new JButton(Localization.lang("Help"));
  helpBut.addActionListener(help);
  try {
    jbInit();
    setSize(new Dimension(650,300));
  }
 catch (  Exception ex) {
    ex.printStackTrace();
  }
}
