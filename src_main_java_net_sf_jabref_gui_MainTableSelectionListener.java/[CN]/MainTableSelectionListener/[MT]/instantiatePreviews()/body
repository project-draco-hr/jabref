{
  previewPanel=new PreviewPanel[]{new PreviewPanel(panel.database(),null,panel,panel.metaData(),Globals.prefs.get("preview0"),true),new PreviewPanel(panel.database(),null,panel,panel.metaData(),Globals.prefs.get("preview1"),true)};
  panel.frame().getSearchBar().addSearchListener(previewPanel[0]);
  panel.frame().getSearchBar().addSearchListener(previewPanel[1]);
}
