{
  if ((4 <= str.length()) && ('\'' == str.charAt(0)) && ('\\' == str.charAt(1))&& (('0' <= str.charAt(2)) && ('7' >= str.charAt(2)))&& ('\'' == str.charAt(str.length() - 1))) {
    Integer x=Integer.valueOf(str.substring(2,str.length() - 1),8);
    return "'\\x" + Integer.toHexString(x.intValue()) + "'";
  }
 else {
    return str;
  }
}
