{
  if (r.getLabel() != null && syntacticPredLevel == 0) {
    println(r.getLabel() + " = " + lt1Value+ ";");
  }
  boolean flag=(grammar instanceof LexerGrammar && (!saveText || (r.getAutoGenType() == GrammarElement.AUTO_GEN_BANG)));
  if (flag)   println("_saveIndex = text.Length;");
  println("matchRange(" + OctalToUnicode(r.beginText) + ","+ OctalToUnicode(r.endText)+ ");");
  if (flag)   println("text.Length = _saveIndex;");
}
