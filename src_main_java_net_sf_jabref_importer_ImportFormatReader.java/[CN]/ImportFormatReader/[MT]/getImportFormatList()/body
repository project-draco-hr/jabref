{
  StringBuilder sb=new StringBuilder();
  for (  ImportFormat imFo : formats) {
    int pad=Math.max(0,14 - imFo.getFormatName().length());
    sb.append("  ");
    sb.append(imFo.getFormatName());
    for (int j=0; j < pad; j++) {
      sb.append(" ");
    }
    sb.append(" : ");
    sb.append(imFo.getCLIId());
    sb.append("\n");
  }
  return sb.toString();
}
