{
  String s=" FOLLOW={";
  Lookahead cache[]=endNode.cache;
  int k=grammar.maxk;
  boolean allNull=true;
  for (int j=1; j <= k; j++) {
    if (cache[j] == null)     continue;
    s+=cache[j].toString(",",grammar.tokenManager.getVocabulary());
    allNull=false;
    if (j < k && cache[j + 1] != null)     s+=";";
  }
  s+="}";
  if (allNull)   s="";
  return ruleName + ": " + super.toString()+ " ;"+ s;
}
