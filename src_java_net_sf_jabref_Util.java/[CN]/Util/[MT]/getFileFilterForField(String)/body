{
  String s=(String)GUIGlobals.FIELD_EXTRAS.get(fieldName);
  final String ext="." + fieldName.toLowerCase();
  final OpenFileFilter off;
  if (s.equals("browseDocZip"))   off=new OpenFileFilter(new String[]{ext,ext + ".gz",ext + ".bz2"});
 else   off=new OpenFileFilter(new String[]{ext});
  return off;
}
