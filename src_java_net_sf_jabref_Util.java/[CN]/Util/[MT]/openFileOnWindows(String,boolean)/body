{
  if (localFile) {
    String[] spl=link.split("\\\\");
    StringBuffer sb=new StringBuffer();
    for (int i=0; i < spl.length; i++) {
      if (i > 0)       sb.append("\\");
      if (spl[i].indexOf(" ") >= 0)       spl[i]="\"" + spl[i] + "\"";
      sb.append(spl[i]);
    }
    link=sb.toString();
  }
  String cmd="cmd.exe /c start " + link;
  Process child=Runtime.getRuntime().exec(cmd);
}
