{
  ArrayList<String> al=new ArrayList<String>();
  for (int i=0; i < all.length; i++) {
    boolean found=false;
    inner:     for (int j=0; j < subset.length; j++) {
      if (subset[j].equals(all[i])) {
        found=true;
        break inner;
      }
    }
    if (!found)     al.add(all[i]);
  }
  return al.toArray(new String[al.size()]);
}
