{
  int lockCheckCount=0;
  while (Util.hasLockFile(file)) {
    System.out.println("File locked... waiting");
    if (lockCheckCount++ == maxWaitCount) {
      System.out.println("Giving up wait.");
      return false;
    }
    try {
      Thread.sleep(500);
    }
 catch (    InterruptedException ex) {
    }
  }
  return true;
}
