{
  if (dest.exists()) {
    if (!deleteIfExists)     return false;
  }
  BufferedInputStream in=new BufferedInputStream(new FileInputStream(source));
  BufferedOutputStream out=new BufferedOutputStream(new FileOutputStream(dest));
  int el;
  while ((el=in.read()) >= 0) {
    out.write(el);
  }
  out.flush();
  in.close();
  out.close();
  return true;
}
