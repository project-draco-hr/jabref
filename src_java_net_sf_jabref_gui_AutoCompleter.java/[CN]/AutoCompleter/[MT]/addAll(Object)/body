{
  if (s == null)   return;
  if (nameField) {
    String[] names=s.toString().split(" and ");
    for (int i=0; i < names.length; i++) {
      String name=names[i].trim();
      addWord(name);
    }
  }
 else {
    StringTokenizer tok=new StringTokenizer(s.toString()," .,\n");
    while (tok.hasMoreTokens()) {
      String word=tok.nextToken();
      addWord(word);
    }
  }
}
