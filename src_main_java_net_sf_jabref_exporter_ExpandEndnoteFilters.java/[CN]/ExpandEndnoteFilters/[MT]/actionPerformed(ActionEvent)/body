{
  String filename=FileDialogs.getNewFile(frame,new File(System.getProperty("user.home")),".zip",JFileChooser.SAVE_DIALOG,false);
  if (filename == null) {
    return;
  }
  file=new File(filename);
  if (file.exists()) {
    int confirm=JOptionPane.showConfirmDialog(frame,Localization.lang("'%0' exists. Overwrite file?",file.getName()),Localization.lang("Unpack EndNote filter set"),JOptionPane.OK_CANCEL_OPTION);
    if (confirm != JOptionPane.OK_OPTION) {
      return;
    }
  }
  ((Worker)Spin.off(this)).run();
  file=null;
}
