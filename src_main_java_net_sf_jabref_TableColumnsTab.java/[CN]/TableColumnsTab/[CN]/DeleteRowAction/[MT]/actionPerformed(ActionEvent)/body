{
  int[] rows=colSetup.getSelectedRows();
  if (rows.length == 0) {
    return;
  }
  int offs=0;
  for (int i=rows.length - 1; i >= 0; i--) {
    if (rows[i] <= tableRows.size() && rows[i] != 0) {
      tableRows.remove(rows[i] - 1);
      offs++;
    }
  }
  rowCount-=offs;
  if (rows.length > 1) {
    colSetup.clearSelection();
  }
  colSetup.revalidate();
  colSetup.repaint();
  tableChanged=true;
}
