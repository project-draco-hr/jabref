{
  StringBuilder res=new StringBuilder();
  if (lastPart != null) {
    res.append(lastPart);
  }
  if (jrPart != null) {
    res.append(", ");
    res.append(jrPart);
  }
  if (firstAbbr != null) {
    res.append(", ");
    res.append(firstAbbr);
  }
  while (res.length() > 0 && res.charAt(0) == '{') {
    res.deleteCharAt(0);
  }
  return res.toString();
}
