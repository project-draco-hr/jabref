{
  fout.println("INSERT INTO groups (groups_id, label, parent_id) " + "VALUES (" + ID + ", \""+ cursor.getGroup().getName()+ "\", "+ parentID+ ");");
  int myID=ID;
  for (Enumeration<GroupTreeNode> e=cursor.children(); e.hasMoreElements(); )   ID=sql_popTabGP(e.nextElement(),myID,++ID,fout);
  return ID;
}
