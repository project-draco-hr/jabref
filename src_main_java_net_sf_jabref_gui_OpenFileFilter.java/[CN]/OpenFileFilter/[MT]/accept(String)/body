{
  String lowerCaseFileName=filename.toLowerCase();
  int dotPos=lowerCaseFileName.lastIndexOf('.');
  if (dotPos == -1) {
    return false;
  }
  int dotDotPos=lowerCaseFileName.lastIndexOf('.',dotPos - 1);
  return extSet.contains(lowerCaseFileName.substring(dotPos)) || ((dotDotPos >= 0) && extSet.contains(lowerCaseFileName.substring(dotDotPos)));
}
