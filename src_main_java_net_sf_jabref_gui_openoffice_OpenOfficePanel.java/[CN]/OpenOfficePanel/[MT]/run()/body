{
  try {
    ooBase=new OOBibBase(sOffice,true);
  }
 catch (  UnknownPropertyException|CreationException|NoSuchElementException|WrappedTargetException|IOException|NoDocumentException|BootstrapException|InvocationTargetException|IllegalAccessException e) {
    ooBase=null;
    connectException=new IOException(e.getMessage());
  }
}
