{
  assertEquals("??",new OOPreFormatter().format("{\\aa}"));
  assertEquals("bb",new OOPreFormatter().format("{\\bb}"));
  assertEquals("?? a",new OOPreFormatter().format("\\aa a"));
  assertEquals("?? a",new OOPreFormatter().format("{\\aa a}"));
  assertEquals("????",new OOPreFormatter().format("\\aa\\AA"));
  assertEquals("bb a",new OOPreFormatter().format("\\bb a"));
}
