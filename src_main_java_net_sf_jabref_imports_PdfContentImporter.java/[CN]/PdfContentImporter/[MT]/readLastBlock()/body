{
  while ((i >= 0) && (split[i].trim().equals(""))) {
    i--;
  }
  int end=i;
  while ((i >= 0) && (!split[i].equals(""))) {
    i--;
  }
  curString="";
  for (int j=i + 1; j <= end; j++) {
    curString=curString.concat(split[j].trim());
    if (j != end) {
      curString=curString.concat(" ");
    }
  }
}
