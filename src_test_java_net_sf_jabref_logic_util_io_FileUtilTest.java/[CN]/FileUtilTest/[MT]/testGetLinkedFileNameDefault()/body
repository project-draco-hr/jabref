{
  when(prefs.get(JabRefPreferences.IMPORT_FILENAMEPATTERN)).thenReturn("\\bibtexkey\\begin{title} - \\format[RemoveBrackets]{\\title}\\end{title}");
  BibEntry entry=new BibEntry();
  entry.setCiteKey("1234");
  entry.setField("title","mytitle");
  assertEquals("1234 - mytitle",FileUtil.createFileNameFromPattern(null,entry,mock(JournalAbbreviationLoader.class),prefs));
}
