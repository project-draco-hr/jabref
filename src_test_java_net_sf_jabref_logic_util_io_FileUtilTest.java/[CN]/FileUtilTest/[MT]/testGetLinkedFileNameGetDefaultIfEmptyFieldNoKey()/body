{
  String fileNamePattern="\\begin{title} - \\format[RemoveBrackets]{\\title}\\end{title}";
  BibEntry entry=new BibEntry();
  assertEquals("default",FileUtil.createFileNameFromPattern(null,entry,fileNamePattern,JabRefPreferences.getInstance().getLayoutFormatterPreferences(mock(JournalAbbreviationLoader.class))));
}
