{
  if (errors != null && !errors.isEmpty()) {
    JOptionPane.showMessageDialog(this,"The import finished with warnings:\n" + "There " + (errors.size() > 1 ? "were " : "was ") + errors.size()+ (errors.size() > 1 ? " files" : " file")+ (errors.size() > 1 ? " which" : " that")+ " could not be imported.",Localization.lang("Warning"),JOptionPane.WARNING_MESSAGE);
  }
  progressBarImporting.setVisible(false);
  labelImportingInfo.setVisible(false);
  buttonApply.setVisible(true);
  buttonClose.setVisible(true);
  disOrEnableDialog(true);
  setDefaultCloseOperation(WindowConstants.DISPOSE_ON_CLOSE);
  frame.basePanel().markBaseChanged();
}
