{
  OpenFileFilter off=null;
  if (extension == null)   off=new OpenFileFilter();
 else   if (!extension.equals(NONE))   off=new OpenFileFilter(extension);
  Object o=getNewFileImpl(owner,directory,extension,null,off,JFileChooser.OPEN_DIALOG,updateWorkingdirectory,false,true);
  String[] toReturn;
  if (o instanceof String[])   toReturn=(String[])o;
 else   toReturn=new String[]{(String)o};
  return toReturn;
}
