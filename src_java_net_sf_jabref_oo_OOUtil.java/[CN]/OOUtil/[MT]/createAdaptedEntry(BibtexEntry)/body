{
  if (entry == null)   return null;
  BibtexEntry e=(BibtexEntry)entry.clone();
  for (  String field : e.getAllFields()) {
    if (field.equals(BibtexFields.KEY_FIELD))     continue;
    String value=e.getField(field);
    if (!OpenOfficePanel.postLayoutSupported && (value != null))     e.setField(field,postformatter.format(value));
  }
  return e;
}
