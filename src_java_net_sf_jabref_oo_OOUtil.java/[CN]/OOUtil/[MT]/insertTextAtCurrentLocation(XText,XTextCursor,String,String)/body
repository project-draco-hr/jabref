{
  text.insertString(cursor,string,true);
  XParagraphCursor parCursor=(XParagraphCursor)UnoRuntime.queryInterface(XParagraphCursor.class,cursor);
  XPropertySet props=(XPropertySet)UnoRuntime.queryInterface(XPropertySet.class,parCursor);
  try {
    props.setPropertyValue("ParaStyleName",parStyle);
  }
 catch (  com.sun.star.lang.IllegalArgumentException ex) {
    throw new UndefinedParagraphFormatException(parStyle);
  }
  cursor.collapseToEnd();
}
