{
  boolean[] isProtected=new boolean[title.length()];
  char[] chars=title.toCharArray();
  int brakets=0;
  for (int i=0; i < title.length(); i++) {
    if (chars[i] == '{') {
      brakets++;
    }
 else     if (chars[i] == '}') {
      brakets--;
    }
 else {
      isProtected[i]=brakets > 0;
    }
  }
  return isProtected;
}
