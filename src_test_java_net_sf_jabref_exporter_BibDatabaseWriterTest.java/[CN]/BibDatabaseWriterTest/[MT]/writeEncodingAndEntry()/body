{
  SavePreferences preferences=new SavePreferences().withEncoding(Charsets.US_ASCII);
  BibEntry entry=new BibEntry();
  entry.setType(BibtexEntryTypes.ARTICLE);
  database.insertEntry(entry);
  databaseWriter.writePartOfDatabase(stringWriter,bibtexContext,Collections.singletonList(entry),preferences);
  assertEquals("% Encoding: US-ASCII" + Globals.NEWLINE + Globals.NEWLINE+ "@Article{,"+ Globals.NEWLINE+ "}"+ Globals.NEWLINE+ Globals.NEWLINE+ "@Comment{jabref-meta: databaseType:bibtex;}"+ Globals.NEWLINE,stringWriter.toString());
}
