{
  BibEntry entry=new BibEntry();
  entry.setType(BibtexEntryTypes.ARTICLE);
  database.insertEntry(entry);
  databaseWriter.writePartOfDatabase(stringWriter,bibtexContext,Collections.singletonList(entry),new SavePreferences());
  assertEquals(Globals.NEWLINE + "@Article{," + Globals.NEWLINE+ "}"+ Globals.NEWLINE,stringWriter.toString());
}
