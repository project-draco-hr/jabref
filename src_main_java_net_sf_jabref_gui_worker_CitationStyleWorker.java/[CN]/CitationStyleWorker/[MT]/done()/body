{
  if (this.isCancelled()) {
    return;
  }
  String text;
  try {
    text=this.get();
  }
 catch (  InterruptedException|ExecutionException e) {
    LOGGER.error("Error while generating citation style",e);
    text=Localization.lang("Error while generating citation style");
  }
  previewPanel.setPreviewLabel(text);
}
