{
  String newType;
  if (type == null) {
    newType=DEFAULT_TYPE;
  }
 else {
    newType=type;
  }
  String oldType=this.type;
  try {
    this.type=newType.toLowerCase(Locale.ENGLISH);
    changed=true;
    firePropertyChangedEvent(TYPE_HEADER,oldType,newType);
  }
 catch (  PropertyVetoException pve) {
    LOGGER.warn(pve);
  }
}
