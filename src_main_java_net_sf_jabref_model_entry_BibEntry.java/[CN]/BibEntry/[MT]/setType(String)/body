{
  if (type == null) {
    type=DEFAULT_TYPE;
  }
  String oldType=this.type;
  type=type.toLowerCase();
  try {
    this.type=type;
    changed=true;
    firePropertyChangedEvent(TYPE_HEADER,oldType == null ? null : oldType,type);
  }
 catch (  PropertyVetoException pve) {
    LOGGER.warn(pve);
  }
}
