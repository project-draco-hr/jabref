{
  Objects.requireNonNull(keywords);
  String oldValue=this.getField("keywords");
  String newValue;
  if (keywords.isEmpty()) {
    newValue=null;
  }
 else {
    newValue=String.join(", ",keywords);
  }
  if (newValue == null) {
    if (oldValue != null) {
      this.clearField("keywords");
    }
    return;
  }
  if ((oldValue == null) || !oldValue.equals(newValue)) {
    this.setField("keywords",newValue);
  }
}
