{
  if (!hasField("year")) {
    return null;
  }
  String year=YearUtil.toFourDigitYear(getField("year"));
  if (hasField("month")) {
    MonthUtil.Month month=MonthUtil.getMonth(getField("month"));
    if (month.isValid()) {
      return year + "-" + month.twoDigitNumber;
    }
  }
  return year;
}
