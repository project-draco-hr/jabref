{
  preferences.setExternalStyles(Collections.emptyList());
  loader=new StyleLoader(preferences,LayoutFormatterPreferences.fromPreferences(Globals.prefs,mock(JournalAbbreviationLoader.class)),Globals.prefs.getDefaultEncoding());
  String filename=Paths.get(JabRefMain.class.getResource(StyleLoader.DEFAULT_AUTHORYEAR_STYLE_PATH).toURI()).toFile().getPath();
  loader.addStyleIfValid(filename);
  assertEquals(numberOfInternalStyles + 1,loader.getStyles().size());
}
