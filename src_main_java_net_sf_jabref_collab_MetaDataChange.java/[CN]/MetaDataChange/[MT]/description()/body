{
  StringBuilder sb=new StringBuilder("<html>" + Localization.lang("Changes have been made to the following metadata elements") + ":<p>");
  for (  MetaDataChangeUnit unit : changes) {
    sb.append("<br>&nbsp;&nbsp;");
    sb.append(unit.key);
  }
  sb.append("</html>");
  tp.setText(sb.toString());
  return sp;
}
