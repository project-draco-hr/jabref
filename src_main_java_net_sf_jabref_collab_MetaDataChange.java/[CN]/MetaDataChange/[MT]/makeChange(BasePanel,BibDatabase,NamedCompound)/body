{
  for (  MetaDataChangeUnit unit : changes) {
switch (unit.type) {
case ADD:
      md.putData(unit.key,unit.value);
    mdSecondary.putData(unit.key,unit.value);
  break;
case REMOVE:
md.remove(unit.key);
mdSecondary.remove(unit.key);
break;
case MODIFY:
md.putData(unit.key,unit.value);
mdSecondary.putData(unit.key,unit.value);
break;
}
}
return true;
}
