{
  JOptionPane.showMessageDialog(panel.frame(),Globals.lang("I couldn't parse the following URL") + ": \"" + badURL+ '\"'+ Globals.lang(" on entry number ")+ (rowNumber + 1)+ ".  "+ Globals.lang("Please refer to the JabRef help manual on using the CiteSeer tools."),Globals.lang("CiteSeer Error"),JOptionPane.ERROR_MESSAGE);
}
