{
  if (authorsAlph != null) {
    return authorsAlph;
  }
  StringBuilder res=new StringBuilder();
  if (size() > 0) {
    res.append(getAuthor(0).getNameForAlphabetization());
    for (int i=1; i < size(); i++) {
      res.append(" and ");
      res.append(getAuthor(i).getNameForAlphabetization());
    }
  }
  authorsAlph=res.toString();
  return authorsAlph;
}
