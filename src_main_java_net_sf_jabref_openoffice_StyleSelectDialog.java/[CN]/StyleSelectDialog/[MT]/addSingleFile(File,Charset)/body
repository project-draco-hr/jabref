{
  try {
    OOBibStyle style=new OOBibStyle(file,Globals.journalAbbreviationLoader.getRepository(),encoding);
    if (style.isValid() && !styles.contains(style)) {
      styles.add(style);
    }
  }
 catch (  IOException e) {
    LOGGER.warn("Unable to read style file: '" + file.getPath() + "'",e);
  }
}
