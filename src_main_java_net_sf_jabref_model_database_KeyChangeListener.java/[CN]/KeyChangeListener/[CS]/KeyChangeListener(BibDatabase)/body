{
  this.database=database;
  for (  String fieldName : InternalBibtexFields.getAllPublicFieldNames()) {
    if (InternalBibtexFields.getFieldExtras(fieldName).contains(FieldProperties.SINGLE_ENTRY_LINK) || InternalBibtexFields.getFieldExtras(fieldName).contains(FieldProperties.MULTIPLE_ENTRY_LINK)) {
      keyFields.add(fieldName);
    }
  }
}
