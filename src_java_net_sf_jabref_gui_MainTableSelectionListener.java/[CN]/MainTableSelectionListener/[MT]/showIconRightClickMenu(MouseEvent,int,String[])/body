{
  BibtexEntry entry=(BibtexEntry)tableRows.get(row);
  JPopupMenu menu=new JPopupMenu();
  int count=0;
  for (int i=0; i < iconType.length; i++) {
    Object o=entry.getField(iconType[i]);
    if (o != null) {
      menu.add(new ExternalFileMenuItem((String)o,(String)o,GUIGlobals.getTableIcon(iconType[i]).getIcon(),panel.metaData()));
      count++;
    }
  }
  if (count == 0) {
    processPopupTrigger(e,row);
    return;
  }
  menu.show(table,e.getX(),e.getY());
}
