{
  if (child != null) {
    if (currentAST.root == null) {
      currentAST.root=child;
    }
 else {
      if (currentAST.child == null) {
        currentAST.root.setFirstChild(child);
      }
 else {
        currentAST.child.setNextSibling(child);
      }
    }
    currentAST.child=child;
    currentAST.advanceChildToEnd();
  }
}
