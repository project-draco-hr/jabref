{
  if (value instanceof Icon) {
    setIcon((Icon)value);
    setText(null);
  }
 else   if (value instanceof JLabel) {
    JLabel lab=(JLabel)value;
    setIcon(lab.getIcon());
    setToolTipText(lab.getToolTipText());
    if (lab.getIcon() != null) {
      setText(null);
    }
  }
 else {
    setIcon(null);
    setToolTipText(null);
    if (value != null) {
      setText(value.toString());
    }
 else {
      setText(null);
    }
  }
}
