{
synchronized (set) {
    if (e.getType() == DatabaseChangeEvent.ADDED_ENTRY) {
      set.add(e.getEntry());
      changed=true;
    }
 else     if (e.getType() == DatabaseChangeEvent.REMOVED_ENTRY) {
      set.remove(e.getEntry());
      changed=true;
    }
 else     if (e.getType() == DatabaseChangeEvent.CHANGED_ENTRY) {
      changed=true;
    }
  }
}
