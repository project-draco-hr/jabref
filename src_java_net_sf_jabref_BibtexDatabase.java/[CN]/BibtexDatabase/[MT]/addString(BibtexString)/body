{
  for (java.util.Iterator i=_strings.keySet().iterator(); i.hasNext(); ) {
    if (((BibtexString)_strings.get(i.next())).getName().equals(string.getName()))     throw new KeyCollisionException("A string with this label already exists,");
  }
  if (_strings.containsKey(string.getId()))   throw new KeyCollisionException("Duplicate BibtexString id.");
  _strings.put(string.getId(),string);
}
