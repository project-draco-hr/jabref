{
  Globals.logger(" checkForDuplicateKeyAndAdd [oldKey = " + oldKey + "] [newKey = "+ newKey+ "]");
  boolean duplicate=false;
  if (oldKey == null) {
    duplicate=addKeyToSet(newKey);
  }
 else {
    if (oldKey.equals(newKey)) {
      duplicate=false;
    }
 else {
      removeKeyFromSet(oldKey);
      duplicate=addKeyToSet(newKey);
    }
  }
  if (duplicate == true && issueWarning == true) {
    JOptionPane.showMessageDialog(null,Globals.lang("Warning there is a duplicate key") + ":" + newKey,Globals.lang("Duplicate Key Warning"),JOptionPane.WARNING_MESSAGE);
  }
  return duplicate;
}
