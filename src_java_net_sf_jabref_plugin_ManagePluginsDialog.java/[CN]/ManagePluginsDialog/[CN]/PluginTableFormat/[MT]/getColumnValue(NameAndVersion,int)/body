{
  if (col == 0)   return nav.name;
 else   if (col == 1) {
    if (!nav.version.equals(PluginInstaller.VersionNumber.ZERO))     return nav.version.toString();
 else     return Globals.lang("Unknown");
  }
 else {
    int status=nav.getStatus();
    if (status == 0)     return Globals.lang("Not loaded");
 else     if (status == 1)     return Globals.lang("Loaded");
 else     return Globals.lang("Error");
  }
}
