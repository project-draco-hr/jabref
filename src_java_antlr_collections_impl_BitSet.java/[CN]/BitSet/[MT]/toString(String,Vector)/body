{
  if (vocabulary == null) {
    return toString(separator);
  }
  String str="";
  for (int i=0; i < (bits.length << LOG_BITS); i++) {
    if (member(i)) {
      if (str.length() > 0) {
        str+=separator;
      }
      if (i >= vocabulary.size()) {
        str+="<bad element " + i + ">";
      }
 else       if (vocabulary.elementAt(i) == null) {
        str+="<" + i + ">";
      }
 else {
        str+=(String)vocabulary.elementAt(i);
      }
    }
  }
  return str;
}
