{
  if (OS.WINDOWS) {
    String progFiles=System.getenv("ProgramFiles(x86)");
    if (progFiles == null) {
      progFiles=System.getenv("ProgramFiles");
    }
    if (windowsDirectory != null) {
      return progFiles + "\\" + windowsDirectory+ "\\"+ programName+ ".exe";
    }
 else {
      return progFiles + "\\" + programName+ ".exe";
    }
  }
 else {
    return programName;
  }
}
