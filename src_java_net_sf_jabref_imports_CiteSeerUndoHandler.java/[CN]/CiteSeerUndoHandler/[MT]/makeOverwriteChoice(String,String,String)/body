{
  boolean overwrite;
  if ((oldValue == null) || (oldValue.equals("")))   overwrite=true;
 else   if (oldValue.equals(newValue))   overwrite=false;
 else   if (overwriteAll == true)   overwrite=true;
 else   if (overwriteNone == true)   overwrite=false;
 else   overwrite=overwriteDialog(oldValue,newValue,fieldName);
  return overwrite;
}
