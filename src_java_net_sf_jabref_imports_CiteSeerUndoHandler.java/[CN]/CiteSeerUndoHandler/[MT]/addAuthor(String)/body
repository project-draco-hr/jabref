{
  if (bibEntry.getField("author") == null) {
    UndoableFieldChange fieldChange=new UndoableFieldChange(bibEntry,"author",bibEntry.getField("author"),newAuthor);
    citeseerNamedCompound.addEdit(fieldChange);
    bibEntry.setField("author",newAuthor);
  }
 else {
    UndoableFieldChange fieldChange=new UndoableFieldChange(bibEntry,"author",bibEntry.getField("author"),bibEntry.getField("author") + " and " + newAuthor);
    citeseerNamedCompound.addEdit(fieldChange);
    bibEntry.setField("author",bibEntry.getField("author") + " and " + newAuthor);
  }
}
