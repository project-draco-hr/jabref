{
  if (newAuthors != null) {
    if (makeOverwriteChoice((String)bibEntry.getField("author"),newAuthors,"author")) {
      UndoableFieldChange fieldChange=new UndoableFieldChange(bibEntry,"author",bibEntry.getField("author"),newAuthors);
      citeseerNamedCompound.addEdit(fieldChange);
      bibEntry.setField("author",newAuthors);
    }
  }
}
