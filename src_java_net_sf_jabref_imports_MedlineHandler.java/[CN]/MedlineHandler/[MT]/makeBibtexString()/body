{
  String out="";
  out="article{,\n" + " author = { " + author + " },\n title = { "+ title+ "},\n journal ={ "+ journal+ "},\n year = "+ year+ "},\n volume = { "+ volume+ "},\n number = { "+ number+ "},\n pages = { "+ fixPageRange(page)+ "},\n abstract = { "+ abstractText+ "},\n}";
  return out;
}
