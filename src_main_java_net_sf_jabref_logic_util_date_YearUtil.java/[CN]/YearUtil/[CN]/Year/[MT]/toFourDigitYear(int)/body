{
  if (relativeYear == yearShort) {
    return this.year;
  }
  if ((relativeYear + 100 - yearShort) % 100 > 30) {
    if (relativeYear < yearShort) {
      return century + relativeYear;
    }
 else {
      return century - 100 + relativeYear;
    }
  }
 else {
    if (relativeYear < yearShort) {
      return century + 100 + relativeYear;
    }
 else {
      return century + relativeYear;
    }
  }
}
