{
  IsiImporter importer=new IsiImporter();
  List<String> list=Arrays.asList("IsiImporterTest1.isi","IsiImporterTest2.isi","IsiImporterTestInspec.isi","IsiImporterTestWOS.isi","IsiImporterTestMedline.isi");
  for (  String str : list) {
    try (InputStream is=IsiImporterTest.class.getResourceAsStream(str)){
      Assert.assertTrue(importer.isRecognizedFormat(is));
    }
   }
}
