{
  IsiImporter importer=new IsiImporter();
  List<BibEntry> entries=importer.importEntries(IsiImporterTest.class.getResourceAsStream("IsiImporterTestWOS.isi"),new OutputPrinterToNull());
  Assert.assertEquals(2,entries.size());
  BibEntry a=entries.get(0);
  BibEntry b=entries.get(1);
  if (a.getField("title").equals("Optical waveguides in Sn2P2S6 by low fluence MeV He+ ion implantation")) {
    BibEntry tmp=a;
    a=b;
    b=tmp;
  }
  Assert.assertEquals("Optical and photoelectric spectroscopy of photorefractive Sn2P2S6 crystals",a.getField("title"));
  Assert.assertEquals("Optical waveguides in Sn2P2S6 by low fluence MeV He+ ion implantation",b.getField("title"));
  Assert.assertEquals("Journal of Physics-condensed Matter",a.getField("journal"));
}
