{
  StringBuffer b=new StringBuffer();
  if (verboseStringConversion && !getText().equalsIgnoreCase(tokenNames[getType()]) && !getText().equalsIgnoreCase(StringUtils.stripFrontBack(tokenNames[getType()],"\"","\""))) {
    b.append('[');
    b.append(getText());
    b.append(",<");
    b.append(tokenNames[getType()]);
    b.append(">]");
    return b.toString();
  }
  return getText();
}
