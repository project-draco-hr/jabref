{
  if (value.length > 0) {
    StringBuffer linked=new StringBuffer();
    for (int i=0; i < value.length - 1; i++) {
      linked.append(makeEscape(value[i]));
      linked.append(";");
    }
    linked.append(makeEscape(value[value.length - 1]));
    put(key,linked.toString());
  }
 else {
    put(key,"");
  }
}
