{
  keyPattern=pattern;
  LabelPattern parent=pattern.getParent();
  if (parent == null)   return;
  Preferences pre=Preferences.userNodeForPackage(net.sf.jabref.labelPattern.LabelPattern.class);
  Iterator i=pattern.keySet().iterator();
  while (i.hasNext()) {
    String s=(String)i.next();
    if (!(pattern.get(s)).equals(parent.get(s)))     pre.put(s,(String)pattern.get(s));
  }
}
