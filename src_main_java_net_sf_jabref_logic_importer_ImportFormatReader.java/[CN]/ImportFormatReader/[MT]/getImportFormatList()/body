{
  StringBuilder sb=new StringBuilder();
  for (  ImportFormat imFo : formats) {
    int pad=Math.max(0,14 - imFo.getFormatName().length());
    sb.append("  ");
    sb.append(imFo.getFormatName());
    sb.append(StringUtil.repeatSpaces(pad));
    sb.append(" : ");
    sb.append(imFo.getId());
    sb.append('\n');
  }
  return sb.toString();
}
