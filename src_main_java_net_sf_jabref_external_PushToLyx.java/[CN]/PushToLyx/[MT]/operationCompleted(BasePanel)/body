{
  if (couldNotFindPipe) {
    panel.output(Localization.lang("Error") + ": " + Localization.lang("verify that LyX is running and that the lyxpipe is valid")+ ". ["+ Globals.prefs.get(JabRefPreferences.LYXPIPE)+ "]");
  }
 else   if (couldNotWrite) {
    panel.output(Localization.lang("Error") + ": " + Localization.lang("unable to write to")+ " "+ Globals.prefs.get(JabRefPreferences.LYXPIPE)+ ".in");
  }
 else {
    panel.output(Localization.lang("Pushed citations to %0",getApplicationName()));
  }
}
