{
  boolean changed;
  do {
    changed=false;
    for (int i=0; i < remove.length(); i++) {
      char c=remove.charAt(i);
      while (s.length() > 0 && s.charAt(s.length() - 1) == c) {
        changed=true;
        s=s.substring(0,s.length() - 1);
      }
    }
  }
 while (changed);
  return s;
}
