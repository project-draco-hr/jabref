{
  super.getListCellRendererComponent(list,value,index,iss,chf);
  if (value != null) {
    IntegrityMessage msg=(IntegrityMessage)value;
    if (msg.getFixed()) {
      setIcon(fixedIcon);
    }
 else {
      int id=msg.getType();
      if (id < 1000) {
        setIcon(infoIcon);
      }
 else       if (id < 2000) {
        setIcon(warnIcon);
      }
 else {
        setIcon(failIcon);
      }
    }
  }
  return this;
}
