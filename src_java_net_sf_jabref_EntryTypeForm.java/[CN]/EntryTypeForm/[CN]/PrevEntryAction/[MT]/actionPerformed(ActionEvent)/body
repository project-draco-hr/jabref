{
  int thisRow=panel.tableModel.getNumberFromName(entry.getId());
  String id=null;
  if (thisRow - 1 >= 0)   id=panel.tableModel.getNameFromNumber(thisRow - 1);
 else   if (thisRow != panel.database.getEntryCount() - 1)   id=panel.tableModel.getNameFromNumber(panel.database.getEntryCount() - 1);
  switchTo(id);
}
