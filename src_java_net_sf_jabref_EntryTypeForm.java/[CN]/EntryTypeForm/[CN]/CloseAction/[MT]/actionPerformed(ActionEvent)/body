{
  if (tabbed.getSelectedComponent() == srcPanel) {
    storeFieldAction.actionPerformed(new ActionEvent(source,0,""));
    if (lastSourceAccepted) {
      panel.entryTypeFormClosing(entry.getId());
      dispose();
    }
  }
 else {
    panel.entryTypeFormClosing(entry.getId());
    dispose();
  }
}
