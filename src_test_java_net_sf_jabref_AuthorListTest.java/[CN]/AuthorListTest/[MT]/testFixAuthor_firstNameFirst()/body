{
  Assert.assertEquals("John Smith",AuthorList.fixAuthor_firstNameFirst("John Smith"));
  Assert.assertEquals("John Smith and Peter Black Brown",AuthorList.fixAuthor_firstNameFirst("John Smith and Black Brown, Peter"));
  Assert.assertEquals("John von Neumann and John Smith and Peter Black Brown",AuthorList.fixAuthor_firstNameFirst("John von Neumann and John Smith and Black Brown, Peter"));
  Assert.assertEquals("First von Last, Jr. III",AuthorList.fixAuthor_firstNameFirst("von Last, Jr. III, First"));
  Assert.assertTrue(AuthorList.fixAuthor_firstNameFirst("John von Neumann and John Smith and Black Brown, Peter").equals(AuthorList.fixAuthor_firstNameFirst("John von Neumann" + (0 == 1 ? "" : " and ") + "John Smith and Black Brown, Peter")));
}
