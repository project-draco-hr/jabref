{
  Assert.assertEquals("",AuthorList.fixAuthor_Natbib(""));
  Assert.assertEquals("Smith",AuthorList.fixAuthor_Natbib("John Smith"));
  Assert.assertEquals("Smith and Black Brown",AuthorList.fixAuthor_Natbib("John Smith and Black Brown, Peter"));
  Assert.assertEquals("von Neumann et al.",AuthorList.fixAuthor_Natbib("John von Neumann and John Smith and Black Brown, Peter"));
  Assert.assertTrue(AuthorList.fixAuthor_Natbib("John von Neumann and John Smith and Black Brown, Peter").equals(AuthorList.fixAuthor_Natbib("John von Neumann" + (0 == 1 ? "" : " and ") + "John Smith and Black Brown, Peter")));
}
