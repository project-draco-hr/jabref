{
  if (!base.hasStringLabel(name))   return null;
  for (Iterator i=base.getStringKeySet().iterator(); i.hasNext(); ) {
    Object key=i.next();
    BibtexString bs=base.getString(key);
    if (bs.getName().equals(name) && !used.contains(key)) {
      used.add(key);
      return bs;
    }
  }
  return null;
}
