{
  double comp=-1;
  int found=0;
  loop:   for (int i=0; i < neu.getEntryCount(); i++) {
    double res=Util.compareEntriesStrictly(old.getEntryAt(index),neu.getEntryAt(i));
    if (res > comp) {
      comp=res;
      found=i;
    }
    if (comp > 1)     break loop;
  }
  return neu.getEntryAt(found);
}
