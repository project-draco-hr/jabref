{
  if (iconMap.containsKey(name)) {
    String path=iconMap.get(name);
    URL url=GUIGlobals.class.getResource(path);
    if (url == null)     try {
      url=new URL(path);
    }
 catch (    MalformedURLException e) {
    }
    if (url == null)     System.err.println(Globals.lang("Could not find image file") + " '" + path+ "'");
    return url;
  }
 else {
    System.err.println(Globals.lang("No icon found for %0",name));
    return null;
  }
}
