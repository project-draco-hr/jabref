{
  Reader in=null;
  try {
    in=new InputStreamReader(url.openStream());
    StringBuilder sb=new StringBuilder();
    int c;
    while ((c=in.read()) != -1)     sb.append((char)c);
    return sb.toString();
  }
  finally {
    try {
      if (in != null)       in.close();
    }
 catch (    IOException ex) {
      ex.printStackTrace();
    }
  }
}
