{
  Assert.assertTrue(DOIUtil.isURI("http://doi.acm.org/10.1145/1294928.1294933"));
  Assert.assertTrue(DOIUtil.isURI("http://dx.doi.org/10.1007/978-3-642-15618-2_19"));
  Assert.assertTrue(DOIUtil.isURI("http://dx.doi.org/10.1000/182"));
  Assert.assertFalse(DOIUtil.isURI("http://www.xyz.com"));
  Assert.assertFalse(DOIUtil.isURI("http://dx.doing.org/fjdlfdsjfdlfdj.htm"));
  Assert.assertFalse(DOIUtil.isURI("thfjtfjglkjjlkk???lm5476576658796"));
  Assert.assertTrue(DOIUtil.isURI("http://doii.acm.org/10.1145/1294928.1294933"));
  Assert.assertTrue(DOIUtil.isURI("http://dx.ddoi.org/10.1007/978-3-642-15618-2_19"));
  Assert.assertTrue(DOIUtil.isURI("http://dx.eoi.org/10.1000/182"));
}
