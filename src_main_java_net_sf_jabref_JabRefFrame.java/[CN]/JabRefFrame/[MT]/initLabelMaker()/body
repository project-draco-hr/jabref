{
  labelMaker=new LabelMaker();
  labelMaker.addRule(new ArticleLabelRule(),BibtexEntryType.ARTICLE);
  labelMaker.addRule(new BookLabelRule(),BibtexEntryType.BOOK);
  labelMaker.addRule(new IncollectionLabelRule(),BibtexEntryType.INCOLLECTION);
  labelMaker.addRule(new InproceedingsLabelRule(),BibtexEntryType.INPROCEEDINGS);
}
