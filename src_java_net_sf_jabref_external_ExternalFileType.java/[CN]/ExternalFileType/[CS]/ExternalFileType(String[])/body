{
  if ((val == null) || (val.length < 4))   throw new IllegalArgumentException("Cannot contruct ExternalFileType without four elements in String[] argument.");
  this.name=val[0];
  label.setToolTipText(this.name);
  this.extension=val[1];
  this.openWith=val[2];
  label.setText(null);
  setIconName(val[3]);
}
