{
  Assert.assertEquals(null,Util.shaveString(null));
  Assert.assertEquals("",Util.shaveString(""));
  Assert.assertEquals("aaa",Util.shaveString("   aaa\t\t\n\r"));
  Assert.assertEquals("a",Util.shaveString("  {a}    "));
  Assert.assertEquals("a",Util.shaveString("  \"a\"    "));
  Assert.assertEquals("{a}",Util.shaveString("  {{a}}    "));
  Assert.assertEquals("{a}",Util.shaveString("  \"{a}\"    "));
  Assert.assertEquals("\"{a\"}",Util.shaveString("  \"{a\"}    "));
}
