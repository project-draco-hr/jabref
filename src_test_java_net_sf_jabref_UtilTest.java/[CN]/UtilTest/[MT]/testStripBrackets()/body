{
  Assert.assertEquals("foo",Util.stripBrackets("[foo]"));
  Assert.assertEquals("[foo]",Util.stripBrackets("[[foo]]"));
  Assert.assertEquals("foo",Util.stripBrackets("foo]"));
  Assert.assertEquals("foo",Util.stripBrackets("[foo"));
  Assert.assertEquals("",Util.stripBrackets(""));
  Assert.assertEquals("",Util.stripBrackets("[]"));
  Assert.assertEquals("",Util.stripBrackets("["));
  Assert.assertEquals("",Util.stripBrackets("]"));
  Assert.assertEquals("f[]f",Util.stripBrackets("f[]f"));
  try {
    Util.stripBrackets(null);
    Assert.fail();
  }
 catch (  NullPointerException ignored) {
  }
}
