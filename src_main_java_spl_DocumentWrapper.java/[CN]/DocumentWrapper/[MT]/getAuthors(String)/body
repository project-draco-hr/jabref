{
  if (this.hasAuthors()) {
    List<Bean> authors=xmlDocument.getAuthors().getCollection();
    authors=DocumentsWrapper.sortAuthors(authors);
    String value="";
    int i=1;
    for (    Bean author : authors) {
      if (i < authors.size()) {
        value=value + getNameComplete((Author)author);
        value=value + ' ' + seperator+ ' ';
      }
 else {
        value=value + getNameComplete((Author)author);
      }
      i++;
    }
    return value;
  }
 else {
    return "";
  }
}
