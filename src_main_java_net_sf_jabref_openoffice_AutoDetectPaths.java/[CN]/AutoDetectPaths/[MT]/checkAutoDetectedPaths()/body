{
  if (Globals.prefs.hasKey(JabRefPreferences.OO_JARS_PATH) && Globals.prefs.hasKey(JabRefPreferences.OO_EXECUTABLE_PATH)) {
    return new File(Globals.prefs.get(JabRefPreferences.OO_JARS_PATH),"jurt.jar").exists() && new File(Globals.prefs.get(JabRefPreferences.OO_EXECUTABLE_PATH)).exists();
  }
 else {
    return false;
  }
}
