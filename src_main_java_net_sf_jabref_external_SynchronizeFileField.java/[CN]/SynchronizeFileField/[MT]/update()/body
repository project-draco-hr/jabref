{
  if (!goOn)   return;
  int entriesChangedCount=0;
  panel.output(Globals.lang("Finished synchronizing %0 links. Entries changed%c %1.",new String[]{fieldName.toUpperCase(),String.valueOf(entriesChangedCount)}));
  panel.frame().setProgressBarVisible(false);
  if (entriesChangedCount > 0) {
    panel.markBaseChanged();
  }
}
