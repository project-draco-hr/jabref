{
  if (!goOn) {
    return;
  }
  int entriesChangedCount=0;
  panel.output(Localization.lang("Finished synchronizing %0 links. Entries changed: %1.",fieldName.toUpperCase(),String.valueOf(entriesChangedCount)));
  panel.frame().setProgressBarVisible(false);
  if (entriesChangedCount > 0) {
    panel.markBaseChanged();
  }
}
