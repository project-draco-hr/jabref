{
  String urlQuery;
  try {
    urlQuery="http://www.jstor.org/search/BasicResults?hp=" + JSTORFetcher.MAX_CITATIONS + "&si=1&gw=jtx&jtxsi=1&jcpsi=1&artsi=1&Query="+ URLEncoder.encode(query,StandardCharsets.UTF_8.name())+ "&wc=on&citationAction=saveAll";
  }
 catch (  UnsupportedEncodingException e) {
    throw new RuntimeException(e);
  }
  URL url=new URL(urlQuery);
  URLConnection conn=url.openConnection();
  conn.setRequestProperty("Cookie",ticket);
  return JSTORFetcher.getCookie(JSTORFetcher.COOKIE_CITATIONS,conn);
}
