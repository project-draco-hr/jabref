{
  try {
    String ticket=openTicket();
    String citations=getCitations(ticket,query);
    Collection<BibEntry> entries=getBibtexEntries(ticket,citations);
    if (entries.isEmpty()) {
      status.showMessage(Localization.lang("No entries found for the search string '%0'",query),Localization.lang("Search %0","JSTOR"),JOptionPane.INFORMATION_MESSAGE);
      return false;
    }
    for (    BibEntry entry : entries) {
      dialog.addEntry(entry);
    }
    return true;
  }
 catch (  IOException e) {
    status.showMessage(Localization.lang("Error while fetching from %0","JSTOR") + ": " + e.getMessage());
  }
  return false;
}
