{
  char la_1=LA(1);
  try {
    super.matchRange(c1,c2);
    parserEventSupport.fireMatch(la_1,"" + c1 + c2,inputState.guessing);
  }
 catch (  MismatchedCharException e) {
    if (inputState.guessing == 0)     parserEventSupport.fireMismatch(la_1,"" + c1 + c2,inputState.guessing);
    throw e;
  }
}
