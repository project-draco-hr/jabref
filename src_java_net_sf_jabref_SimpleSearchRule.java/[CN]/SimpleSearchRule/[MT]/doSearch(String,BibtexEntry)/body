{
  int score=0;
  int counter=0;
  if (m_searchAll) {
    score+=searchAllFields(searchString,bibtexEntry);
  }
 else {
    if (m_searchReq)     score+=searchRequiredFields(searchString,bibtexEntry);
    if (m_searchOpt)     score+=searchOptionalFields(searchString,bibtexEntry);
    if (m_searchGen)     score+=searchGeneralFields(searchString,bibtexEntry);
  }
  return score;
}
