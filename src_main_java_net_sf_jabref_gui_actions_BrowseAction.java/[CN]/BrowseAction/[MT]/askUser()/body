{
  if (dirsOnly) {
    Path path=new NewFileDialog(frame,comp.getText()).dirsOnly().withExtensions(extensions).openDialogAndGetSelectedFile().orElse(Paths.get(""));
    String file=path.toString();
    return file;
  }
 else {
    Path path=new NewFileDialog(frame,comp.getText()).withExtensions(extensions).openDialogAndGetSelectedFile().orElse(Paths.get(""));
    String file=path.toString();
    return file;
  }
}
