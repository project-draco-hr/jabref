{
  if (dirsOnly) {
    Path path=new FileDialog(frame,comp.getText()).withExtensions(extensions).showDialogAndGetSelectedDirectory().orElse(Paths.get(""));
    String file=path.toString();
    return file;
  }
 else {
    Path path=new FileDialog(frame,comp.getText()).withExtensions(extensions).showDialogAndGetSelectedFile().orElse(Paths.get(""));
    String file=path.toString();
    return file;
  }
}
