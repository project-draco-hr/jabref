{
  list=new BasicEventList();
  this.comp=comp;
  list.getReadWriteLock().writeLock().lock();
  Set keySet=entries.keySet();
  if (keySet != null) {
    Iterator i=keySet.iterator();
    while (i.hasNext()) {
      list.add(entries.get(i.next()));
    }
  }
  list.getReadWriteLock().writeLock().unlock();
}
