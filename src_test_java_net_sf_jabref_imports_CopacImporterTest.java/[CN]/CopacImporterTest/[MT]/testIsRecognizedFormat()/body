{
  CopacImporter importer=new CopacImporter();
  Assert.assertTrue(importer.isRecognizedFormat(CopacImporterTest.class.getResourceAsStream("CopacImporterTest1.txt")));
  Assert.assertTrue(importer.isRecognizedFormat(CopacImporterTest.class.getResourceAsStream("CopacImporterTest2.txt")));
  Assert.assertFalse(importer.isRecognizedFormat(CopacImporterTest.class.getResourceAsStream("IsiImporterTest1.isi")));
  Assert.assertFalse(importer.isRecognizedFormat(CopacImporterTest.class.getResourceAsStream("IsiImporterTestInspec.isi")));
  Assert.assertFalse(importer.isRecognizedFormat(CopacImporterTest.class.getResourceAsStream("IsiImporterTestWOS.isi")));
  Assert.assertFalse(importer.isRecognizedFormat(CopacImporterTest.class.getResourceAsStream("IsiImporterTestMedline.isi")));
}
