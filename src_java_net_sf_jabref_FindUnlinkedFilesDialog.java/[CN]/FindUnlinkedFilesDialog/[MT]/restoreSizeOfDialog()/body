{
  String store=Globals.prefs.get(GLOBAL_PREFS_DIALOG_SIZE_KEY);
  Dimension dimension=null;
  if (store != null) {
    try {
      String[] dim=store.split(";");
      dimension=new Dimension(new Integer(dim[0]),new Integer(dim[1]));
    }
 catch (    Exception e) {
    }
  }
  if (dimension != null) {
    setPreferredSize(dimension);
  }
}
