{
  return new LatexFieldFormatterPreferences(prefs.getBoolean(JabRefPreferences.RESOLVE_STRINGS_ALL_FIELDS),prefs.getValueDelimiters(0),prefs.getValueDelimiters(1),prefs.getStringList(JabRefPreferences.DO_NOT_RESOLVE_STRINGS_FOR),prefs.getInt(JabRefPreferences.LINE_LENGTH),FieldContentParserPreferences.fromPreferences(prefs));
}
