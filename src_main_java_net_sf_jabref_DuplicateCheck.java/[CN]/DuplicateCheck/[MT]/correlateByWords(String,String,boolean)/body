{
  String[] w1=s1.split("\\s"), w2=s2.split("\\s");
  int n=Math.min(w1.length,w2.length);
  int misses=0;
  for (int i=0; i < n; i++) {
    double corr=DuplicateCheck.correlateStrings(w1[i],w2[i],truncate);
    if (corr < 0.75) {
      misses++;
    }
  }
  double missRate=((double)misses) / ((double)n);
  return 1 - missRate;
}
