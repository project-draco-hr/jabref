{
  int thisRow=panel.getMainTable().findEntry(entry);
  int newRow;
  if ((thisRow - 1) >= 0) {
    newRow=thisRow - 1;
  }
 else   if (thisRow != (panel.getDatabase().getEntryCount() - 1)) {
    newRow=panel.getDatabase().getEntryCount() - 1;
  }
 else {
    return;
  }
  scrollTo(newRow);
  panel.getMainTable().setRowSelectionInterval(newRow,newRow);
}
