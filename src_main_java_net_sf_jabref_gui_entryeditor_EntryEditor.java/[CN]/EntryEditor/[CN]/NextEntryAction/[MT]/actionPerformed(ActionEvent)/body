{
  int thisRow=panel.getMainTable().findEntry(entry);
  int newRow;
  if ((thisRow + 1) < panel.getDatabase().getEntryCount()) {
    newRow=thisRow + 1;
  }
 else   if (thisRow > 0) {
    newRow=0;
  }
 else {
    return;
  }
  scrollTo(newRow);
  panel.getMainTable().setRowSelectionInterval(newRow,newRow);
}
