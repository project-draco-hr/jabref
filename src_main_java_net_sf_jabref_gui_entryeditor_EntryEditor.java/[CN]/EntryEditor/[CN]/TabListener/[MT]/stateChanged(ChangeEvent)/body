{
  SwingUtilities.invokeLater(new Runnable(){
    @Override public void run(){
      activateVisible();
    }
  }
);
  SwingUtilities.invokeLater(new Runnable(){
    @Override public void run(){
      Object activeTab=tabs.get(tabbed.getSelectedIndex());
      if (activeTab instanceof EntryEditorTab) {
        ((EntryEditorTab)activeTab).updateAll();
      }
    }
  }
);
}
