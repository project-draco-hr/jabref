{
  if (this.entry == swtichEntry) {
    updateSource();
    return;
  }
  storeCurrentEdit();
  this.entry.removePropertyChangeListener(this);
  swtichEntry.addPropertyChangeListener(this);
  this.entry=swtichEntry;
  updateAllFields();
  validateAllFields();
  updateSource();
  panel.newEntryShowing(swtichEntry);
}
