{
  EntryEditorTab otherPanel=new EntryEditorTab(frame,panel,otherFields,this,false,false,Localization.lang("Other fields"));
  if (otherPanel.fileListEditor != null) {
    fileListEditor=otherPanel.fileListEditor;
  }
  tabbed.addTab(Localization.lang("Other fields"),IconTheme.JabRefIcon.OPTIONAL.getSmallIcon(),otherPanel.getPane(),Localization.lang("Show remaining fields"));
  tabs.add(otherPanel);
}
