{
  panel.output(Globals.lang("Warning") + ": " + Globals.lang("Empty BibTeX key."));
  if (prefs.getBoolean("dialogWarningForEmptyKey")) {
    CheckBoxMessage jcb=new CheckBoxMessage(Globals.lang("Warning") + ": " + Globals.lang("Empty BibTeX key. Grouping may not work for this entry."),Globals.lang("Disable this warning dialog"),false);
    JOptionPane.showMessageDialog(frame,jcb,Globals.lang("Warning"),JOptionPane.WARNING_MESSAGE);
    if (jcb.isSelected())     prefs.putBoolean("dialogWarningForEmptyKey",false);
  }
}
