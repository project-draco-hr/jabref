{
  GroupTreeNode grandParent=getNodeInSimpleTree();
  GroupTreeNode parent=grandParent.addSubgroup(new KeywordGroup("node","author","author2",true,false,GroupHierarchyType.INDEPENDENT,Globals.prefs));
  GroupTreeNode node=parent.addSubgroup(new KeywordGroup("node","author","author1",true,false,GroupHierarchyType.INDEPENDENT,Globals.prefs));
  assertEquals(2,node.numberOfHits(entries));
}
