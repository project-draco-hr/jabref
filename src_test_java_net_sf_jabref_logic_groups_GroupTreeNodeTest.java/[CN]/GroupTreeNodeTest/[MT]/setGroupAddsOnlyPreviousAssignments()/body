{
  AbstractGroup oldGroup=new ExplicitGroup("OldGroup",GroupHierarchyType.INDEPENDENT,JabRefPreferences.getInstance());
  assertFalse(oldGroup.isMatch(entry));
  GroupTreeNode node=GroupTreeNode.fromGroup(oldGroup);
  AbstractGroup newGroup=new ExplicitGroup("NewGroup",GroupHierarchyType.INDEPENDENT,JabRefPreferences.getInstance());
  node.setGroup(newGroup,true,entries);
  assertFalse(newGroup.isMatch(entry));
}
