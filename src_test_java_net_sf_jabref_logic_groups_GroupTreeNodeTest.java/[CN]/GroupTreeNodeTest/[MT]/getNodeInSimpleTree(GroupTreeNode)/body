{
  root.addSubgroup(new ExplicitGroup("ExplicitA",GroupHierarchyType.INCLUDING,JabRefPreferences.getInstance().get(JabRefPreferences.KEYWORD_SEPARATOR)));
  GroupTreeNode parent=root.addSubgroup(new ExplicitGroup("ExplicitParent",GroupHierarchyType.INDEPENDENT,JabRefPreferences.getInstance().get(JabRefPreferences.KEYWORD_SEPARATOR)));
  GroupTreeNode node=parent.addSubgroup(new ExplicitGroup("ExplicitNode",GroupHierarchyType.REFINING,JabRefPreferences.getInstance().get(JabRefPreferences.KEYWORD_SEPARATOR)));
  return node;
}
