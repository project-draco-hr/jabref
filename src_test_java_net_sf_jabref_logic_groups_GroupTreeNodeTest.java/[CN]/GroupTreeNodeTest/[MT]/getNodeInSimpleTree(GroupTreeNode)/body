{
  root.addSubgroup(new ExplicitGroup("ExplicitA",GroupHierarchyType.INCLUDING,Globals.prefs));
  GroupTreeNode parent=root.addSubgroup(new ExplicitGroup("ExplicitParent",GroupHierarchyType.INDEPENDENT,Globals.prefs));
  GroupTreeNode node=parent.addSubgroup(new ExplicitGroup("ExplicitNode",GroupHierarchyType.REFINING,Globals.prefs));
  return node;
}
