{
  String s=getAbbreviatedName(journalName,true);
  if (s != null) {
    s=s.toLowerCase();
  }
 else {
    return null;
  }
  Object o=abbrNameKeyed.get(s);
  if (o == null) {
    if (fullNameKeyed.containsKey(s)) {
      o=s;
    }
 else {
      return null;
    }
  }
  s=(String)o;
  return s;
}
