{
  StringBuffer res=new StringBuffer(getLastOnly());
  if (jrPart != null) {
    res.append(", ").append(jrPart);
  }
  if (abbr) {
    if (firstAbbr != null) {
      res.append(", ").append(firstAbbr);
    }
  }
 else {
    if (firstPart != null) {
      res.append(", ").append(firstPart);
    }
  }
  return res.toString();
}
