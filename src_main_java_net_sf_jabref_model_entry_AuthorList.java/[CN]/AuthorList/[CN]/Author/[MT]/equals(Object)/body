{
  if (this == o) {
    return true;
  }
  if (o instanceof Author) {
    Author a=(Author)o;
    return EntryUtil.equals(firstPart,a.firstPart) && EntryUtil.equals(firstAbbr,a.firstAbbr) && EntryUtil.equals(vonPart,a.vonPart)&& EntryUtil.equals(lastPart,a.lastPart)&& EntryUtil.equals(jrPart,a.jrPart);
  }
  return false;
}
