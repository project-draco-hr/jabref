{
  if (this == o) {
    return true;
  }
  if (o instanceof Author) {
    Author a=(Author)o;
    return Objects.equals(firstPart,a.firstPart) && Objects.equals(firstAbbr,a.firstAbbr) && Objects.equals(vonPart,a.vonPart)&& Objects.equals(lastPart,a.lastPart)&& Objects.equals(jrPart,a.jrPart);
  }
  return false;
}
