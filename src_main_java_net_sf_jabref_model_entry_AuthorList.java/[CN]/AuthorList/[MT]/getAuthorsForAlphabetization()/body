{
  if (authorsAlph != null) {
    return authorsAlph;
  }
  StringBuilder result=new StringBuilder();
  if (size() > 0) {
    result.append(getAuthor(0).getNameForAlphabetization());
    for (int i=1; i < size(); i++) {
      result.append(" and ");
      result.append(getAuthor(i).getNameForAlphabetization());
    }
  }
  authorsAlph=result.toString();
  return authorsAlph;
}
