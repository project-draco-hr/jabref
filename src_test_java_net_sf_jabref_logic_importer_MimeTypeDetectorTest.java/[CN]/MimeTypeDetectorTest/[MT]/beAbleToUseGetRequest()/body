{
  String mimeTypeVariation="http://localhost:8080/mimevariation";
  stubFor(head(urlEqualTo("/mimevariation")).willReturn(aResponse().withStatus(404)));
  stubFor(get(urlEqualTo("/mimevariation")).willReturn(aResponse().withHeader("Content-Type","application/pdf;charset=ISO-8859-1")));
  assertTrue(MimeTypeDetector.isPdfContentType(mimeTypeVariation));
}
