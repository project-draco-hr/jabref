{
  String translation=null;
  try {
    if (intMessages != null) {
      translation=intMessages.getString(key);
    }
  }
 catch (  MissingResourceException ex) {
    translation=key;
    LOGGER.warn("Warning: could not get menu item translation for \"" + key + "\"");
  }
  if (translation != null && !translation.isEmpty()) {
    return translation;
  }
 else {
    return key;
  }
}
