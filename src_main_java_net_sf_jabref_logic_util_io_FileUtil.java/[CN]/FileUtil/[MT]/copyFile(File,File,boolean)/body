{
  if (dest.exists() && !deleteIfExists) {
    return false;
  }
  try (BufferedInputStream in=new BufferedInputStream(new FileInputStream(source));BufferedOutputStream out=new BufferedOutputStream(new FileOutputStream(dest))){
    int el;
    while ((el=in.read()) >= 0) {
      out.write(el);
    }
    out.flush();
  }
   return true;
}
