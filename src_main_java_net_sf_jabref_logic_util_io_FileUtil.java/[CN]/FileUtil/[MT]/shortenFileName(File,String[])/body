{
  if (fileName == null || fileName.length() == 0) {
    return fileName;
  }
  if (!fileName.isAbsolute() || dirs == null) {
    return fileName;
  }
  for (  String dir : dirs) {
    if (dir != null) {
      File result=shortenFileName(fileName,dir);
      if (result != null && !result.equals(fileName)) {
        return result;
      }
    }
  }
  return fileName;
}
