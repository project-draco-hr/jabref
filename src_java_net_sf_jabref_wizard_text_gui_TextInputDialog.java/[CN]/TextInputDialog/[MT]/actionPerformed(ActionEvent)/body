{
  Object source=e.getSource();
  if (source == this.okButton) {
    okPressed=true;
    dispose();
  }
 else   if (source == this.cancelButton) {
    dispose();
  }
 else   if (source == this.insertButton) {
    insertTextForTag();
  }
 else   if (source == this.parseWithFreeCiteButton) {
    parseWithFreeCiteAndAddEntries();
    okPressed=false;
    dispose();
  }
}
