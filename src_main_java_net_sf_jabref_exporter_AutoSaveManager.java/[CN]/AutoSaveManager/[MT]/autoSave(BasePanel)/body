{
  File backupFile=AutoSaveManager.getAutoSaveFile(panel.getLoadedDatabase().getDatabaseFile());
  try {
    SaveSession ss=FileActions.saveDatabase(panel.database(),panel.loadedDatabase.getMetaData(),backupFile,Globals.prefs,false,false,panel.getEncoding(),true);
    ss.commit();
  }
 catch (  SaveException e) {
    e.printStackTrace();
    return false;
  }
catch (  Throwable ex) {
    ex.printStackTrace();
    return false;
  }
  return true;
}
