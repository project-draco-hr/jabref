{
  File databaseFile=panel.getBibDatabaseContext().getDatabaseFile();
  File backupFile=AutoSaveManager.getAutoSaveFile(databaseFile);
  try {
    SaveSession ss=FileActions.saveDatabase(panel.getBibDatabaseContext(),backupFile,Globals.prefs,false,false,panel.getEncoding(),true);
    ss.commit();
  }
 catch (  SaveException e) {
    e.printStackTrace();
    return false;
  }
catch (  Throwable ex) {
    ex.printStackTrace();
    return false;
  }
  return true;
}
