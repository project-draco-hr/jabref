{
  StringBuilder sb=new StringBuilder();
  sb.append(ExplicitGroup.ID).append(ModelStringUtil.quote(getName(),AbstractGroup.SEPARATOR,AbstractGroup.QUOTE_CHAR)).append(AbstractGroup.SEPARATOR).append(getContext().ordinal()).append(AbstractGroup.SEPARATOR);
  Set<String> sortedKeys=new TreeSet<>();
  sortedKeys.addAll(legacyEntryKeys);
  for (  String sortedKey : sortedKeys) {
    sb.append(ModelStringUtil.quote(sortedKey,AbstractGroup.SEPARATOR,AbstractGroup.QUOTE_CHAR)).append(AbstractGroup.SEPARATOR);
  }
  return sb.toString();
}
