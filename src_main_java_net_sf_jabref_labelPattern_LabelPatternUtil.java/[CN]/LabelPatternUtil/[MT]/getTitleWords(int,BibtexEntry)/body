{
  String ss=new RemoveLatexCommands().format(_entry.getField("title"));
  StringBuilder _sbvalue=new StringBuilder();
  StringBuffer current;
  int piv=0;
  int words=0;
  mainl:   while (piv < ss.length() && words < number) {
    current=new StringBuffer();
    while (piv < ss.length() && !Character.isWhitespace(ss.charAt(piv)) && ss.charAt(piv) != '-') {
      current.append(ss.charAt(piv));
      piv++;
    }
    piv++;
    String word=current.toString().trim();
    if (word.isEmpty()) {
      continue;
    }
    for (    String SKIP_WORD : SKIP_WORDS) {
      if (word.equalsIgnoreCase(SKIP_WORD)) {
        continue mainl;
      }
    }
    if (_sbvalue.length() > 0) {
      _sbvalue.append(" ");
    }
    _sbvalue.append(word);
    words++;
  }
  return LabelPatternUtil.keepLettersAndDigitsOnly(_sbvalue.toString());
}
