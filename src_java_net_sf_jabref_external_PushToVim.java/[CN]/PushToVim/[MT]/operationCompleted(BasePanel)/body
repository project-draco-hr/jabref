{
  if (couldNotConnect)   JOptionPane.showMessageDialog(panel.frame(),"<HTML>" + Globals.lang("Could not connect to Vim server. Make sure that " + "Vim is running<BR>with correct server name.") + "</HTML>",Globals.lang("Error"),JOptionPane.ERROR_MESSAGE);
 else   if (couldNotRunClient)   JOptionPane.showMessageDialog(panel.frame(),Globals.lang("Could not run the 'vim' program."),Globals.lang("Error"),JOptionPane.ERROR_MESSAGE);
 else {
    panel.output(Globals.lang("Pushed citations to Vim"));
  }
}
