{
  println("Start ZERO-OR-MORE (...)+ block:");
  tabs++;
  genBlockPreamble(blk);
  boolean ok=grammar.theLLkAnalyzer.deterministic(blk);
  if (!ok) {
    println("Warning: This zero-or-more block is non-deterministic");
  }
  genCommonBlock(blk);
  tabs--;
  println("End ZERO-OR-MORE block.");
}
