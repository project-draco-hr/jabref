{
  if (s.startsWith(KeywordGroup.ID))   return KeywordGroup.fromString(s);
  if (s.startsWith(AllEntriesGroup.ID))   return AllEntriesGroup.fromString(s);
  if (s.startsWith(SearchGroup.ID))   return SearchGroup.fromString(s);
  if (s.startsWith(ExplicitGroup.ID))   return ExplicitGroup.fromString(s);
  return null;
}
