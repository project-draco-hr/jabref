{
  out.write("@comment{");
  out.write(GUIGlobals.ENTRYTYPE_FLAG);
  out.write(getName());
  out.write(": req[");
  out.write(getRequiredFieldsString());
  out.write("] opt[");
  StringBuilder sb=new StringBuilder();
  for (int i=0; i < opt.length; i++) {
    sb.append(opt[i]);
    if (i < (opt.length - 1)) {
      sb.append(";");
    }
  }
  out.write(sb.toString());
  out.write("]}" + Globals.NEWLINE);
}
