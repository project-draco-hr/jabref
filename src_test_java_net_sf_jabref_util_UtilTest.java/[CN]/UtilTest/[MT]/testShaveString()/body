{
  Assert.assertEquals(null,StringUtil.shaveString(null));
  Assert.assertEquals("",StringUtil.shaveString(""));
  Assert.assertEquals("aaa",StringUtil.shaveString("   aaa\t\t\n\r"));
  Assert.assertEquals("a",StringUtil.shaveString("  {a}    "));
  Assert.assertEquals("a",StringUtil.shaveString("  \"a\"    "));
  Assert.assertEquals("{a}",StringUtil.shaveString("  {{a}}    "));
  Assert.assertEquals("{a}",StringUtil.shaveString("  \"{a}\"    "));
  Assert.assertEquals("\"{a\"}",StringUtil.shaveString("  \"{a\"}    "));
}
