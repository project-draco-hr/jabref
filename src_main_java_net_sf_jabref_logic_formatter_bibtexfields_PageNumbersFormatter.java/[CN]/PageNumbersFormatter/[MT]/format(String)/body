{
  if ((value == null) || value.isEmpty()) {
    return value;
  }
  String cleanValue=value.replaceAll(REJECT_LITERALS,"");
  Matcher matcher=PAGES_PATTERN.matcher(cleanValue);
  String newValue=matcher.replaceFirst(REPLACE);
  if (!newValue.equals(cleanValue)) {
    return newValue;
  }
  return value;
}
