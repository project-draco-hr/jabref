{
  Vector v=new Vector(10);
  StringBuffer buf=new StringBuffer(100);
  int i=0;
  while (i < list.length()) {
    while (i < list.length() && list.charAt(i) != separator) {
      buf.append(list.charAt(i));
      i++;
    }
    v.appendElement(buf.toString());
    buf.setLength(0);
    if (i < list.length()) {
      i++;
    }
  }
  if (v.size() == 0)   return null;
  return v;
}
