{
  tableRows.clear();
  String[] names=JabRefPreferences.getInstance().getStringArray(JabRefPreferences.XMP_PRIVACY_FILTERS);
  Collections.addAll(tableRows,names);
  rowCount=tableRows.size() + 5;
  privacyFilterCheckBox.setSelected(JabRefPreferences.getInstance().getBoolean(JabRefPreferences.USE_XMP_PRIVACY_FILTER));
}
