{
  if (!connectToDB)   return;
  for (  Object[] res : databases) {
    database=(BibtexDatabase)res[0];
    metaData=(MetaData)res[1];
    if (database != null) {
      BasePanel pan=frame.addTab(database,null,metaData,Globals.prefs.get("defaultEncoding"),true);
      pan.metaData().setDBStrings(dbs);
      frame.setTabTitle(pan,res[2] + "(Imported)","Imported DB");
      pan.markBaseChanged();
    }
  }
  frame.output(Globals.lang("Imported " + databases.size() + " databases successfully"));
}
