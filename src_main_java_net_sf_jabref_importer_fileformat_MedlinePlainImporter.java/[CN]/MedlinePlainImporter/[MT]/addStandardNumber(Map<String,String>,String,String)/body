{
  if ("IS".equals(lab)) {
    String key="issn";
    if (value.indexOf('(') > 0) {
      int keyStart=value.indexOf('(');
      int keyEnd=value.indexOf(')');
      key=value.substring(keyStart + 1,keyEnd) + "-" + key;
      String numberValue=value.substring(0,keyStart - 1);
      hm.put(key,numberValue);
    }
 else {
      hm.put(key,value);
    }
  }
 else   if ("ISBN".equals(lab)) {
    hm.put("isbn",value);
  }
}
