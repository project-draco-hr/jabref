{
  Rule overriddenRule=(Rule)rules.getElement(r.getName());
  if (overriddenRule != null) {
    if (!overriddenRule.sameSignature(r)) {
      antlrTool.warning("rule " + getName() + "."+ overriddenRule.getName()+ " has different signature than "+ superG.getName()+ "."+ overriddenRule.getName());
    }
  }
 else {
    addRule(r);
  }
}
