{
  if (this == o)   return true;
  if (o == null || getClass() != o.getClass())   return false;
  SaveOrderConfig that=(SaveOrderConfig)o;
  boolean sortCriteriaEquals=sortCriteria[0].equals(that.sortCriteria[0]) && sortCriteria[1].equals(that.sortCriteria[1]) && sortCriteria[2].equals(that.sortCriteria[2]);
  return Objects.equals(saveInOriginalOrder,that.saveInOriginalOrder) && sortCriteriaEquals;
}
