{
  couldNotCall=false;
  String winEdt=Globals.prefs.get("winEdtPath");
  try {
    StringBuffer toSend=new StringBuffer("\"[InsText('\\").append(Globals.prefs.get("citeCommand")).append("{").append(keyString).append("}');]\"");
