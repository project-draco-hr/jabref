{
  DefaultFormBuilder builder=new DefaultFormBuilder(new FormLayout("left:pref, 4dlu, fill:pref",""));
  builder.append(Localization.lang("Cite command") + ":");
  builder.append(citeCommand);
  builder.nextLine();
