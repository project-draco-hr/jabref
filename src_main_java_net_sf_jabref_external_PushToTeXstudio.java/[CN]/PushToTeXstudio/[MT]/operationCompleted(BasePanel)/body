{
  if (couldNotConnect) {
    JOptionPane.showMessageDialog(panel.frame(),"TeXstudio: could not connect",Localization.lang("Error"),JOptionPane.ERROR_MESSAGE);
  }
 else   if (couldNotRunClient) {
    String programPath=Globals.prefs.get("TeXstudioPath");
    if (programPath == null) {
      programPath=defaultProgramPath();
    }
    JOptionPane.showMessageDialog(panel.frame(),"TeXstudio: " + Localization.lang("Program '%0' not found",programPath),Localization.lang("Error"),JOptionPane.ERROR_MESSAGE);
  }
 else {
    panel.output(Localization.lang("Pushed citations to %0","TeXstudio"));
  }
}
