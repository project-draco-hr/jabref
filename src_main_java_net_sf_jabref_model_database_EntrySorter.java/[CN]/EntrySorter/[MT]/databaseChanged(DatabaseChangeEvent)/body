{
synchronized (set) {
    int pos;
switch (e.getType()) {
case ADDED_ENTRY:
      pos=Collections.binarySearch(set,e.getEntry(),comp);
    if (pos < 0) {
      set.add(0,e.getEntry());
    }
  break;
case REMOVED_ENTRY:
set.remove(e.getEntry());
changed=true;
break;
case CHANGED_ENTRY:
pos=Collections.binarySearch(set,e.getEntry(),comp);
int posOld=set.indexOf(e.getEntry());
if (pos < 0) {
set.add(0,e.getEntry());
}
break;
}
}
}
