{
  int[] rows=colSetup.getSelectedRows();
  if (rows.length == 0) {
    rowCount++;
    colSetup.revalidate();
    colSetup.repaint();
    return;
  }
  for (int i=0; i < rows.length; i++) {
    if (rows[i] + i - 1 < tableRows.size())     tableRows.add(Math.max(0,rows[i] + i - 1),new TableRow(GUIGlobals.DEFAULT_FIELD_LENGTH));
  }
  rowCount+=rows.length;
  if (rows.length > 1)   colSetup.clearSelection();
  colSetup.revalidate();
  colSetup.repaint();
  tableChanged=true;
}
