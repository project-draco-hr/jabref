{
  char[] c=s.toCharArray();
  for (int i=0; i < c.length; i++) {
    if (Character.isLetter(c[i])) {
      return String.valueOf(c[i]);
    }
    if (c[i] == '{') {
      if (((i + 1) < c.length) && (c[i + 1] == '\\')) {
        StringBuffer sb=new StringBuffer();
        BibtexNameFormatter.consumeToMatchingBrace(sb,c,i);
        return sb.toString();
      }
    }
  }
  return "";
}
