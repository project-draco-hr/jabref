{
  int[] rows=colSetup.getSelectedRows();
  if (rows.length == 0)   return;
  for (int i=0; i < rows.length; i++) {
    if (rows[i] - i < tableRows.size())     tableRows.remove(rows[i] - i);
  }
  rowCount-=rows.length;
  if (rows.length > 1)   colSetup.clearSelection();
  colSetup.revalidate();
  colSetup.repaint();
  tableChanged=true;
}
