{
  this.fieldNames=fieldNames;
  this.lastNameOnly=lastNameOnly;
  if (Globals.prefs.getBoolean("autoCompFF")) {
    autoCompFF=true;
    autoCompLF=false;
  }
 else   if (Globals.prefs.getBoolean("autoCompLF")) {
    autoCompFF=false;
    autoCompLF=true;
  }
 else {
    autoCompFF=true;
    autoCompLF=true;
  }
}
