{
  if (howToFinish.needAnErrorClause && (howToFinish.generatedAnIf || howToFinish.generatedSwitch)) {
    if (howToFinish.generatedAnIf) {
      tabs++;
      println("else");
    }
 else {
    }
    tabs++;
    println(noViableAction);
    tabs--;
    if (howToFinish.generatedAnIf) {
      println("end; -- if");
      tabs--;
    }
    if (howToFinish.generatedSwitch)     println("end; -- case");
  }
  if (!howToFinish.needAnErrorClause && howToFinish.generatedSwitch)   println("end; -- case");
  if (howToFinish.postscript != null) {
    println(howToFinish.postscript);
  }
}
