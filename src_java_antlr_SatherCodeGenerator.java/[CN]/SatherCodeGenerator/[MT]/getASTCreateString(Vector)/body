{
  if (v.size() == 0) {
    return "";
  }
  StringBuffer buf=new StringBuffer();
  buf.append("ANTLR_AST_UTIL{AST}::make( ( #ANTLR_AST_ARRAY{AST}(" + v.size() + "))");
  for (int i=0; i < v.size(); i++) {
    buf.append(".add(" + v.elementAt(i) + ")");
  }
  buf.append(")");
  return buf.toString();
}
