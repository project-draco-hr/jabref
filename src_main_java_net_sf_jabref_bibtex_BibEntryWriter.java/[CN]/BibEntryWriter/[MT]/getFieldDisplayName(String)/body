{
  if (field.isEmpty()) {
    field="UNKNOWN";
  }
  StringBuilder suffixSB=new StringBuilder();
  for (int i=BibEntryWriter.maxFieldLength - field.length(); i > 0; i--) {
    suffixSB.append(" ");
  }
  String suffix=suffixSB.toString();
  String result;
  result=field.toLowerCase() + " = " + suffix;
  return result;
}
