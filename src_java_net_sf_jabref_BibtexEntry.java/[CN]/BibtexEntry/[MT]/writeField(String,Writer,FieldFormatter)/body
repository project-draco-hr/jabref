{
  Object o=getField(name);
  if (o != null) {
    out.write("  " + name + " = ");
    try {
      out.write(ff.format(o.toString(),GUIGlobals.isStandardField(name)));
    }
 catch (    Throwable ex) {
      throw new IOException("Error in field '" + name + "': "+ ex.getMessage());
    }
    out.write(",\n");
  }
}
