{
  Object o=getField(name);
  if (o != null) {
    out.write("  " + name + " = ");
    try {
      out.write(ff.format(o.toString(),name));
    }
 catch (    Throwable ex) {
      throw new IOException(Globals.lang("Error in field") + " '" + name+ "': "+ ex.getMessage());
    }
    out.write("," + Globals.NEWLINE);
  }
}
