{
  if (type == null) {
    throw new NullPointerException("Every BibtexEntry must have a type.  Instead of null, use type OTHER");
  }
  BibtexEntryType oldType=_type;
  try {
    _type=type;
    firePropertyChangedEvent(GUIGlobals.TYPE_HEADER,oldType != null ? oldType.getName() : null,type.getName());
  }
 catch (  PropertyVetoException pve) {
    pve.printStackTrace();
  }
}
