{
  success=false;
  canceled=false;
  fileLockedError=false;
  if (panel.getBibDatabaseContext().getDatabaseFile().isPresent()) {
    if (checkExternalModification()) {
      return;
    }
    panel.frame().output(Localization.lang("Saving database") + "...");
    panel.setSaving(true);
  }
 else {
    saveAs();
  }
}
