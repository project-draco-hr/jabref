{
  File file=null;
  while (file == null) {
    FileDialog dialog=new FileDialog(frame);
    dialog.withExtension(FileExtensions.BIBTEX_DB);
    dialog.setDefaultExtension(FileExtensions.BIBTEX_DB);
    Optional<Path> path=dialog.saveNewFile();
    if (path.isPresent()) {
      saveAs(path.get().toFile());
    }
 else {
      canceled=true;
      return;
    }
  }
}
