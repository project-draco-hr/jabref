{
  if (!Globals.prefs.getBoolean(JabRefPreferences.SHOW_FILE_LINKS_UPGRADE_WARNING)) {
    return false;
  }
  if (pr.getJabrefMajorVersion() < 0) {
    return false;
  }
  if (pr.getJabrefMajorVersion() < 2) {
    return true;
  }
  if (pr.getJabrefMajorVersion() > 2) {
    return false;
  }
  return pr.getJabrefMinorVersion() <= 2;
}
