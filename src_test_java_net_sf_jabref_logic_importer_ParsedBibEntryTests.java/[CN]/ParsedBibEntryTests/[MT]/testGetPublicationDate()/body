{
  Assert.assertEquals(Optional.of("2003-02"),(BibtexParser.singleFromStringOptional("@ARTICLE{HipKro03, year = {2003}, month = #FEB# }",importFormatPreferences)).get().getPublicationDate());
  Assert.assertEquals(Optional.of("2003-03"),(BibtexParser.singleFromStringOptional("@ARTICLE{HipKro03, year = {2003}, month = 3 }",importFormatPreferences)).get().getPublicationDate());
  Assert.assertEquals(Optional.of("2003"),(BibtexParser.singleFromStringOptional("@ARTICLE{HipKro03, year = {2003}}",importFormatPreferences)).get().getPublicationDate());
  Assert.assertEquals(Optional.empty(),(BibtexParser.singleFromStringOptional("@ARTICLE{HipKro03, month = 3 }",importFormatPreferences)).get().getPublicationDate());
  Assert.assertEquals(Optional.empty(),(BibtexParser.singleFromStringOptional("@ARTICLE{HipKro03, author={bla}}",importFormatPreferences)).get().getPublicationDate());
  Assert.assertEquals(Optional.of("2003-12"),(BibtexParser.singleFromStringOptional("@ARTICLE{HipKro03, year = {2003}, month = #DEC# }",importFormatPreferences)).get().getPublicationDate());
}
