{
  Document docin=null;
  try {
    DocumentBuilder dbuild=DocumentBuilderFactory.newInstance().newDocumentBuilder();
    docin=dbuild.parse(in);
  }
 catch (  Exception e) {
    return false;
  }
  if (docin != null && !docin.getDocumentElement().getTagName().contains("Sources"))   return false;
  return true;
}
