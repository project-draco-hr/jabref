{
  String author="";
  String[] tokens=AuthorList.fixAuthorForAlphabetization(authorField).split("\\s+\\band\\b\\s+");
  int i=0;
  while ((tokens.length > i) && (i < n)) {
    String lastName=tokens[i].replaceAll(",\\s+.*","");
    author+=lastName;
    i++;
  }
  if (tokens.length <= n) {
    return author;
  }
  return author + "EtAl";
}
