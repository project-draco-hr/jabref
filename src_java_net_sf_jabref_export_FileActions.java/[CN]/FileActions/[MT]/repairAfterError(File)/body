{
  String name=file.getName();
  String path=file.getParent();
  File temp=new File(path,name + GUIGlobals.tempExt);
  File back=new File(path,name + GUIGlobals.backupExt);
  if (file.exists()) {
    file.delete();
  }
  if (temp.exists()) {
    temp.renameTo(file);
  }
 else {
    back.renameTo(file);
  }
}
