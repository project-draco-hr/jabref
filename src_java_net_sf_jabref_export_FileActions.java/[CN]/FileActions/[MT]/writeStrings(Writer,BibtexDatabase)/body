{
  for (int i=0; i < database.getStringCount(); i++) {
    BibtexString bs=database.getString(i);
    fw.write("@STRING{" + bs.getName() + " = ");
    if (!bs.getContent().equals(""))     fw.write((new LatexFieldFormatter()).format(bs.getContent(),true));
 else     fw.write("{}");
    fw.write("}\n\n");
  }
}
