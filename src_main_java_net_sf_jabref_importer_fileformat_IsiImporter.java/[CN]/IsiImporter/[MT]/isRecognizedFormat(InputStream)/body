{
  try (BufferedReader in=new BufferedReader(ImportFormatReader.getReaderDefaultEncoding(stream))){
    String str;
    int i=0;
    while (((str=in.readLine()) != null) && (i < 50)) {
      if (IsiImporter.ISI_PATTERN.matcher(str).find()) {
        return true;
      }
      i++;
    }
  }
   return false;
}
