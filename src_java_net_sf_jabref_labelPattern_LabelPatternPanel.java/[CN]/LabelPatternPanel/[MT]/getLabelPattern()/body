{
  LabelPattern keypatterns=new LabelPattern();
  for (  String s1 : textFields.keySet()) {
    String s=s1, text=textFields.get(s).getText();
    if (!"".equals(text.trim()))     keypatterns.addLabelPattern(s,text);
  }
  String text=defaultPat.getText();
  if (!"".equals(text.trim())) {
    keypatterns.setDefaultValue(text);
  }
  return keypatterns;
}
