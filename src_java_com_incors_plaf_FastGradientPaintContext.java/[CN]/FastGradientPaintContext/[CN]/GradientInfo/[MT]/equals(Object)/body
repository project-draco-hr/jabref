{
  if (!(o instanceof GradientInfo)) {
    return false;
  }
  GradientInfo gi=(GradientInfo)o;
  if (gi.model.equals(model) && (gi.parallelLength == parallelLength) && (gi.startColor == startColor)&& (gi.endColor == endColor)&& (gi.isVertical == isVertical)) {
    return true;
  }
  return false;
}
