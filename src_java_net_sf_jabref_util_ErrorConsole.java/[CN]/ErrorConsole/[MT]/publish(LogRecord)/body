{
  String msg=fmt.format(record);
  logOutput.add(msg);
  if (logOutput.size() < MAXLOGLINES) {
    logOutputCache=logOutputCache + msg;
  }
 else {
    logOutputCacheRefreshNeeded=true;
    while (logOutput.size() > MAXLOGLINES) {
      logOutput.remove(0);
    }
  }
  logOutputCacheRefreshNeeded=true;
}
