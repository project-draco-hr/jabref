{
  this.entryTable=entryTable;
  this.frame=frame;
  this.panel=panel;
  stringFlavor=DataFlavor.stringFlavor;
  try {
    urlFlavor=new DataFlavor("application/x-java-url; class=java.net.URL");
  }
 catch (  ClassNotFoundException e) {
