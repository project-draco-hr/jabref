{
  BibtexEntry e=BibtexParser.singleFromString("@article{canh05," + "title = {\nHallo \nWorld \nthis \n is\n\nnot \n\nan \n\n exercise \n \n.\n \n\n},\n" + "tabs = {\nHallo \tWorld \tthis \t is\t\tnot \t\tan \t\n exercise \t \n.\t \n\t},\n"+ "file = {Bemerkung:H:\\bla\\ups \n\tsala.pdf:PDF}, \n"+ "}");
