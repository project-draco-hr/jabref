{
  int selected=0;
  for (  TableEntry entry : entries) {
    if (entry.isWanted())     selected++;
  }
  if (selected > warningLimit) {
    int result=JOptionPane.showConfirmDialog(this,Globals.lang("You have selected more than %0 entries for download. Some web sites " + "might block you if you make too many rapid downloads. Do you want to continue?",String.valueOf(warningLimit)),Globals.lang("Confirm selection"),JOptionPane.YES_NO_OPTION,JOptionPane.WARNING_MESSAGE);
    return result == JOptionPane.YES_OPTION;
  }
 else   return true;
}
