{
  LayoutFormatter chars=new XMLChars();
  if (bibtex.getField("title") != null)   title=chars.format(bibtex.getField("title").toString());
  if (bibtex.getField("publisher") != null)   publisher=chars.format(bibtex.getField("publisher").toString());
  if (bibtex.getField("bibtexkey") != null)   id=bibtex.getField("bibtexkey").toString();
  if (bibtex.getField("place") != null)   place=chars.format(bibtex.getField("place").toString());
  date=getDate(bibtex);
  genre=getMODSgenre(bibtex);
  if (bibtex.getField("author") != null)   authors=getAuthors(bibtex.getField("author").toString());
  if (bibtex.getType() == BibtexEntryType.ARTICLE) {
    host=new MODSEntry();
    host.title=(String)bibtex.getField("booktitle");
    host.publisher=(String)bibtex.getField("publisher");
    host.pages=(String)bibtex.getField("pages");
    host.number=(String)bibtex.getField("number");
    host.volume=(String)bibtex.getField("volume");
    host.issuance="continuing";
  }
  populateExtensionFields(bibtex);
}
