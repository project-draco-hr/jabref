{
  Objects.requireNonNull(listOfNames);
  original=listOfNames;
  tokenStart=0;
  tokenEnd=0;
  List<Author> authors=new ArrayList<>(5);
  while (tokenStart < original.length()) {
    Author author=getAuthor();
    if (author != null) {
      authors.add(author);
    }
  }
  return new AuthorList(authors);
}
