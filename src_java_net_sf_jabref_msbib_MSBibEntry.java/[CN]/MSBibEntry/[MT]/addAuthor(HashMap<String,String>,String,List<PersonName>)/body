{
  if (authorsLst == null)   return;
  String allAuthors="";
  boolean First=true;
  for (Iterator<PersonName> iter=authorsLst.iterator(); iter.hasNext(); ) {
    PersonName name=iter.next();
    if (!First)     allAuthors+=" and ";
    allAuthors+=name.getFullname();
    First=false;
  }
  hm.put(type,allAuthors);
}
