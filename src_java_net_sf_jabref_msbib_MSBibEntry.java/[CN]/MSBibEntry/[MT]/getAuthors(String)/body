{
  List<PersonName> result=new LinkedList<PersonName>();
  if (authors.indexOf(" and ") == -1) {
    result.add(new PersonName(authors));
  }
 else {
    String[] names=authors.split(" and ");
    for (int i=0; i < names.length; i++) {
      result.add(new PersonName(names[i]));
    }
  }
  return result;
}
