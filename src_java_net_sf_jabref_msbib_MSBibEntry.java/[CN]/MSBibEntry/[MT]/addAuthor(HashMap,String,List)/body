{
  if (authorsLst == null)   return;
  String allAuthors="";
  boolean First=true;
  for (Iterator iter=authorsLst.iterator(); iter.hasNext(); ) {
    PersonName name=(PersonName)iter.next();
    if (First == false)     allAuthors+="; ";
    allAuthors+=name.getFullname();
    First=false;
  }
  hm.put(type,allAuthors);
}
