{
  if (!(o instanceof KeywordGroup)) {
    return false;
  }
  KeywordGroup other=(KeywordGroup)o;
  return name.equals(other.name) && searchField.equals(other.searchField) && searchExpression.equals(other.searchExpression)&& (caseSensitive == other.caseSensitive)&& (regExp == other.regExp)&& (getHierarchicalContext() == other.getHierarchicalContext());
}
