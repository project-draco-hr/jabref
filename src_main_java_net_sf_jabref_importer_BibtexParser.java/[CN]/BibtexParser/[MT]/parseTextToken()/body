{
  StringBuilder token=new StringBuilder(20);
  while (true) {
    int c=read();
    if (c == -1) {
      eof=true;
      return token.toString();
    }
    if (Character.isLetterOrDigit((char)c) || c == ':' || c == '-' || c == '_' || c == '*' || c == '+' || c == '.' || c == '/' || c == '\'') {
      token.append((char)c);
    }
 else {
      unread(c);
      return token.toString();
    }
  }
}
