{
  if (deprecated) {
    System.err.print("Warning: ");
    if (longOption != null) {
      System.err.print("--" + longOption);
    }
    if (shortOption != '\0' && longOption != null) {
      System.err.print(" or ");
    }
    if (shortOption != '\0') {
      System.err.println("-" + shortOption + " is deprecated.");
    }
  }
}
