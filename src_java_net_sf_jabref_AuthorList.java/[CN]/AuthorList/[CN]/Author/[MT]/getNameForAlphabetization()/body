{
  StringBuilder res=new StringBuilder();
  if (last_part != null)   res.append(last_part);
  if (jr_part != null) {
    res.append(", ");
    res.append(jr_part);
  }
  if (first_abbr != null) {
    res.append(", ");
    res.append(first_abbr);
  }
  while ((res.length() > 0) && (res.charAt(0) == '{'))   res.deleteCharAt(0);
  return res.toString();
}
