{
  if (authorsNatbib != null)   return authorsNatbib;
  StringBuffer res=new StringBuffer();
  if (size() > 0) {
    res.append(getAuthor(0).getLastOnly());
    if (size() == 2) {
      res.append(" and ");
      res.append(getAuthor(1).getLastOnly());
    }
 else     if (size() > 2) {
      res.append(" et al.");
    }
  }
  authorsNatbib=res.toString();
  return authorsNatbib;
}
