{
  while ((i >= 0) && "".equals(paragraphs[i].trim())) {
    i--;
  }
  int end=i;
  while ((i >= 0) && !"".equals(paragraphs[i])) {
    i--;
  }
  curString="";
  for (int j=i + 1; j <= end; j++) {
    curString=curString.concat(paragraphs[j].trim());
    if (j != end) {
      curString=curString.concat(" ");
    }
  }
}
