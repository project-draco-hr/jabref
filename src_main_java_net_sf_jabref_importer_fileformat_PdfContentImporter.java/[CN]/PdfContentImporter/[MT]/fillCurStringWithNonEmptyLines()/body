{
  curString=curString.trim();
  while ((i < split.length) && !"".equals(split[i])) {
    String curLine=split[i].trim();
    if (!"".equals(curLine)) {
      if (!curString.isEmpty()) {
        curString=curString.concat(" ");
      }
      curString=curString.concat(split[i]);
    }
    i++;
  }
  proceedToNextNonEmptyLine();
}
