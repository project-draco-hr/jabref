{
  input=input.trim();
  if (input.isEmpty()) {
    return input;
  }
  char lastC=input.charAt(input.length() - 1);
  while (!Character.isLetter(lastC) && (lastC != ')')) {
    input=input.substring(0,input.length() - 1);
    if (!input.isEmpty()) {
      lastC=input.charAt(input.length() - 1);
    }
 else {
      break;
    }
  }
  return input;
}
