{
  this.prefs=prefs;
  SearchExpressionParser parser=new SearchExpressionParser(new SearchExpressionLexer(new StringReader(searchOptions.elements().nextElement().toString())));
  parser.caseSensitive=this.prefs.getBoolean("caseSensitiveSearch");
  parser.regex=this.prefs.getBoolean("regExpSearch");
  parser.searchExpression();
  ast=parser.getAST();
}
